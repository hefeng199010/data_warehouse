    <connection>uat_xd_db</connection>
    <sql>SELECT tt.* FROM (SELECT
  DISTINCT
  -- 分公司
  a.branch_name,
  a.borrow_limit,
  -- 逾期本金
  IF(IFNULL(c.principal,0)-IFNULL(f.principal,0)&lt;=0,0,IFNULL(c.principal,0)-IFNULL(f.principal,0)) AS overdue_days_surplus_money,
  -- 逾期利息
  IFNULL(c.interest,0)-IFNULL(f.interest,0) AS overdue_days_interests,
  -- (SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t JOIN tb_yidian_repayment_actual ta ON t.actual_id=ta.log_id JOIN tb_yidian_wind_control_approval tca ON ta.business_id=tca.business_id  where t.fee_type = 5   LIMIT 1) AS overdue_days_interests,
  -- 区域
  a.business_area,
  -- 客户名称
  -- (SELECT b.customer_name FROM tb_yidian_car_personal_information b WHERE b.business_id=a.business_id AND b.ismain_customer=1 LIMIT 1) AS customer_name,
  tbi.RealName AS customer_name,
  -- 业务编号
  a.business_id,
  -- 类型
  c.loan_extension_type,
  -- 出款日期
   (SELECT DATE_FORMAT(d.application_time,'%Y-%m-%d') FROM `tuandai_bm`.tb_yidian_withdrawals_loans_information d WHERE d.issue_id=c.issue_id LIMIT 1) AS application_time,
 -- twli.application_time as application_time,
  -- 借款金额
  IFNULL(a.borrow_money,0) AS borrow_money,
  -- 还款方式
  e.payment_method,
  -- 剩余本金
  IF(IFNULL(a.borrow_money,0)-(SELECT IFNULL(SUM(t.principal),0) FROM tb_yidian_repayment_actual t  WHERE t.business_id=a.business_id AND CONVERT(t.business_after_id, UNSIGNED INTEGER) &lt;=CONVERT(c.business_after_id, UNSIGNED INTEGER))&lt;=0,0,
  IFNULL(a.borrow_money,0)-(SELECT IFNULL(SUM(t.principal),0) FROM tb_yidian_repayment_actual t WHERE t.business_id=a.business_id AND CONVERT(t.business_after_id, UNSIGNED INTEGER) &lt;=CONVERT(c.business_after_id, UNSIGNED INTEGER)))
     AS surplus_money,
  -- 期数
  c.business_after_id,
 -- 逾期天数
  CASE 
  WHEN tt.business_after_id IS NOT NULL AND CONVERT(tt.business_after_id, UNSIGNED INTEGER) &lt;CONVERT(c.business_after_id, UNSIGNED INTEGER) THEN
   0
  WHEN f.repayment_date IS NULL THEN
    IF((TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))&lt;=0,0,TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))
  WHEN f.repayment_date IS NOT NULL THEN
   -- IF(f.overdue_days&lt;=0,0,IF((TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date))&lt;=0,0,TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date)))
      IF(f.overdue_days&lt;=0,0, IF(f.repay_type=1 OR f.repay_type=2 OR repay_type=3,0,IF((TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))&lt;=0,0,TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))))
  END AS overdue_days,

CASE 
  WHEN tt.business_after_id IS NOT NULL AND CONVERT(tt.business_after_id, UNSIGNED INTEGER) &lt;CONVERT(c.business_after_id, UNSIGNED INTEGER) THEN
	  'over'
  WHEN f.repayment_date IS NULL THEN
	    CASE 
		WHEN (TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))>=1 AND (TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))&lt;=30 THEN
		 'M1'
		WHEN (TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))>=31 AND (TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))&lt;=60 THEN
		 'M2'
		WHEN (TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))>=61 AND (TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))&lt;=90 THEN
		'M3'
		WHEN (TO_DAYS(NOW()) - TO_DAYS(c.repayment_date))>=91 THEN
		'M3+'
		ELSE
		 'M0'
		END 
   WHEN f.repayment_date IS NOT NULL THEN
	     CASE 
		WHEN (TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date))>=1 AND (TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date))&lt;=30 THEN
		  IF(f.overdue_days&lt;=0,'M0',IF(f.repay_type=1 OR f.repay_type=2 OR repay_type=3,'M0','M1')) 
		WHEN (TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date))>=31 AND (TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date))&lt;=60 THEN
		 IF(f.overdue_days&lt;=0,'M0',IF(f.repay_type=1 OR f.repay_type=2 OR repay_type=3,'M0','M2')) 
		WHEN (TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date))>=61 AND (TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date))&lt;=90 THEN
		IF(f.overdue_days&lt;=0,'M0',IF(f.repay_type=1 OR f.repay_type=2 OR repay_type=3,'M0','M3')) 
		WHEN (TO_DAYS(f.repayment_date) - TO_DAYS(c.repayment_date))>=91 THEN
		IF(f.overdue_days&lt;=0,'M0',IF(f.repay_type=1 OR f.repay_type=2 OR repay_type=3,'M0','M3+')) 
		ELSE
		 'M0'
		END 
   END AS overdue_stage
,
  -- 应收日期
  DATE_FORMAT(c.repayment_date,'%Y-%m-%d') AS receive_repayment_date,
  -- 应收本金（元）
  IFNULL(c.principal,0) AS receive_principal,
  -- 应收利息（元)
  IFNULL(c.interest,0) AS receive_interest,
  -- 应收GPS月收服务费（元)
   -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_plan_other_fee t WHERE t.fee_name='GPS服务费' AND t.plan_id=c.log_id),0) AS 'receive_gps_month_service',
   gps.fee AS 'receive_gps_month_service',
  -- 应收月保证金（元）
  -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_plan_other_fee t WHERE t.fee_name='月保证金' AND t.plan_id=c.log_id LIMIT 1),0) AS 'receive_month_cash_deposi',
  mon.fee AS 'receive_month_cash_deposi',
  -- 应收月平台服务费（元） 
  -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_plan_other_fee t WHERE t.fee_name='月平台服务费' AND t.plan_id=c.log_id LIMIT 1),0) AS 'receive_month_platform_service',
  platform.fee AS 'receive_month_platform_service',
  -- P2P平台服务费
  -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_plan_other_fee t WHERE t.fee_type=3 AND t.plan_id=c.log_id LIMIT 1),0) AS 'receive_p2p_platform_service',
  p2p.fee AS 'receive_p2p_platform_service',
  -- 应收小计
  IFNULL(c.principal+c.interest,0) AS receive_little,
  -- 应收合计（元）
  (IFNULL(c.principal,0)+IFNULL(c.interest,0)+IFNULL(gps.fee,0)+
  IFNULL(mon.fee,0)+
  IFNULL(platform.fee,0)+
  IFNULL(p2p.fee,0)
  ) AS receive_total,
  -- 实收日期
  DATE_FORMAT(f.repayment_date,'%Y-%m-%d') AS actual_repayment_date,
  -- 实收本金（元）
  IFNULL(f.principal,0) AS actual_principal,
  -- 实收利息（元）
  IFNULL(f.interest,0) AS actual_interest,
  -- 实收GPS月收服务费（元）
   -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_name='GPS服务费' AND t.actual_id=f.log_id ),0) AS 'actual_gps_month_service',
  IFNULL(actual_gps.fee,0) AS 'actual_gps_month_service',
  -- 实收月保证金（元）
  -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_name='月保证金' AND t.actual_id=f.log_id LIMIT 1),0) AS 'actual_month_cash_deposi',
  IFNULL(actual_mon.fee,0) AS  'actual_month_cash_deposi',
  -- 实收月平台服务费（元）
  -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_name='月平台服务费' AND t.actual_id=f.log_id LIMIT 1),0) AS 'actual_month_platform_service',
  IFNULL(actual_platform.fee,0) AS 'actual_month_platform_service',
  -- 实收团贷P2P平台服务费
 -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=3 AND t.actual_id=f.log_id LIMIT 1),0) AS 'actual_p2p_platform_service',
  IFNULL(actual_p2p.fee,0) AS 'actual_p2p_platform_service',
  -- 实收滞纳金（元）
 --  IFNULL((SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=6 AND t.actual_id=f.log_id LIMIT 1),0) AS 'actual_delaying_payment',
 IFNULL(actual_delaying_payment.fee,0) AS 'actual_delaying_payment',
  -- 实收逾期利息
  -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=5 AND t.actual_id=f.log_id LIMIT 1),0) AS 'actual_delaying_interest',
  IFNULL(actual_delaying_interest.fee,0) AS 'actual_delaying_interest',
  -- 实收提前还款违约金（元）
  -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=7 AND t.actual_id=f.log_id LIMIT 1),0) AS 'actual_prepayment_contract',
  IFNULL(actual_prepayment_contract.fee,0) AS 'actual_prepayment_contract',
  -- 实收结余（元）
  -- IFNULL((SELECT t.fee FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=4 AND t.actual_id=f.log_id LIMIT 1),0) AS 'actual_surplus',
  IFNULL(actual_surplus.fee,0) AS 'actual_surplus',
  -- 实收小计
IFNULL(f.principal,0)+IFNULL(f.interest,0) AS actual_little,
  -- 实收合计（元）
(IFNULL(f.principal,0)+IFNULL(f.interest,0)+IFNULL(actual_gps.fee,0)+
IFNULL(actual_mon.fee,0)+
 IFNULL(actual_platform.fee,0)+
IFNULL( actual_p2p.fee,0) +
IFNULL(actual_delaying_payment.fee,0)+
IFNULL(actual_delaying_interest.fee,0)+
IFNULL(actual_prepayment_contract.fee,0)+
IFNULL(actual_surplus.fee,0) 
  ) AS actual_total,
  'admin' AS update_user,
  NOW() AS update_time
FROM   tuandai_bm.tb_yidian_business_information a  
   JOIN  tb_yidian_repayment_plan c
    ON a.business_id = c.business_id 
   JOIN tb_yidian_wind_control_approval e 
    ON a.business_id = e.business_id 
  LEFT JOIN  tb_yidian_repayment_actual  f 
     ON f.business_id = c.business_id
   AND f.business_after_id=c.business_after_id AND f.issue_id=c.issue_id 
    LEFT JOIN (SELECT t.`business_after_id`,t.business_id FROM tb_yidian_repayment_actual t WHERE t.`clear_type`=2 OR t.`clear_type`=3) tt
   ON tt.business_id=c.business_id
  LEFT JOIN (SELECT t.fee,t.plan_id FROM tb_yidian_repayment_plan_other_fee t WHERE t.fee_name='GPS服务费') gps
   ON gps.plan_id=c.log_id
   LEFT JOIN (SELECT t.fee,t.plan_id FROM tb_yidian_repayment_plan_other_fee t WHERE t.fee_name='月保证金') mon
   ON  mon.plan_id=c.log_id 
   LEFT JOIN (SELECT t.fee,t.plan_id FROM tb_yidian_repayment_plan_other_fee t WHERE t.fee_name='月平台服务费') platform
   ON platform.plan_id=c.log_id 
   LEFT JOIN (SELECT t.fee,t.plan_id FROM tb_yidian_repayment_plan_other_fee t WHERE t.fee_type=3) p2p
   ON p2p.plan_id=c.log_id
   LEFT JOIN (SELECT t.fee,t.actual_id FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_name='GPS服务费') actual_gps
    ON actual_gps.actual_id=f.log_id 
   LEFT JOIN (SELECT t.fee,t.actual_id FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_name='月保证金') actual_mon
   ON actual_mon.actual_id=f.log_id
   LEFT JOIN (SELECT t.fee,t.actual_id FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_name='月平台服务费') actual_platform
   ON actual_platform.actual_id=f.log_id
   LEFT JOIN (SELECT t.fee,t.actual_id FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=3) actual_p2p
   ON actual_p2p.actual_id=f.log_id
   LEFT JOIN (SELECT t.fee,t.actual_id FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=6) actual_delaying_payment
   ON actual_delaying_payment.actual_id=f.log_id
   LEFT JOIN (SELECT t.fee,t.actual_id FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=5) actual_delaying_interest
   ON actual_delaying_interest.actual_id=f.log_id
   LEFT JOIN (SELECT t.fee,t.actual_id FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=7) actual_prepayment_contract
   ON actual_prepayment_contract.actual_id=f.log_id
    LEFT JOIN (SELECT t.fee,t.actual_id FROM tb_yidian_repayment_actual_other_fee t WHERE t.fee_type=4) actual_surplus
   ON actual_surplus.actual_id=f.log_id
   LEFT JOIN tb_issue tbi
   ON c.issue_id=tbi.issueId
    ) tt WHERE  tt.overdue_stage!='over';</sql>

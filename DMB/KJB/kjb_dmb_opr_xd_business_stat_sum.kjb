DROP TEMPORARY TABLE IF EXISTS `dmb_business_stat_sum_tmp4`;
CREATE TEMPORARY TABLE `dmb_business_stat_sum_tmp4` (
	`business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务编号',
	`business_type` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务类型',
	`customer_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '客户id',
	`is_extension` INT(11) NULL DEFAULT NULL COMMENT '是否展期',
	`is_issue` INT(11) NULL DEFAULT NULL COMMENT '是否上标',
	`input_dt` VARCHAR(8) NULL DEFAULT NULL COMMENT '受理日期',
	`apply_dt` VARCHAR(8) NULL DEFAULT NULL COMMENT '申请日期',
	`pass_dt` VARCHAR(8) NULL DEFAULT NULL COMMENT '通过日期',
	`sign_dt` VARCHAR(8) NULL DEFAULT NULL COMMENT '签约日期',
	`cur_day_apply_cnt` INT(11) NULL DEFAULT NULL COMMENT '当日申请单笔数',
	`cur_day_pass_cnt` INT(11) NULL DEFAULT NULL COMMENT '当日通过单笔数                        ',
	`cur_day_sign_cnt` INT(11) NULL DEFAULT NULL COMMENT '当日签约单笔数                           ',
	`cur_day_output_cnt` INT(11) NULL DEFAULT NULL COMMENT '当日出款单笔数                           ',
	`cur_day_apply_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日申请单金额',
	`cur_day_pass_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日通过单金额',
	`cur_day_sign_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日签约单金额',
	`cur_day_output_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日出款单金额',
KEY `dmb_business_stat_sum_tmp4_business_id`(business_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8;

INSERT INTO dmb_business_stat_sum_tmp4
select  b.business_id                                                                         as business_id         ,
		b.business_type                                                                       as business_type       ,
		b.customer_id                                                                         as customer_id         ,
		b.is_extension                                                                        as is_extension        ,
		b.issue_type                                                                          as is_issue            ,
		min(date_format(b.input_time,'%Y%m%d'))                                                    as input_dt            ,
		min(date_format(case when b.business_type IN ('房速贷标准件','房速贷非标准件','T600') and bl.`reserve_1` IN ('H0530','B0350','F0530') AND bl.audit_type = 1 then bl.create_time 
		                     when b.business_type NOT IN ('房速贷标准件','房速贷非标准件','T600') and (bl.audit_type = 1 or bl.audit_type =4) then bl.create_time else null end,'%Y%m%d')) as apply_dt ,
		min(date_format(case when b.business_type IN ('房速贷标准件','房速贷非标准件','T600') AND bl.`reserve_1` IN ('F1110','H1110') then bl.create_time  
		                     when b.business_type NOT IN ('房速贷标准件','房速贷非标准件','T600') AND bl.audit_type = 1 and bl.log_type > 530 then bl.create_time else null end,'%Y%m%d')) as pass_dt ,
		min(date_format(case when b.business_type IN ('房速贷标准件','房速贷非标准件','T600') AND bl.`reserve_1` IN ('H1131','F1131') then bl.create_time
               		         when b.business_type NOT IN ('房速贷标准件','房速贷非标准件','T600') AND bl.log_type >= 1131 then bl.create_time else null end,'%Y%m%d'))      as sign_dt ,
		COUNT(DISTINCT case when b.business_type IN ('房速贷标准件','房速贷非标准件','T600') and bl.`reserve_1` IN ('H0530','B0350','F0530') AND bl.audit_type = 1 then b.business_id 
		                    when b.business_type NOT IN ('房速贷标准件','房速贷非标准件','T600') and (bl.audit_type = 1 or bl.audit_type =4) then b.business_id else null end) as cur_day_apply_cnt  ,    
		COUNT(DISTINCT CASE when b.business_type IN ('房速贷标准件','房速贷非标准件','T600') AND bl.`reserve_1` IN ('F1110','H1110') then  b.business_id
		                    when b.business_type NOT IN ('房速贷标准件','房速贷非标准件','T600') AND bl.audit_type = 1 and bl.log_type > 530 then  b.business_id else null end) as cur_day_pass_cnt    ,
		COUNT(DISTINCT case when b.business_type IN ('房速贷标准件','房速贷非标准件','T600') AND bl.`reserve_1` IN ('H1131','F1131') then  b.business_id
               		        when b.business_type NOT IN ('房速贷标准件','房速贷非标准件','T600') AND bl.log_type >= 1131 then  b.business_id else null end)                as cur_day_sign_cnt    ,
		0                                                       							   as cur_day_output_cnt  ,
		max(case when bl.audit_type = 1 or bl.audit_type =4 then b.apply_borrow_money else 0 end)      as cur_day_apply_amt   ,  
		max(case when bl.audit_type = 1 and bl.log_type > 530 then b.apply_borrow_money else 0 end)       as cur_day_pass_amt    ,
		max(case when bl.log_type >= 1131  then b.apply_borrow_money else 0 end )             as cur_day_sign_amt    ,
		0                                                                                     as cur_day_output_amt  
from dmb_business_stat_sum_tmp1 b
join dmb_business_stat_sum_tmp2 bl on bl.car_business_id = b.business_id
group by b.business_id,
		b.business_type,
		b.customer_id,
		b.is_extension,
		b.issue_type;

INSERT INTO dmb_business_stat_sum_tmp4
select  b.business_id                                                                         as business_id         ,
		b.business_type                                                                       as business_type       ,
		b.customer_id                                                                         as customer_id         ,
		b.is_extension                                                                        as is_extension        ,
		b.issue_type                                                                          as is_issue            ,
	    min(date_format(b.input_time,'%Y%m%d'))                                               as input_dt,          
		null										                                          as apply_dt ,      
		null										                                          as pass_dt ,
		null                                                                                  as sign_dt ,
		0 as cur_day_apply_cnt  ,    
		0 as cur_day_pass_cnt    ,
		0 as cur_day_sign_cnt    ,
		count(distinct tt.BUSINESS_ID)                                                        as cur_day_output_cnt  ,
		0 as cur_day_apply_amt   ,  
		0 as cur_day_pass_amt    ,
		0 as cur_day_sign_amt    ,
		sum(tt.out_amt + tt.out_fee)                                                           as cur_day_output_amt  
from dmb_business_stat_sum_tmp1 b
join dmb_business_stat_sum_tmp3 tt on tt.business_id = b.business_id
group by b.business_id,
		b.business_type,
		b.customer_id,
		b.is_extension,
		b.issue_type;


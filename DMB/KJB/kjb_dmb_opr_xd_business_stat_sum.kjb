		min(pass_dt) as   pass_dt          ,
		min(sign_dt) as   sign_dt           ,
		sum(cur_day_apply_cnt) as cur_day_apply_cnt ,    
		sum(cur_day_pass_cnt) as cur_day_pass_cnt   ,
		sum(cur_day_sign_cnt) as cur_day_sign_cnt   ,
		sum(cur_day_output_cnt) as cur_day_output_cnt   ,
		sum(cur_day_apply_amt ) as cur_day_apply_amt,  
		sum(cur_day_pass_amt)  as cur_day_pass_amt    ,
		sum(cur_day_sign_amt) as cur_day_sign_amt    ,
		sum(cur_day_output_amt)  as cur_day_output_amt,  
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'dws_data.dws_evt_xd_business_log..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	,--	更新用户
		DATE_FORMAT('${pt_day}','%Y-%m-%d')	as	part_dt	--	分区日期
from dmb_business_stat_sum_tmp4 tt
group by tt.business_id,
		tt.business_type,
		tt.customer_id,
		tt.is_extension,
		ifnull(is_issue,0) ;



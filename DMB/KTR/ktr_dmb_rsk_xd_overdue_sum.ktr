    <connection>dwd_data</connection>
    <sql>select 
 substring('${p_day}',1,8)  stat_dt,
 substring('${p_day}',1,8)  dat_dt,
t1.business_id,
       sum(t1.overdue_prpa) overdue_prpa,
       sum(t1.overdue_acal) overdue_acal,
       sum(ifnull(t2.overdue_plty,0)) overdue_plty,
 'tuandai_bm' as dw_src_sys,
 'tb_car_business_after_ods...' as dw_src_tbl,
  CURRENT_TIMESTAMP as dw_ins_tm,
  CURRENT_TIMESTAMP as dw_upd_tm,
  'dl' as dw_ins_usr,
  'dl' as dw_upd_usr,
  DATE_FORMAT('${pt_day}','%Y-%m-%d') as part_dt  
from 
(select a.`car_business_id` business_id,
       sum(a.`current_Principa`) overdue_prpa,
       SUM(a.`current_accrual` ) overdue_acal 
FROM ODS_XD_TUANDAI_BM.tb_car_business_after_ods a
WHERE part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')
  and a.`car_business_after_type` ='逾期'
  group by car_business_id
  )t1
left join 
(
select a.`business_id` business_id,
       sum(a.`plan_fee_value`) overdue_plty
FROM ODS_XD_TUANDAI_BM.tb_issue_business_after_detail_ods a
WHERE part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')
  and a.`fee_id` ='79069922-e13a-4229-8656-2a1e19b44879'
  group by business_id
)t2 on t1.business_id = t2.business_id
group by t1.business_id
 ; 
 </sql>

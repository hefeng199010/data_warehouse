    <connection>dwd_data</connection>
    <sql>SELECT 
SUBSTRING('${p_day}',1,8) AS stat_dt	,-- 统计日期                                                                                                                                                                                                                as stat_dt                   ,
DATE_FORMAT(ba.plan_repayment_date,'%Y%m%d') AS dat_dt,
ba.business_id AS business_id,
ba.business_after_id AS cur_repay_period,
xb.business_type AS business_type,
xb.customer_id AS customer_id,
xb.is_extension AS is_extension,
CASE WHEN xb.issue_type = '1' THEN 1 ELSE 0 END AS is_issue,
DATE_FORMAT(xb.input_time,'%Y-%m-%d') AS input_dt,
SUM(CASE WHEN ba.fee_id='b69a84dc-ed67-4c9f-80bf-89ee8efd5167' THEN ba.plan_fee_value END ) AS day_Principa_amt,
SUM(CASE WHEN ba.fee_id='556bce4f-f3a9-4b7a-a8b1-43368bebb49c' THEN ba.plan_fee_value END ) AS day_accrual_amt,
SUM(CASE WHEN ba.fee_id='b163247d-41b7-40db-9b59-7c9337a5c659' THEN ba.plan_fee_value END ) AS day_brh_srv_amt,
0 AS day_extsn_nofsh_srv_amt,
SUM(CASE WHEN ba.fee_id IN ('72f5c955-7a80-4758-a159-779f0e714042','165e9383-13a6-4e63-b00a-9dea6ccaa955') THEN ba.plan_fee_value END ) AS day_pltfm_amt,
SUM(CASE WHEN ba.fee_id IN ('db58adad-cb24-4b69-abba-26eba989a23f') THEN ba.plan_fee_value END ) AS day_grnt_amt              ,
SUM(CASE WHEN ba.fee_id IN ('203e172b-9102-4820-9e06-76a64aa119e3',
							'41c7afc6-833b-4647-912f-0ad5d6adc91f',
							'62231380-f967-49b5-9d86-b7c98cd89ed0',
							'c28230bf-6a8a-42ba-90a0-44f89ec18305'
							) THEN ba.plan_fee_value END ) AS day_mon_brh_srv_amt,
SUM(CASE WHEN ba.fee_id IN ('6529a6b5-f5aa-11e7-94ed-94c69109b34a','d0c27c1d-8963-401d-b909-e08725c43171') THEN ba.plan_fee_value END ) AS day_mon_pltfm_amt,
0   AS day_gps_amt               ,
0	AS day_last_accrual_amt      ,
0	AS day_visit_amt             ,
0	AS day_agency_amt            ,
0	AS day_guarantee_amt         ,
0	AS day_scrty_depst_amt       ,
0	AS day_stop_amt              ,
0	AS day_other_type_amt        ,
0	AS day_margin_amt            ,
0	AS day_consult_amt           ,
0	AS day_act_income_diff_amt   ,
0	AS day_extsn_charge_amt      ,
0	AS day_extsn_nofsh_other_amt ,
0	AS day_extsn_rake_back_amt   ,
0	AS day_annual_verfy_amt      ,
0	AS day_lawyer_amt            ,
0	AS day_trail_amt             ,
0	AS day_adv_settle_amt        ,
0	AS day_mdf_scr_bnf_amt       ,
0	AS day_consult_file_amt      ,
0	AS day_chanel_amt            ,
SUM(CASE WHEN ba.fee_id IN ('06d33476-ae36-4399-a8c7-2b3a3b7fba9b',
							'a275f25d-e4ef-4012-bf39-7478fcabee13',
							'c837a3b9-307e-4341-8487-42ab9ade3751',
							'dea74fa1-0f32-49cd-9954-fa4ccff7d046'
							) THEN ba.plan_fee_value END ) AS day_admin_amt             ,
0 AS day_Litigation_amt        ,
SUM(CASE WHEN ba.fee_id IN ('0b7bec91-722e-4f3c-9677-6620ea61526d','e1409e23-3b4a-48af-85b0-7f38e81ba2a0') THEN ba.plan_fee_value END ) AS day_investigation_amt     ,
0 AS day_acct_mgr_amt          ,
0 AS day_id_depst_amt          ,
SUM(CASE WHEN ba.fee_id IN ('ceb9df0f-9f81-4b99-ba7e-84a3dded506d') THEN ba.plan_fee_value END ) AS day_rake_back_amt         ,
0 AS day_illegal_depst_amt     ,
0 AS day_ovd_brh_srv_amt       ,
0 AS day_ovd_accrual_amt       ,
0 AS day_ovd_income_amt        ,
0 AS day_key_depst_amt         ,
0 AS day_derate_principa_amt   ,
0 AS day_rsk_mgr_amt           ,
0 AS day_late_amt              ,
0 AS day_rush_recb_amt         ,
SUM(IFNULL(CASE WHEN ba.after_fee_type='1' THEN ba.plan_fee_value END,0)) AS day_hj_principa_amt       ,
SUM(IFNULL(CASE WHEN ba.after_fee_type='2' THEN ba.plan_fee_value END,0)) AS day_hj_accrual_amt        ,
SUM(IFNULL(CASE WHEN ba.after_fee_type='3' THEN ba.plan_fee_value END,0)) AS day_hj_srv_amt            ,
SUM(IFNULL(CASE WHEN ba.after_fee_type='4' THEN ba.plan_fee_value END,0)) AS day_hj_other_amt          ,
SUM(IFNULL(CASE WHEN ba.after_fee_type='5' THEN ba.plan_fee_value END,0)) AS day_hj_brk_cntct_amt      ,
SUM(IFNULL(CASE WHEN ba.after_fee_type='6' THEN ba.plan_fee_value END,0)) AS day_hj_rush_recb_amt      ,
'tuandai_bm'	AS	dw_src_sys	,--	来源系统
'DWS_DATA.dws_agr_xd_business_after,DWS_DATA.dws_agr_xd_business..'	AS	dw_src_tbl	,--	来源表
NOW()	AS	dw_ins_tm	,--	插入时间
NOW()	AS	dw_upd_tm	,--	更新时间
'dailiang'	AS	dw_ins_usr	,--	插入用户
'dailiang'	AS	dw_upd_usr,	--	更新用户
DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	--	分区日期
FROM ODS_XD_TUANDAI_BM.tb_car_business_after_detail_ods ba
LEFT JOIN DWS_DATA.dws_agr_xd_business xb
       ON xb.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
	  AND ba.business_id = xb.business_id
WHERE ba.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
GROUP BY DATE_FORMAT(ba.plan_repayment_date,'%Y%m%d') ,
			ba.business_id,
			ba.business_after_id ,
			xb.business_type ,
			xb.customer_id ,
			xb.is_extension ,
			CASE WHEN xb.issue_type = '1' THEN 1 ELSE 0 END  ,
			DATE_FORMAT(xb.input_time,'%Y-%m-%d') </sql>

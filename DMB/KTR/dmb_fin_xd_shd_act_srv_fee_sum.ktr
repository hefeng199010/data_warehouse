    <sql>select substring('20180326',1,8) as stat_dt	,
       tt1.dat_dt,
	   tt1.issue_id,
	   tt1.business_id,
		DATE_FORMAT(tt2.full_issue_date,'%Y-%m-%d') as full_issue_dt,
		tt2.RealName as issue_cut_nm,
		tt2.full_borrow_money as issue_borrow_amt,
		tt3.business_type,--	业务类型
		tt3.is_extension  type_id,--	类型
		tt3.apply_borrow_limit	as borrow_limit,--	借款期限（月）
		tt3.loan_rate	as loan_rate,--	利率(年化)
		tt4.branch_name as branch_nm, -- 上标主体(上标分公司)
	   sum(ifnull(tt1.day_shd_srv_fee_amt,0)) as day_shd_srv_fee_amt,
	   sum(ifnull(tt1.day_act_srv_fee_amt,0)) as day_act_srv_fee_amt,
	   sum(ifnull(tt1.day_tx_srv_fee_amt,0)) as day_tx_srv_fee_amt,
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'DWS_DATA.dws_agr_xd_business_after,DWD_DATA.dwd_evt_core_account_list..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	,--	更新用户
		DATE_FORMAT('2018-03-26','%Y-%m-%d')	as	part_dt	--	分区日期	   
from DMB_FIN_DATA.dmb_fin_xd_shd_act_srv_fee_sum_tmp tt1
left join DWS_DATA.dws_agr_xd_issue_business tt2
       on tt1.issue_id = tt2.issue_id
	  and tt2.part_dt = DATE_FORMAT('20180322','%Y-%m-%d')
left join DWS_DATA.dws_agr_xd_business tt3
       on tt1.business_id = tt3.business_id
	  and tt3.part_dt = DATE_FORMAT('20180322','%Y-%m-%d')
left join (select distinct tt.business_id,branch_name
		   from (
				  select business_id,branch_name
					from DWD_DATA.dwd_cut_xd_business_output tt3
				   where tt3.part_dt=DATE_FORMAT('20180322','%Y-%m-%d')
					 and tt3.BUSINESS_ID not  like '%ZQ%'
				  union all
				  select car_business_extension_id as business_id,branch_name
					from DWD_DATA.dwd_agr_xd_car_business_exhibition tt3
				   where tt3.part_dt=DATE_FORMAT('20180322','%Y-%m-%d')
				 )tt
		  )tt4
	   on tt1.business_id = tt4.business_id
group by substring('20180326',1,8)	,
       tt1.dat_dt,
	   tt1.issue_id,
	   tt1.business_id,
		DATE_FORMAT(tt2.full_issue_date,'%Y-%m-%d') ,
		tt2.RealName,
		tt2.full_borrow_money ,
		tt3.business_type,--	业务类型
		tt3.is_extension ,--	类型
		tt3.apply_borrow_limit,--	借款期限（月）
		tt3.loan_rate,--	利率(年化)
		tt4.branch_name -- 上标主体(上标分公司)</sql>

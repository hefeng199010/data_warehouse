    <connection>dwd_data</connection>
    <sql>select  substring('${p_day}',1,8) as stat_dt	,-- 统计日期                                                                                                                                                                              
		business_id         ,
		business_type       ,
		customer_id         ,
		is_extension        ,
		is_issue            ,
		max(input_dt) as  input_dt           ,
		max(apply_dt) as  apply_dt           ,
		max(pass_dt) as   pass_dt          ,
		max(sign_dt) as   sign_dt           ,
		sum(cur_day_apply_cnt) as cur_day_apply_cnt ,    
		sum(cur_day_pass_cnt) as cur_day_pass_cnt   ,
		sum(cur_day_sign_cnt) as cur_day_sign_cnt   ,
		sum(cur_day_output_cnt) as cur_day_output_cnt   ,
		sum(cur_day_apply_amt ) as cur_day_apply_amt,  
		sum(cur_day_pass_amt)  as cur_day_pass_amt    ,
		sum(cur_day_sign_amt) as cur_day_sign_amt    ,
		sum(cur_day_output_amt)  as cur_day_output_amt,  
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'dws_data.dws_evt_xd_business_log,dws_data.dws_agr_xd_business,dws_data.dws_evt_xd_output_list'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	,--	更新用户
		DATE_FORMAT('${pt_day}','%Y-%m-%d')	as	part_dt	--	分区日期
from (select    b.business_id                                                                         as business_id         ,
				b.business_type                                                                       as business_type       ,
				b.customer_id                                                                         as customer_id         ,
				b.is_extension                                                                        as is_extension        ,
				b.issue_type                                                                          as is_issue            ,
				max(date_format(b.input_time,'%Y%m%d'))                                                    as input_dt            ,
				max(date_format(case when bl.audit_type = 1 or bl.audit_type =4 then bl.create_time else null end,'%Y%m%d')) as apply_dt ,
				max(date_format(case when bl.audit_type = 1 and log_type > 530 then bl.create_time else null end,'%Y%m%d')) as pass_dt ,
				max(date_format(case when bl.log_type >= 1131 then bl.create_time else null end,'%Y%m%d'))      as sign_dt ,
				COUNT(DISTINCT case when bl.audit_type = 1 or bl.audit_type =4 then b.business_id else null end) as cur_day_apply_cnt  ,    
				COUNT(DISTINCT case when bl.audit_type = 1 and log_type > 530 then b.business_id else null end) as cur_day_pass_cnt    ,
				COUNT(DISTINCT case when bl.log_type >= 1131 then b.business_id end)                as cur_day_sign_cnt    ,
				0                                                       							   as cur_day_output_cnt  ,
				max(case when bl.audit_type = 1 or bl.audit_type =4 then b.apply_borrow_money else 0 end)      as cur_day_apply_amt   ,  
				max(case when bl.audit_type = 1 and log_type > 530 then b.apply_borrow_money else 0 end)       as cur_day_pass_amt    ,
				max(case when bl.log_type >= 1131  then b.apply_borrow_money else 0 end )             as cur_day_sign_amt    ,
				0                                                                                     as cur_day_output_amt  
			from DWS_DATA.dws_agr_xd_business b
		    join DWS_DATA.dws_evt_xd_business_log bl
				 on bl.business_id = b.business_id
				and bl.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
			where b.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
			group by b.business_id,
					b.business_type,
					b.customer_id,
					b.is_extension,
					b.issue_type
			
			union all
			
			select  b.business_id                                                                         as business_id         ,
					b.business_type                                                                       as business_type       ,
					b.customer_id                                                                         as customer_id         ,
					b.is_extension                                                                        as is_extension        ,
					b.issue_type                                                                          as is_issue            ,
				    max(date_format(b.input_time,'%Y%m%d'))                                               as input_dt,          
				    null										                                          as apply_dt ,      
				    null										                                          as pass_dt ,
				    null                                                                                  as sign_dt ,
					0 as cur_day_apply_cnt  ,    
					0 as cur_day_pass_cnt    ,
					0 as cur_day_sign_cnt    ,
					count(distinct tt.BUSINESS_ID)                                                        as cur_day_output_cnt  ,
					0 as cur_day_apply_amt   ,  
					0 as cur_day_pass_amt    ,
					0 as cur_day_sign_amt    ,
					sum(tt.out_amt + tt.out_fee)                                                           as cur_day_output_amt  
			from DWS_DATA.dws_agr_xd_business b
			join DWS_DATA.dws_evt_xd_output_list tt
				 on tt.business_id = b.business_id
				and tt.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
			where b.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
			  and tt.out_date is not null
			group by b.business_id,
					b.business_type,
					b.customer_id,
					b.is_extension,
					b.issue_type
   )tt
   group by tt.business_id,
			tt.business_type,
			tt.customer_id,
			tt.is_extension,
			tt.is_issue</sql>

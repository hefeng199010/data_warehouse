    <entry>
      <name>insert_tb_monitor_error_log</name>
      <description/>
      <type>SQL</type>
      <sql>truncate table public_data.tb_monitor_error_log;

insert into public_data.tb_monitor_error_log(duty_id,state_dt,duty_nm,deal_nm,step_desc)
SELECT duty_id,GROUP_CONCAT(state_dt) state_dt,duty_nm,deal_nm,step_desc
FROM (
	SELECT duty_id,state_dt,duty_nm,deal_nm,step_desc
	FROM public_data.ods_state_log  
	WHERE step_desc LIKE '%异常%' 
	  AND create_tm>=DATE_SUB(NOW(),INTERVAL 2 HOUR)
	  AND LENGTH(state_dt)= 10
	  AND (duty_id &lt;&gt; 126 OR duty_id =126 AND SUBSTRING(state_dt,9,2) NOT IN ('00','01','02'))
	  AND duty_id IN (SELECT duty_id
			    FROM  (SELECT duty_id,duty_nm,deal_nm,step_desc,COUNT(0)
				    FROM public_data.ods_state_log  
				   WHERE step_desc LIKE '%异常%' 
				     AND create_tm>=DATE_SUB(NOW(),INTERVAL 2 HOUR)
				     AND LENGTH(state_dt)= 10
				     AND (duty_id &lt;&gt; 126 OR duty_id =126 AND SUBSTRING(state_dt,9,2) NOT IN ('00','01','02'))
				  GROUP BY duty_id,duty_nm,deal_nm,step_desc
				  HAVING COUNT(0)>1
			          )tt
			    )
	  AND step_desc IN (SELECT step_desc
			    FROM  (SELECT duty_id,duty_nm,deal_nm,step_desc,COUNT(0)
				    FROM public_data.ods_state_log  
				   WHERE step_desc LIKE '%异常%' 
				     AND create_tm>=DATE_SUB(NOW(),INTERVAL 2 HOUR)
				     AND LENGTH(state_dt)= 10
				     AND (duty_id &lt;&gt; 126 OR duty_id =126 AND SUBSTRING(state_dt,9,2) NOT IN ('00','01','02'))
				  GROUP BY duty_id,duty_nm,deal_nm,step_desc
				  HAVING COUNT(0)>1
			          )tt
			    )
			    ) tt
			    GROUP BY duty_id,duty_nm,deal_nm,step_desc
UNION ALL
SELECT t.dutyno,t.dutydate AS state_dt,a.dutydemo AS duty_nm,'insert_data' AS deal_nm,'插入数据异常' AS step_desc
FROM public_data.tb_duty_log t
LEFT JOIN public_data.tb_duty_info a ON a.dutyno = t.dutyno
WHERE t.`status` = 0 
  AND t.create_tm >= DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 HOUR),'%Y-%m-%d %H:00:00')
  AND t.create_tm &lt; DATE_FORMAT(NOW(),'%Y-%m-%d %H:00:00')
			    
			    

</sql>

      <description/>
      <type>SQL</type>
      <sql>SET SESSION tmp_table_size=1024*1024*1024;
SET SESSION max_heap_table_size=1024*1024*1024;


truncate table DIM_DATA.dim_prd_xd_business_type;

INSERT INTO DIM_DATA.dim_prd_xd_business_type
SELECT DISTINCT a.`TYPE_ID` AS business_type
, a.`TYPE_NAME` AS business_type_nm
,(CASE 
WHEN a.`TYPE_ID` =  'A100'               THEN '汽车垫资代采'  
WHEN a.`TYPE_ID` =  'T140'               THEN '扶贫贷'        
WHEN a.`TYPE_ID` =  'T160'               THEN '小微企业贷授信'
WHEN a.`TYPE_ID` =  'T170'               THEN '小微企业贷用信'
WHEN a.`TYPE_ID` =  'T251'               THEN '优房贷'        
WHEN a.`TYPE_ID` =  'T290'               THEN '二手车商贷'    
WHEN a.`TYPE_ID` =  'T500'               THEN '车易贷展期'    
WHEN a.`TYPE_ID` =  'T600'               THEN '房速贷展期'    
WHEN a.`TYPE_ID` =  '房速贷标准件'       THEN '房速贷标准件'  
WHEN a.`TYPE_ID` =  '房速贷非标准件'     THEN '房速贷非标准件'
WHEN a.`TYPE_ID` =  '车易贷'             THEN '车易贷'   
ELSE '其他' END) AS   business_mid_type_nm
,(CASE 
WHEN a.`TYPE_ID` =  'A100'               THEN '汽车垫资代采'
WHEN a.`TYPE_ID` =  'T140'               THEN '扶贫贷'      
WHEN a.`TYPE_ID` =  'T160'               THEN '小微企业贷'  
WHEN a.`TYPE_ID` =  'T170'               THEN '小微企业贷'  
WHEN a.`TYPE_ID` =  'T251'               THEN '优房贷'      
WHEN a.`TYPE_ID` =  'T290'               THEN '二手车商贷'  
WHEN a.`TYPE_ID` =  'T500'               THEN '车易贷'      
WHEN a.`TYPE_ID` =  'T600'               THEN '房速贷'      
WHEN a.`TYPE_ID` =  '房速贷标准件'       THEN '房速贷'      
WHEN a.`TYPE_ID` =  '房速贷非标准件'     THEN '房速贷'      
WHEN a.`TYPE_ID` =  '车易贷'             THEN '车易贷'       
ELSE '其他' END) AS   business_big_type_nm  
,NOW() dw_ins_tm
FROM ODS_XD_TUANDAI_BM.`tb_business_type_ods` a
WHERE a.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d');


-- 新增贷后系统业务管理系统的业务类型维度
INSERT INTO DIM_DATA.dim_prd_xd_business_type
SELECT DISTINCT a.`business_type_id` AS business_type
, a.`business_type_name` AS business_type_nm
, a.`business_type_name` AS   business_mid_type_nm
,(CASE 
WHEN a.`business_type_id` =  '12'               THEN '汽车垫资代采'
WHEN a.`business_type_id` =  '13'               THEN '扶贫贷'      
WHEN a.`business_type_id` =  '25'               THEN '小微企业贷'  
WHEN a.`business_type_id` =  '35'               THEN '优房贷'      
WHEN a.`business_type_id` =  '15'               THEN '二手车商贷'  
WHEN a.`business_type_id` =  '9'               THEN '车易贷'      
WHEN a.`business_type_id` =  '1'               THEN '车易贷'      
WHEN a.`business_type_id` =  '11'       THEN '房速贷'      
WHEN a.`business_type_id` =  '2'     THEN '房速贷'      
WHEN a.`business_type_id` =  '9'             THEN '车易贷'     
WHEN a.`business_type_id` =  '20'             THEN '一点车贷'       
ELSE '其他' END) AS   business_big_type_nm  
,NOW() dw_ins_tm
FROM ODS_XD_TUANDAI_BM.`tb_basic_business_type_dh_ods` a
WHERE a.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d');

</sql>

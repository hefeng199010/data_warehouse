/*存在 407 个单在tb_yidian_business_information中不存在 都为CDJK的单
  所以看到这些单的区域 分公司 借款期限 等信息为空
滞纳金=滞纳金+平台滞纳金+逾期服务费；
提前结清违约金 = 提前结清违约金+资产端提前结清违约金
月平台费=月平台费+月资产端服务费
*/

INSERT INTO  DMA_CEO_RPT_DATA.dma_ceo_rpt_fin_revenue_yd_his
(
 stat_dt                              
,actual_repayment_date                
,area_nm                        
,branch_nm                      
,business_type_nm                     
,borrow_limit                     
,repay_type_nm                        
,business_id                      
,customer_nm                          
,borrow_amt                           
,comp_srv_amt                         
,gps_cost_amt                         
,guarantee_amt                        
,actual_month_platform_service        
,actual_p2p_platform_service          
,one_tm_db_fee                        
,ode_acal_amt                         
,current_Breach                       
,trail_amt                            
,day_adv_settle_amt                   
,dw_src_sys                           
,dw_src_tbl                           
,dw_ins_tm                            
,dw_upd_tm                            
,dw_ins_usr                           
,dw_upd_usr                           
,part_dt    
)
SELECT  
 DATE_FORMAT('${pt_day}','%Y-%m-%d')  AS stat_dt                                   -- 统计日期           
,DATE_FORMAT(t.actual_repayment_date,'%Y-%m-%d') AS actual_repayment_date          -- 实收日期              
,ifnull(a.business_area,'一点车贷') AS area_nm                        --    片区                    
,a.branch_name   AS branch_nm                      --    分公司                 
,'一点车贷'      AS business_type_nm               --    业务类型              
,a.borrow_limit                                    --    借款期限      
,CASE WHEN a.repayment_type in (1,2) THEN '先息后本'
      WHEN a.repayment_type = 5      THEN '等额本息' else a.repayment_type END  AS repay_type_nm           -- 还款方式	  
,t.business_id                                                                           --  业务编号              
,per.customer_name AS customer_nm                                                        --  客户名称              
,MAX(IFNULL(a.borrow_money,0))       AS borrow_amt                     --    放款额                 
,SUM(0                             ) AS comp_srv_amt                   --    分公司服务费  是否需要把分公司服务费剔除包括如下3项之和     
,SUM(actual_gps_month_service      ) AS gps_cost_amt                   --    GPS服务费              
,SUM(actual_month_cash_deposi      ) AS guarantee_amt                  --    月保证金              
,SUM(actual_month_platform_service + actual_month_asset_service ) AS actual_month_platform_service  -- 月平台费=月平台费+月资产端服务费        
,SUM(actual_p2p_platform_service   ) AS actual_p2p_platform_service    --    团贷P2P平台服务费  
,SUM(0                             ) AS one_tm_db_fee                  --    担保费    ??             
,SUM(actual_delaying_interest      ) AS ode_acal_amt                   --    逾期利息              
,SUM(actual_delaying_payment       ) AS current_Breach                 --    滞纳金=滞纳金+平台滞纳金+逾期服务费；                 
,SUM(0                             ) AS trail_amt                      --    拖车费   ??              
,SUM(actual_prepayment_contract    ) AS day_adv_settle_amt             --    提前结清违约金 = 提前结清违约金+资产端提前结清违约金     
, 'tuandai_bm' AS dw_src_sys
, 'dmb_fin_yidian_repay_actual_and_other_dtl,tb_yidian_business_information_ods,tb_yidian_car_personal_information_ods' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期
FROM DMB_FIN_DATA.dmb_fin_yidian_repay_actual_and_other_dtl t
LEFT JOIN ODS_XD_TUANDAI_BM.tb_yidian_business_information_ods a 
 ON t.business_id = a.business_id
AND a.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')
LEFT JOIN tb_yidian_car_personal_information_tmp per                -- 主借款人
 ON a.business_id = per.business_id
WHERE t.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')
GROUP BY
 DATE_FORMAT(t.actual_repayment_date,'%Y-%m-%d')            
,ifnull(a.business_area,'一点车贷')                       --    片区                    
,a.branch_name                       --    分公司                           
,a.borrow_limit                      --    借款期限     
,CASE WHEN a.repayment_type in (1,2) THEN '先息后本'
      WHEN a.repayment_type = 5      THEN '等额本息' else a.repayment_type END                 
,t.business_id                    --    业务编号              
,per.customer_name
;

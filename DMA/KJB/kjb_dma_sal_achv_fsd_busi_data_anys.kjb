alter table dma_sal_achv_fsd_busi_data_anys_his truncate partition dma_sal_achv_fsd_busi_data_anys_his_${pt_day};

insert into dma_sal_achv_fsd_busi_data_anys_his
select t1.stat_dt,
	   t1.data_dt,
	   t1.business_id,
	   t2.branch_nm,
	   t2.area_nm,
	  sum(`settle_zdtx_cnt`) settle_zdtx_cnt,
	  sum(`settle_mfjq_cnt`) settle_mfjq_cnt,
	  sum(`settle_yqzc_cnt`) settle_yqzc_cnt,
	  sum(`settle_jqzd_cnt`) settle_jqzd_cnt,
	  sum(`settle_zcjq_cnt`) settle_zcjq_cnt,
	  sum(`settle_hjjq_cnt`) settle_hjjq_cnt,
	  sum(`settle_act_amt`) settle_act_amt,
	  sum(`settle_shd_amt`) settle_shd_amt,
	  sum(mort_spf_cnt) as `mort_spf_cnt`,
	  sum(mort_bs_cnt) as `mort_bs_cnt`,
	  sum(mort_sp_cnt) as `mort_sp_cnt`,
	  sum(mort_xzl_cnt) as `mort_xzl_cnt`,
	  sum(mort_gy_cnt) as `mort_gy_cnt`,
	  sum(mort_other_cnt) as `mort_other_cnt`,
	  sum(fst_pldge_out_cnt) as `fst_pldge_out_cnt`,
	  sum(fst_pldge_out_amt) as `fst_pldge_out_amt`,
	  sum(scd_pldge_out_cnt) as `scd_pldge_out_cnt`,
	  sum(scd_pldge_out_amt) as `scd_pldge_out_amt`,
	  sum(mny_range_less_one_cnt) as `mny_range_less_one_cnt`,
	  sum(mny_range_one_to_two_cnt) as `mny_range_one_to_two_cnt`,
	  sum(mny_range_two_to_thd_cnt) as `mny_range_two_to_thd_cnt`,
	  sum(mny_range_more_thd_cnt) as `mny_range_more_thd_cnt`,
	  sum(mny_range_less_one_amt) as `mny_range_less_one_amt`,
	  sum(mny_range_one_to_two_amt) as `mny_range_one_to_two_amt`,
	  sum(mny_range_two_to_thd_amt) as `mny_range_two_to_thd_amt`,
	  sum(mny_range_more_thd_amt) as `mny_range_more_thd_amt`,
	 'tuandai_bm' AS dw_src_sys,
	 'dws_data.dws_agr_xd_business' AS dw_src_tbl,
	  CURRENT_TIMESTAMP AS dw_ins_tm,
	  CURRENT_TIMESTAMP AS dw_upd_tm,
	  'dailiang' AS dw_ins_usr,
	  'dailiang' AS dw_upd_usr,
	  DATE_FORMAT('${pt_day}','%Y-%m-%d') AS part_dt
from 
(
		select   
			  SUBSTRING('${p_day}',1,8)  `stat_dt`,
			  `data_dt`,
			  business_id,
			  `branch_id`,
			  sum(`settle_zdtx_cnt`) settle_zdtx_cnt,
			  sum(`settle_mfjq_cnt`) settle_mfjq_cnt,
			  sum(`settle_yqzc_cnt`) settle_yqzc_cnt,
			  sum(`settle_jqzd_cnt`) settle_jqzd_cnt,
			  sum(`settle_zcjq_cnt`) settle_zcjq_cnt,
			  sum(`settle_hjjq_cnt`) settle_hjjq_cnt,
			  sum(`settle_act_amt`) settle_act_amt,
			  sum(`settle_shd_amt`) settle_shd_amt,
			  0 as `mort_spf_cnt`,
			  0 as `mort_bs_cnt`,
			  0 as `mort_sp_cnt`,
			  0 as `mort_xzl_cnt`,
			  0 as `mort_gy_cnt`,
			  0 as `mort_other_cnt`,
			  0 as `fst_pldge_out_cnt`,
			  0 as `fst_pldge_out_amt`,
			  0 as `scd_pldge_out_cnt`,
			  0 as `scd_pldge_out_amt`,
			  0 as `mny_range_less_one_cnt`,
			  0 as `mny_range_one_to_two_cnt`,
			  0 as `mny_range_two_to_thd_cnt`,
			  0 as `mny_range_more_thd_cnt`,
			  0 as `mny_range_less_one_amt`,
			  0 as `mny_range_one_to_two_amt`,
			  0 as `mny_range_two_to_thd_amt`,
			  0 as `mny_range_more_thd_amt`
		from dma_busi_data_tmp2 
		group by `data_dt`,
		       business_id,
			  `branch_id`
		union all
		select    
			  SUBSTRING('${p_day}',1,8)  `stat_dt`,
			  `data_dt`,
			  business_id,
			  `branch_id`,
			  0 as settle_zdtx_cnt,
			  0 as settle_mfjq_cnt,
			  0 as settle_yqzc_cnt,
			  0 as settle_jqzd_cnt,
			  0 as settle_zcjq_cnt,
			  0 as settle_hjjq_cnt,
			  0 as settle_act_amt,
			  0 as settle_shd_amt,
			  sum(mort_spf_cnt) as `mort_spf_cnt`,
			  sum(mort_bs_cnt) as `mort_bs_cnt`,
			  sum(mort_sp_cnt) as `mort_sp_cnt`,
			  sum(mort_xzl_cnt) as `mort_xzl_cnt`,
			  sum(mort_gy_cnt) as `mort_gy_cnt`,
			  sum(mort_other_cnt) as `mort_other_cnt`,
			  sum(fst_pldge_out_cnt) as `fst_pldge_out_cnt`,
			  sum(fst_pldge_out_amt) as `fst_pldge_out_amt`,
			  sum(scd_pldge_out_cnt) as `scd_pldge_out_cnt`,
			  sum(scd_pldge_out_amt) as `scd_pldge_out_amt`,
			  sum(mny_range_less_one_cnt) as `mny_range_less_one_cnt`,
			  sum(mny_range_one_to_two_cnt) as `mny_range_one_to_two_cnt`,
			  sum(mny_range_two_to_thd_cnt) as `mny_range_two_to_thd_cnt`,

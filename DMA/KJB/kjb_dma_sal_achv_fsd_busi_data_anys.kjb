DROP TEMPORARY TABLE IF EXISTS `dma_busi_data_tmp1`;
CREATE TEMPORARY TABLE `dma_busi_data_tmp1` (
	`business_id` VARCHAR(50) NOT NULL DEFAULT '' COMMENT '业务单号',
	`settle_zdtx_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-转单同行单数',
	`settle_mfjq_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-卖方结清单数',
	`settle_yqzc_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-逾期转出单数',
	`settle_jqzd_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-结清再贷单数',
	`settle_zcjq_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-正常结清单数',
	`settle_hjjq_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-合计结清单数',
	KEY `dma_busi_data_tmp1_business_id` (`business_id`)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert into dma_busi_data_tmp1
select a.`business_id`,
       CASE WHEN a.settle_status ='转单同行' THEN 1 ELSE 0 END  AS settle_zdtx_cnt,
       CASE WHEN a.settle_status ='卖房' THEN 1 ELSE 0 END  AS settle_mfjq_cnt,
       CASE WHEN a.settle_status ='逾期转出' THEN 1 ELSE 0 END  AS settle_yqzc_cnt,
       CASE WHEN a.settle_status ='结清再贷' THEN 1 ELSE 0 END  AS settle_jqzd_cnt,
       CASE WHEN a.settle_status ='正常结清' THEN 1 ELSE 0 END  AS settle_zcjq_cnt,
       1 AS settle_hjjq_cnt
from ODS_XD_TUANDAI_BM.tb_business_extension_ods a
where a.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
and ifnull(a.settle_status,'') != '';



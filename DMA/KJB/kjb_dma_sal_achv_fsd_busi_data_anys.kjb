 DROP TEMPORARY TABLE IF EXISTS `dma_busi_data_tmp5`;
CREATE TEMPORARY TABLE `dma_busi_data_tmp5` (
	`business_id` VARCHAR(50) NOT NULL DEFAULT '' COMMENT '业务单号',
	`branch_id` VARCHAR(50) NOT NULL DEFAULT '' COMMENT '业务单号',
	`data_dt` VARCHAR(10) NULL DEFAULT NULL COMMENT '结清日期',
	`mort_spf_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-商品房单数',
	`mort_bs_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-别墅单数',
	`mort_sp_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-商铺单数',
	`mort_xzl_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-写字楼单数',
	`mort_gy_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-公寓单数',
	`mort_other_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-其他单数',
	`fst_pldge_out_cnt` INT(12) NULL DEFAULT NULL COMMENT '一抵-放款单数',
	`fst_pldge_out_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '一抵-放款金额',
	`scd_pldge_out_cnt` INT(12) NULL DEFAULT NULL COMMENT '二抵-放款单数',
	`scd_pldge_out_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '二抵-放款金额',
	`mny_range_less_one_cnt` INT(12) NULL DEFAULT NULL COMMENT '业务借款金额区间-借款总额≤100万单数',
	`mny_range_one_to_two_cnt` INT(12) NULL DEFAULT NULL COMMENT '业务借款金额区间-100＜借款总额≤200万单数',
	`mny_range_two_to_thd_cnt` INT(12) NULL DEFAULT NULL COMMENT '业务借款金额区间-200＜借款总额≤300万单数',
	`mny_range_more_thd_cnt` INT(12) NULL DEFAULT NULL COMMENT '业务借款金额区间-300＜借款总额单数',
	`mny_range_less_one_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '业务借款金额区间-借款总额≤100万金额',
	`mny_range_one_to_two_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '业务借款金额区间-100＜借款总额≤200万金额',
	`mny_range_two_to_thd_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '业务借款金额区间-200＜借款总额≤300万金额',
	`mny_range_more_thd_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '业务借款金额区间-300＜借款总额金额',
	KEY `dma_busi_data_tmp5_business_id` (`business_id`)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert into dma_busi_data_tmp5
select t1.business_id,
       t2.branch_id,
	    DATE_FORMAT(t1.full_issue_date,'%Y-%m-%d')  as data_dt,
	   case when t3.mort_spf_cnt = 1 then 1 else 0 end mort_spf_cnt,
       CASE WHEN t3.mort_bs_cnt = 1 THEN 1 ELSE 0 END mort_bs_cnt,
       CASE WHEN t3.mort_sp_cnt = 1 THEN 1 ELSE 0 END mort_sp_cnt,
       CASE WHEN t3.mort_xzl_cnt = 1 THEN 1 ELSE 0 END mort_xzl_cnt,
       CASE WHEN t3.mort_gy_cnt = 1 THEN 1 ELSE 0 END mort_gy_cnt,
       CASE WHEN t3.mort_other_cnt = 1 THEN 1 ELSE 0 END mort_other_cnt,
	   case when t3.dy_flg = '一抵' THEN 1 ELSE 0 end fst_pldge_out_cnt,
	   case when t3.dy_flg = '一抵' THEN ifnull(t1.full_borrow_money,0) ELSE 0 end fst_pldge_out_amt,
	   case when t3.dy_flg = '二抵' THEN 1 ELSE 0 end scd_pldge_out_cnt,
	   case when t3.dy_flg = '二抵' THEN ifnull(t1.full_borrow_money,0) ELSE 0 end scd_pldge_out_amt,
	   case when t1.full_borrow_money >=0 and t1.full_borrow_money &lt;=1000000 then 1 else 0 end as mny_range_less_one_cnt,
	   case when t1.full_borrow_money >1000000 and t1.full_borrow_money &lt;=2000000 then 1 else 0 end as mny_range_one_to_two_cnt,
	   case when t1.full_borrow_money >2000000 and t1.full_borrow_money &lt;=3000000 then 1 else 0 end as mny_range_two_to_thd_cnt,
	   case when t1.full_borrow_money >3000000 then 1 else 0 end as mny_range_more_thd_cnt,
	   case when t1.full_borrow_money >=0 and t1.full_borrow_money &lt;=1000000 then t1.full_borrow_money else 0 end as mny_range_less_one_amt,
	   case when t1.full_borrow_money >1000000 and t1.full_borrow_money &lt;=2000000 then t1.full_borrow_money else 0 end as mny_range_one_to_two_amt,
	   case when t1.full_borrow_money >2000000 and t1.full_borrow_money &lt;=3000000 then t1.full_borrow_money else 0 end as mny_range_two_to_thd_amt,
	   case when t1.full_borrow_money >3000000 then t1.full_borrow_money else 0 end as mny_range_more_thd_amt
from dma_busi_data_tmp3 t1
join dma_busi_data_tmp0 t2 on t1.business_id = t2.business_id
left join dma_busi_data_tmp4 t3 on t1.business_id = t3.business_id;
  


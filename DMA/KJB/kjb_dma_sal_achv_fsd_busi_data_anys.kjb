DROP TEMPORARY TABLE IF EXISTS `dma_busi_data_tmp4`;
CREATE TEMPORARY TABLE `dma_busi_data_tmp4` (
	`business_id` VARCHAR(50) NOT NULL DEFAULT '' COMMENT '业务单号',
	`dy_flg` VARCHAR(50) NULL DEFAULT NULL COMMENT '主办人编号',
	`mort_spf_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-商品房单数',
	`mort_bs_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-别墅单数',
	`mort_sp_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-商铺单数',
	`mort_xzl_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-写字楼单数',
	`mort_gy_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-公寓单数',
	`mort_other_cnt` INT(12) NULL DEFAULT NULL COMMENT '抵押物类型-其他单数',
	KEY `dma_busi_data_tmp4_business_id` (`business_id`)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert into dma_busi_data_tmp4
SELECT BUSINESS_ID,
       CASE WHEN SUM(IFNULL(BORROW_BALANCE,0))>0 THEN '二抵' ELSE '一抵' END AS dy_flg ,
       MAX(case when ifnull(a.RESERVE_5,'未知') ='商品房' then 1 else 0 end) mort_spf_cnt,
       MAX(CASE WHEN ifnull(a.RESERVE_5,'未知') ='别墅' THEN 1 ELSE 0 END) mort_bs_cnt,
       MAX(CASE WHEN ifnull(a.RESERVE_5,'未知') ='商铺' THEN 1 ELSE 0 END) mort_sp_cnt,
       MAX(CASE WHEN ifnull(a.RESERVE_5,'未知') ='写字楼' THEN 1 ELSE 0 END) mort_xzl_cnt,
       MAX(CASE WHEN ifnull(a.RESERVE_5,'未知') ='公寓' THEN 1 ELSE 0 END) mort_gy_cnt,
       MAX(CASE WHEN ifnull(a.RESERVE_5,'未知') not in ('公寓','写字楼','商铺','别墅','商品房') THEN 1 ELSE 0 END) mort_other_cnt
FROM ODS_XD_TUANDAI_BM.tb_fsd_house_ods a
WHERE part_dt=DATE_FORMAT('${pt_yesterday}','%Y-%m-%d')
GROUP BY BUSINESS_ID  ;
  
 

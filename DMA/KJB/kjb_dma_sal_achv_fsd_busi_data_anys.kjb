			  sum(mny_range_two_to_thd_amt) as `mny_range_two_to_thd_amt`,
			  sum(mny_range_more_thd_amt) as `mny_range_more_thd_amt`
		from dma_busi_data_tmp5 
		group by `data_dt`,
		      business_id,
			  `branch_id`
)t1
left join (select * from DIM_DATA.dim_org_info where dw_src_sys ='xd') t2 on t1.branch_id = t2.branch_id
group by t1.stat_dt,
	   t1.data_dt,
	   t1.business_id,
	   t2.branch_nm,
	   t2.area_nm
;


	

DROP TEMPORARY TABLE IF EXISTS `dma_busi_data_tmp2`;
CREATE TEMPORARY TABLE `dma_busi_data_tmp2` (
	`business_id` VARCHAR(50) NOT NULL DEFAULT '' COMMENT '业务单号',
	`branch_id` VARCHAR(50) NOT NULL DEFAULT '' COMMENT '业务单号',
	`data_dt` VARCHAR(10) NULL DEFAULT NULL COMMENT '结清日期',
	`settle_zdtx_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-转单同行单数',
	`settle_mfjq_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-卖方结清单数',
	`settle_yqzc_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-逾期转出单数',
	`settle_jqzd_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-结清再贷单数',
	`settle_zcjq_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-正常结清单数',
	`settle_hjjq_cnt` INT(12) NULL DEFAULT NULL COMMENT '结清原因-合计结清单数',
	`settle_act_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '结清原因-合计结清实还总金额',
	`settle_shd_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '结清原因-合计结清应还总金额',
	KEY `dma_busi_data_tmp2_business_id` (`business_id`)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert into dma_busi_data_tmp2
select a.`car_business_id` business_id,
       c.branch_id,
       MAX(DATE_FORMAT(a.`fatct_replayDate`,'%Y-%m-%d')) as data_dt,
	   sum(b.settle_zdtx_cnt) as settle_zdtx_cnt,
	   sum(b.settle_mfjq_cnt) as settle_mfjq_cnt,
	   sum(b.settle_yqzc_cnt) as settle_yqzc_cnt,
	   sum(b.settle_jqzd_cnt) as settle_jqzd_cnt,
	   sum(b.settle_zcjq_cnt) as settle_zcjq_cnt,
	   sum(b.settle_hjjq_cnt) as settle_hjjq_cnt,
       SUM(a.`fact_principa`) as settle_act_amt,
       SUM(a.`current_Principa`) as settle_shd_amt
from ODS_XD_TUANDAI_BM.tb_car_business_after_ods a 
join dma_busi_data_tmp1 b on a.car_business_id = b.business_id
join dma_busi_data_tmp0 c on a.car_business_id = c.business_id
where part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
group by a.car_business_id;



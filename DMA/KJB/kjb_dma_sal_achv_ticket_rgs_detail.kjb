DROP TEMPORARY TABLE IF EXISTS `dma_sal_achv_ticket_rgs_detail_tmp`;
CREATE TEMPORARY TABLE `dma_sal_achv_ticket_rgs_detail_tmp` (
  `stat_dt` varchar(8) DEFAULT NULL COMMENT '统计日期',
  `issue_id` varchar(50) DEFAULT NULL COMMENT '标的ID',
  `business_id` varchar(50) DEFAULT NULL COMMENT '业务编号',
  `business_after_id` varchar(10) DEFAULT NULL COMMENT '期数',
  `shd_repayment_date` varchar(10) DEFAULT NULL COMMENT '应收_应收日期',
  `act_repayment_date` varchar(10) DEFAULT NULL COMMENT '实收_实收日期',
  `full_issue_date` varchar(10) DEFAULT NULL COMMENT '满标时间',
  `origin_full_issue_date` varchar(10) DEFAULT NULL COMMENT '原业务满标时间',
  `zq_full_issue_date` varchar(10) DEFAULT NULL COMMENT '展期业务满标时间',
  `issue_split_type` varchar(50) DEFAULT NULL COMMENT '是否共借标',
  `is_tuandai_repay` varchar(50) DEFAULT NULL COMMENT '是否需要进行平台还款',
  `is_main_cust` varchar(50) DEFAULT NULL COMMENT '是否主借款人',
  `business_type` varchar(50) DEFAULT NULL COMMENT '业务类型',
  `apply_borrow_limit` int(11) DEFAULT NULL COMMENT '申请借款期限',
  `loan_rate` decimal(10,4) DEFAULT NULL COMMENT '年化利率',
  `branch_nm` varchar(50) DEFAULT NULL COMMENT '分公司',
  `area_nm` varchar(50) DEFAULT NULL COMMENT '片区',
  `platform_id` varchar(50) DEFAULT NULL COMMENT '平台编号 团贷网、你我金融',
  `branch_name` varchar(100) DEFAULT NULL COMMENT '合同主体',
  `issue_branch_name` varchar(100) DEFAULT NULL COMMENT '上标主体',
  `type_nm` varchar(50) DEFAULT NULL COMMENT '类型 新增、展期',
  `RealName` varchar(50) DEFAULT NULL COMMENT '客户名称（甲方姓名）',
  `full_borrow_money` decimal(10,2) DEFAULT NULL COMMENT '满标金额',
  `shd_total_fee_amt` decimal(12,2) DEFAULT NULL COMMENT '应收_分公司费用',
  `shd_fisrt_fee_amt` decimal(12,2) DEFAULT NULL COMMENT '应收_分公司服务费',
  `shd_mon_fee_amt` decimal(12,2) DEFAULT NULL COMMENT '应收_月收分公司服务费',
  `shd_gps_fee_amt` decimal(12,2) DEFAULT NULL COMMENT '应收_GPS费用',
  `act_total_fee_amt` varchar(10) DEFAULT NULL COMMENT '实收_分公司费用',
  `act_fisrt_fee_amt` decimal(12,2) DEFAULT NULL COMMENT '实收_分公司服费用',
  `act_mon_fee_amt` decimal(12,2) DEFAULT NULL COMMENT '实收_月收分公司服务费',
  `act_gps_fee_amt` decimal(12,2) DEFAULT NULL COMMENT '实收_GPS费用',
  `act_adv_settle_fee_amt` decimal(12,2) DEFAULT NULL COMMENT '实收_提前结清服务费违约金',
  `dw_src_sys` varchar(50) DEFAULT NULL COMMENT '数据来源',
  `dw_src_tbl` varchar(130) NOT NULL DEFAULT '',
  `dw_ins_tm` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `dw_upd_tm` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `dw_ins_usr` varchar(50) NOT NULL DEFAULT '',
  `dw_upd_usr` varchar(50) NOT NULL DEFAULT ''
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='开票登记表'
;
  
insert into dma_sal_achv_ticket_rgs_detail_tmp 
select substring('${p_day}',1,8) as stat_dt	,
       t1.issue_id,
	   t1.business_id,
	   t1.business_after_id,
	   t1.shd_repayment_date,
	   case when t1.business_after_id like '%00%' then t2.full_issue_date else t1.act_repayment_date end act_repayment_date,
	   t2.full_issue_date,
	   t2.origin_full_issue_date,
	   t2.zq_full_issue_date,
	   t3.issue_split_type,
	   case when t3.is_tuandai_repay = 1 then '是' when  t3.is_tuandai_repay = 2 then '否' else '未知' end as is_tuandai_repay,
	   case when t3.customer_name = t2.RealName then '是' else '否' end as is_main_cust,
        case when FIND_IN_SET( t3.business_type,'房速贷标准件,房速贷非标准件,T600') then '房速贷'  
			 when ifnull(t3.business_type_detail,'')='T136' then '农机贷'
			 when FIND_IN_SET( t3.business_type,'车易贷,T500') then '车易贷' else t6.business_type_nm end  as business_type,
	   t3.apply_borrow_limit	,--	借款期限（月）
	   t3.loan_rate,
	   t3.branch_nm,
	   t3.area_nm,
	   case when t3.platform_id = 1 then  '团贷网' when t3.platform_id = 2 then  '你我金融'  else '未知' end as platform_id,
	   t4.branch_name,
	   t5.branch_name as issue_branch_name,
	   case when t1.business_after_id like '%ZQ%' then '展期' else '新增' end as type_nm,
		t2.RealName,
		t2.full_borrow_money,
	   sum(t1.shd_total_fee_amt),
	   sum(t1.shd_fisrt_fee_amt),
	   sum(t1.shd_mon_fee_amt),
	   sum(t1.shd_gps_fee_amt),
	   sum(t1.act_total_fee_amt),
	   sum(t1.act_fisrt_fee_amt),
	   sum(t1.act_mon_fee_amt),
	   sum(t1.act_gps_fee_amt),
	   sum(t1.act_adv_settle_fee_amt),
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'DWS_DATA.dws_agr_xd_business_after..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	
from ticket_rgs_tmp3 t1
left join ticket_rgs_tmp6 t2 on t1.issue_id = t2.issue_id
left join ticket_rgs_tmp4 t3 on t1.business_id = t3.business_id
left join ticket_rgs_tmp7 t4 on t1.business_id = t4.business_id
left join ticket_rgs_tmp8 t5 on t1.issue_id = t5.issue_id
left join DIM_DATA.`dim_prd_xd_business_type` t6 on t3.business_type = t6.business_type
group by t1.issue_id,
	   t1.business_id,
	   t1.business_after_id,
	   t1.shd_repayment_date,
	   case when t1.business_after_id like '%00%' then t2.full_issue_date else t1.act_repayment_date end ,
	   t2.full_issue_date,
	   t2.origin_full_issue_date,
	   t2.zq_full_issue_date,
	   t3.issue_split_type,
	   case when t3.is_tuandai_repay = 1 then '是' when  t3.is_tuandai_repay = 2 then '否' else '未知' end ,
	   case when t3.customer_name = t2.RealName then '是' else '否' end ,
        case when FIND_IN_SET( t3.business_type,'房速贷标准件,房速贷非标准件,T600') then '房速贷'  
			 when ifnull(t3.business_type_detail,'')='T136' then '农机贷'
			 when FIND_IN_SET( t3.business_type,'车易贷,T500') then '车易贷' else '其他' end  ,
	   t3.apply_borrow_limit	,--	借款期限（月）
	   t3.loan_rate,
	   t3.branch_nm,
	   t3.area_nm,
	   case when t3.platform_id = 1 then  '团贷网' when t3.platform_id = 2 then  '你我金融'  else '未知' end ,
	   t4.branch_name,
	   t5.branch_name ,
	   case when t1.business_after_id like '%ZQ%' then '展期' else '新增' end ,
		t2.RealName,
		t2.full_borrow_money;


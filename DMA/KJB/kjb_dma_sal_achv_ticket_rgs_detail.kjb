insert into ticket_rgs_tmp2 
select issue_id,
	   business_id,
	   business_after_id,
	   shd_repayment_date,
	   sum(shd_total_fee_amt) as shd_total_fee_amt,
	   sum(shd_total_fee_amt)-  sum(shd_mon_fee_amt) - sum(shd_gps_fee_amt) as shd_fisrt_fee_amt,
	   sum(shd_mon_fee_amt) as shd_mon_fee_amt,
	   sum(shd_gps_fee_amt) as shd_gps_fee_amt,
	   act_repayment_date,
	   sum(act_total_fee_amt) as act_total_fee_amt,
	   sum(act_total_fee_amt)-  sum(act_mon_fee_amt) - sum(act_gps_fee_amt) as act_fisrt_fee_amt,
	   sum(act_mon_fee_amt) as act_mon_fee_amt,
	   sum(act_gps_fee_amt) as act_gps_fee_amt,
	   sum(act_adv_settle_fee_amt) as act_adv_settle_fee_amt
from 
(  
SELECT  ba.issue_id,
        ba.business_id,
		ba.business_after_id,
		DATE_FORMAT(plan_repayment_date,'%Y-%m-%d') AS shd_repayment_date,
		SUM(IFNULL(ba.plan_fee_value,0)) AS shd_total_fee_amt,-- 应收_分公司费用
		SUM(CASE WHEN fee_name LIKE '%月收分公司服务费%' THEN  IFNULL(ba.plan_fee_value,0) ELSE 0 END) AS shd_mon_fee_amt,-- 应收_月收分公司服务费
		SUM(CASE WHEN fee_id IN ('129161a8-4a48-4f23-b04b-6e0807830899','339fcc81-1e85-4291-87d7-e3c34a8f84f2') THEN  IFNULL(ba.plan_fee_value,0) ELSE 0 END) AS shd_gps_fee_amt,-- 应收_GPS费用
		-- DATE_FORMAT(actual_repayment_date,'%Y-%m-%d') AS act_repayment_date,
		DATE_FORMAT(case when business_after_id like '%00%' then plan_repayment_date else actual_repayment_date end,'%Y-%m-%d') as act_repayment_date,
		SUM(CASE WHEN business_after_id LIKE '%00%' THEN IFNULL(ba.plan_fee_value,0) ELSE IFNULL(ba.actual_fee_value,0) END ) AS act_total_fee_amt,-- 实收_分公司费用
		SUM(CASE WHEN business_after_id LIKE '%00%' AND fee_name LIKE '%月收分公司服务费%' THEN IFNULL(ba.plan_fee_value,0)
                 WHEN business_after_id NOT LIKE '%00%' AND fee_name LIKE '%月收分公司服务费%' THEN IFNULL(ba.actual_fee_value,0) ELSE 0 END) AS act_mon_fee_amt,-- 实收_月收分公司服务费
		SUM(CASE WHEN  business_after_id LIKE '%00%' AND fee_id IN ('129161a8-4a48-4f23-b04b-6e0807830899','339fcc81-1e85-4291-87d7-e3c34a8f84f2') THEN  IFNULL(ba.plan_fee_value,0)
		     WHEN  business_after_id NOT LIKE '%00%' AND fee_id IN ('129161a8-4a48-4f23-b04b-6e0807830899','339fcc81-1e85-4291-87d7-e3c34a8f84f2') THEN IFNULL(ba.actual_fee_value,0) ELSE 0 END) AS act_gps_fee_amt,-- 实收_GPS费用
	    0 AS act_adv_settle_fee_amt -- 实收_提前结清服务费违约金
FROM ODS_XD_TUANDAI_BM.tb_issue_business_after_detail_ods ba
WHERE ba.part_dt =  DATE_FORMAT('${pt_day}','%Y-%m-%d')
  AND ba.fee_id IN (SELECT fee_item_id FROM ticket_rgs_tmp1)
  GROUP BY  ba.issue_id,
        ba.business_id,
		ba.business_after_id,
		DATE_FORMAT(plan_repayment_date,'%Y-%m-%d'),
		DATE_FORMAT(case when business_after_id like '%00%' then plan_repayment_date else actual_repayment_date end,'%Y-%m-%d')
union all
select  ba.issue_id,
        ba.business_id,
		ba.business_after_id,
		DATE_FORMAT(plan_repayment_date,'%Y-%m-%d') as shd_repayment_date,
		0 as shd_total_fee_amt,-- 应收_分公司费用
		0 as shd_mon_fee_amt,-- 应收_月收分公司服务费
		0 as shd_gps_fee_amt,-- 应收_GPS费用
		-- DATE_FORMAT(actual_repayment_date,'%Y-%m-%d') as act_repayment_date,
		DATE_FORMAT(case when business_after_id like '%00%' then plan_repayment_date else actual_repayment_date end,'%Y-%m-%d') as act_repayment_date,
		0 as act_total_fee_amt,-- 实收_分公司费用
		0 as act_mon_fee_amt,-- 实收_月收分公司服务费
		0 as act_gps_fee_amt,-- 实收_GPS费用
		sum(case when business_after_id ='00' then IFNULL(ba.plan_fee_value,0) else IFNULL(ba.actual_fee_value,0) end) as act_adv_settle_fee_amt -- 实收_提前结清服务费违约金
from ODS_XD_TUANDAI_BM.tb_issue_business_after_detail_ods ba
where ba.part_dt =  DATE_FORMAT('${pt_day}','%Y-%m-%d')
  -- and ba.fee_id='65bf9587-37b4-11e7-9482-000c2928bb0d' 
   and ba.fee_id='9e0a6fc1-4448-11e8-b4f8-0242ac110003'  -- 20180814 更改为 提前结清分公司服务费违约金
  group by  ba.issue_id,
        ba.business_id,
		ba.business_after_id,
		DATE_FORMAT(plan_repayment_date,'%Y-%m-%d'),
		DATE_FORMAT(case when business_after_id like '%00%' then plan_repayment_date else actual_repayment_date end,'%Y-%m-%d') 
  )t
  group by issue_id,
	   business_id,
	   business_after_id,
	   shd_repayment_date,
	   act_repayment_date;

	   
	   
INSERT INTO ticket_rgs_tmp2 
SELECT issue_id,
	   business_id,
	   business_after_id,
	   shd_repayment_date,
	   SUM(shd_total_fee_amt) AS shd_total_fee_amt,
	   SUM(shd_total_fee_amt)-  SUM(shd_mon_fee_amt) - SUM(shd_gps_fee_amt) AS shd_fisrt_fee_amt,
	   SUM(shd_mon_fee_amt) AS shd_mon_fee_amt,
	   SUM(shd_gps_fee_amt) AS shd_gps_fee_amt,
	   act_repayment_date,
	   SUM(act_total_fee_amt) AS act_total_fee_amt,
	   SUM(act_total_fee_amt)-  SUM(act_mon_fee_amt) - SUM(act_gps_fee_amt) AS act_fisrt_fee_amt,
	   SUM(act_mon_fee_amt) AS act_mon_fee_amt,
	   SUM(act_gps_fee_amt) AS act_gps_fee_amt,
	   SUM(act_adv_settle_fee_amt) AS act_adv_settle_fee_amt
FROM 
(  
SELECT t3.`project_id` AS issue_id,
       t1.`business_id`,
       t2.`after_id` AS business_after_id,
       DATE_FORMAT(t1.due_date,'%Y-%m-%d') AS shd_repayment_date,
       SUM(IFNULL(t1.`proj_plan_amount`,0)) AS shd_total_fee_amt,-- 应收_分公司费用
       SUM(CASE WHEN t1.`fee_id` = '203e172b-9102-4820-9e06-76a64aa119e3' THEN  IFNULL(t1.`proj_plan_amount`,0) ELSE 0 END) AS shd_mon_fee_amt,-- 应收_月收分公司服务费
       SUM(CASE WHEN t1.fee_id IN ('129161a8-4a48-4f23-b04b-6e0807830899','339fcc81-1e85-4291-87d7-e3c34a8f84f2') THEN  IFNULL(t1.`proj_plan_amount`,0) ELSE 0 END) AS shd_gps_fee_amt,-- 应收_GPS费用
     --  DATE_FORMAT(t1.fact_repay_date,'%Y-%m-%d') AS act_repayment_date,
       DATE_FORMAT(t2.fact_repay_date,'%Y-%m-%d') AS act_repayment_date, -- 20180930 update by dailiang 陈泽圣反馈以t2.fact_repay_date为准
       SUM(IFNULL(t1.`proj_fact_amount`,0)) AS act_total_fee_amt,
       SUM(CASE WHEN t1.`fee_id` = '203e172b-9102-4820-9e06-76a64aa119e3' THEN  IFNULL(t1.`proj_fact_amount`,0) ELSE 0 END) AS act_mon_fee_amt,-- 实收_月收分公司服务费
       SUM(CASE WHEN t1.fee_id IN ('129161a8-4a48-4f23-b04b-6e0807830899','339fcc81-1e85-4291-87d7-e3c34a8f84f2') THEN  IFNULL(t1.`proj_fact_amount`,0) ELSE 0 END) AS  act_gps_fee_amt,-- 实收_GPS费用
	0 AS act_adv_settle_fee_amt -- 实收_提前结清服务费违约金
FROM ODS_XD_TUANDAI_BM.tb_repayment_proj_plan_list_detail_dh_ods t1
LEFT JOIN (SELECT * FROM ODS_XD_TUANDAI_BM.tb_repayment_proj_plan_list_dh_ods t2 WHERE part_dt =  DATE_FORMAT('${pt_day}','%Y-%m-%d')) t2 ON t1.`proj_plan_list_id` = t2.`proj_plan_list_id`
LEFT JOIN (SELECT * FROM ODS_XD_TUANDAI_BM.tb_repayment_proj_plan_dh_ods t3 WHERE part_dt =  DATE_FORMAT('${pt_day}','%Y-%m-%d') ) t3 ON t2.`proj_plan_id` = t3.`proj_plan_id`
WHERE t1.`fee_id`  IN (SELECT fee_item_id FROM ticket_rgs_tmp1)
  AND t1.part_dt =  DATE_FORMAT('${pt_day}','%Y-%m-%d')
GROUP BY t3.`project_id` ,
       t1.`business_id`,
       t2.`after_id` ,
       DATE_FORMAT(t1.due_date,'%Y-%m-%d') ,
       DATE_FORMAT(t2.fact_repay_date,'%Y-%m-%d')
UNION ALL
SELECT  t1.issue_id,
	t2.business_id,
	'1-00' AS business_after_id,
       DATE_FORMAT(t2.query_full_success_date,'%Y-%m-%d') AS shd_repayment_date,
       SUM(IFNULL(t1.`fee_value`,0)) AS shd_total_fee_amt,-- 应收_分公司费用
       0 AS shd_mon_fee_amt,
       0 AS shd_gps_fee_amt,
       DATE_FORMAT(t2.query_full_success_date,'%Y-%m-%d') AS act_repayment_date,
       SUM(IFNULL(t1.`fee_value`,0)) AS act_total_fee_amt,-- 应收_分公司费用
       0 AS act_mon_fee_amt,
       0 AS act_gps_fee_amt,
       0 AS act_adv_settle_fee_amt -- 实收_提前结清服务费违约金
FROM ODS_XD_TUANDAI_BM.tb_issue_business_apply_output_ods t1
JOIN (SELECT * FROM ODS_XD_TUANDAI_BM.tb_repayment_proj_plan_dh_ods t3 WHERE part_dt =  DATE_FORMAT('${pt_day}','%Y-%m-%d') ) t2 ON t1.issue_id = t2.proj_plan_id
WHERE t1.`fee_item_id`  IN (SELECT fee_item_id FROM ticket_rgs_tmp01)
  AND t1.part_dt =  DATE_FORMAT('${pt_day}','%Y-%m-%d')
  AND t1.is_one_time_charge = 2 -- 一次性收取
  GROUP BY  t1.issue_id,
	t2.business_id,
       DATE_FORMAT(t2.query_full_success_date,'%Y-%m-%d'),
       DATE_FORMAT(t2.query_full_success_date,'%Y-%m-%d') 
   )t
  GROUP BY issue_id,
	   business_id,
	   business_after_id,
	   shd_repayment_date,
	   act_repayment_date;
	   


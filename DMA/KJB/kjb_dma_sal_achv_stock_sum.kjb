)ENGINE=MEMORY DEFAULT CHARSET=utf8;

INSERT INTO dma_sal_achv_stock_sum_tmp6
select  t1.business_id,
		sum( t1.day_Principa_amt) total_Principa_amt
from dma_sal_achv_stock_sum_tmp1 t1
group by t1.business_id;


DROP TEMPORARY TABLE IF EXISTS `dma_sal_achv_stock_sum_tmp7`;
CREATE TEMPORARY TABLE `dma_sal_achv_stock_sum_tmp7` (
	`business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务编号',
	`dat_dt` VARCHAR(8) NULL DEFAULT NULL COMMENT '数据日期',
	`is_issue` INT(11) NULL DEFAULT NULL COMMENT '是否上标',
	`day_Principa_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日应收本金',
	`day_accrual_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日应收利息',
	`Principa_bln_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日应收本金',
KEY `dma_sal_achv_stock_sum_tmp7_business_id`(business_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8;

SET @amt=0;       
SET @var_business_id='';  
INSERT INTO dma_sal_achv_stock_sum_tmp7
SELECT t1.business_id,
	t1.dat_dt,
	t1.is_issue,
	t1.day_Principa_amt,
	t1.day_accrual_amt,
	t1.total_Principa_amt - sum_income AS Principa_bln_amt
FROM(
SELECT t1.business_id,
	   t1.dat_dt,
	   t1.is_issue,
	   t1.day_Principa_amt,
	   t1.day_accrual_amt,
	   t6.total_Principa_amt,
	   IF(@var_business_id = t1.business_id, @amt := @amt + t1.day_Principa_amt, @amt := t1.day_Principa_amt) sum_income ,
	   @var_business_id := t1.business_id AS var_business_id 
FROM   dma_sal_achv_stock_sum_tmp1 t1
LEFT JOIN dma_sal_achv_stock_sum_tmp6 t6
       ON t1.business_id = t6.business_id
ORDER BY t1.business_id ASC, t1.dat_dt ASC
)t1; 



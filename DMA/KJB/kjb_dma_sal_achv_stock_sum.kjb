INSERT INTO `DMA_SAL_ACHV_DATA`.`dma_sal_achv_stock_sum_his` 
 select 
		DATE_FORMAT('${p_day}','%Y%m%d')											  as stat_dt               ,
		date_format(t1.dat_dt,'%Y')                                                  as shd_repay_year        ,
		date_format(t3.out_date,'%Y')                                           	  as out_year              ,
		IFNULL(t4.DEPT_NAME ,'未知')                                       as area_nm               ,
		IFNULL(t5.DEPT_NAME ,'未知')                                     	as branch_nm             ,
		case when t1.is_issue='1' then '线上' else '线下'  end   as is_issue              ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='01' then t1.Principa_bln_amt end),0)  as jan_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='01' then t1.day_Principa_amt end),0) as jan_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='01' then t1.day_accrual_amt end),0)  as jan_shd_accrual_amt   ,
	    IFNULL(sum(case when date_format(t1.dat_dt,'%m')='02' then t1.Principa_bln_amt end),0)  as feb_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='02' then t1.day_Principa_amt end),0) as feb_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='02' then t1.day_accrual_amt end),0)  as feb_shd_accrual_amt   ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='03' then t1.Principa_bln_amt end),0)  as mar_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='03' then t1.day_Principa_amt end),0) as mar_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='03' then t1.day_accrual_amt end),0)  as mar_shd_accrual_amt   ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='04' then t1.Principa_bln_amt end),0)  as apr_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='04' then t1.day_Principa_amt end),0) as apr_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='04' then t1.day_accrual_amt end),0)  as apr_shd_accrual_amt   ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='05' then t1.Principa_bln_amt end),0)  as may_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='05' then t1.day_Principa_amt end),0) as may_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='05' then t1.day_accrual_amt end),0)  as may_shd_accrual_amt   ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='06' then t1.Principa_bln_amt end),0)  as june_Principa_bln_amt ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='06' then t1.day_Principa_amt end),0) as june_shd_principa_amt ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='06' then t1.day_accrual_amt end),0)  as june_shd_accrual_amt  ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='07' then t1.Principa_bln_amt end),0)  as july_Principa_bln_amt ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='07' then t1.day_Principa_amt end),0) as july_shd_principa_amt ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='07' then t1.day_accrual_amt end),0)  as july_shd_accrual_amt  ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='08' then t1.Principa_bln_amt end),0) as aug_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='08' then t1.day_Principa_amt end),0) as aug_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='08' then t1.day_accrual_amt end),0)  as aug_shd_accrual_amt   ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='09' then t1.Principa_bln_amt end),0)  as sept_Principa_bln_amt ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='09' then t1.day_Principa_amt end),0) as sept_shd_principa_amt ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='09' then t1.day_accrual_amt end),0)  as sept_shd_accrual_amt  ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='10' then t1.Principa_bln_amt end),0)  as oct_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='10' then t1.day_Principa_amt end),0) as oct_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='10' then t1.day_accrual_amt end),0)  as oct_shd_accrual_amt   ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='11' then t1.Principa_bln_amt end),0) as nov_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='11' then t1.day_Principa_amt end),0) as nov_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='11' then t1.day_accrual_amt end),0)  as nov_shd_accrual_amt   ,
        IFNULL(sum(case when date_format(t1.dat_dt,'%m')='12' then t1.Principa_bln_amt end),0) as dec_Principa_bln_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='12' then t1.day_Principa_amt end),0) as dec_shd_principa_amt  ,
		IFNULL(sum(case when date_format(t1.dat_dt,'%m')='12' then t1.day_accrual_amt end),0)  as dec_shd_accrual_amt   ,
		'tuandai_bm'	as	dw_src_sys,	--	来源系统
		'DMB_FIN_DATA.dmb_fin_xd_due_amt_recab_sum..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	,--	更新用户
	DATE_FORMAT('${pt_day}','%Y-%m-%d')	as	part_dt	--	分区日期
from dma_sal_achv_stock_sum_tmp7 t1
left join dma_sal_achv_stock_sum_tmp5 t2 on t1.business_id = t2.business_id 
left join dma_sal_achv_stock_sum_tmp2 t3 on t1.business_id = t3.business_id
left join dma_sal_achv_stock_sum_tmp3 t4 on t2.area_id = t4.dept_id
left join dma_sal_achv_stock_sum_tmp4 t5 on t2.branch_id = t5.dept_id
group by date_format(t1.dat_dt,'%Y'),
		 date_format(t3.out_date,'%Y'),
		 t4.DEPT_NAME,
		 t5.DEPT_NAME,
		 case when t1.is_issue='1' then '线上' else '线下'  end;
	

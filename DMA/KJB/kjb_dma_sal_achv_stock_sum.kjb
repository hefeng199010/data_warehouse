DROP TEMPORARY TABLE IF EXISTS `dma_sal_achv_stock_sum_tmp1`;
CREATE TEMPORARY TABLE `dma_sal_achv_stock_sum_tmp1` (
	`dat_dt` VARCHAR(8) NULL DEFAULT NULL COMMENT '数据日期',
	`business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务编号',
	`is_issue` INT(11) NULL DEFAULT NULL COMMENT '是否上标',
	`day_Principa_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日应收本金',
	`day_accrual_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '当日应收利息',
KEY `dma_sal_achv_stock_sum_tmp1_business_id`(business_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8;

INSERT INTO dma_sal_achv_stock_sum_tmp1
select  t1.dat_dt,
		t1.business_id,
		 t1.is_issue,
		 sum(ifnull(t1.day_Principa_amt,0)) as day_Principa_amt,
		 sum(ifnull(t1.day_accrual_amt,0)) as day_accrual_amt
from DMB_FIN_DATA.dmb_fin_xd_due_amt_recab_sum t1
where t1.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
  and FIND_IN_SET(t1.business_type,'房速贷标准件,房速贷非标准件,T600')
  group by t1.dat_dt,
		t1.business_id,
		 t1.is_issue;
 

INSERT INTO DMA_SAL_ACHV_DATA.dma_sal_achv_yd_repayment_actual_detail_his
SELECT 
 DATE_FORMAT('${pt_day}','%Y-%m-%d')  AS stat_dt                     -- 统计日期
,a.runperiod 
,a.customer_name                --  客户名                                                                                        
,a.vehicle_number               --  车牌号 
,a.org_address_province                                                                                        
,a.org_name                     --  机构名 
,a.actual_period                --  实际还款期数                                                                                                
,a.refund_actual_id             --  真实还款表ID                                                                                
,a.loan_id                      --  借款ID                                                                                         
,a.issue_id                     --  发标ID                                                                                        
,UPPER(a.contract_number) AS  contract_number                        --  合同号                                                                                        
,DATE_FORMAT(a.loan_date,'%Y-%m-%d') AS loan_date                    --  合同日期 
,a.loan_money                                                        --  审批金额  add by caihl20180927 
,a.bidding_money                                                     --  发标金额                                                                                   
,DATE_FORMAT(a.full_scale_time,'%Y-%m-%d') AS full_scale_time        --  满标时间                                                                                     
,a.loan_period                  --  审核期限
-- ,DATE_FORMAT(bo.loan_end_date,'%Y-%m-%d')  AS  loan_end_date         --  借款终止日/到期日  add by caihl20180927                                                                                                                                                                                                                                     
,DATE_SUB(DATE_ADD(DATE_FORMAT(yl.loan_date,'%Y-%m-%d'),INTERVAL yl.loan_period MONTH),INTERVAL 1 DAY) AS loan_end_date
,a.jkylilv                      --  月利率                                                                                        
,a.bdnhlv
,a.repayment_mode                                                                                                              --  还款方式(等额等息:8000、先息后本:8001) 
,CASE a.repayment_mode WHEN '8000' THEN  '等额等息'
                      WHEN '8001' THEN '先息后本'
                      WHEN '8002' THEN '等本等息'
                      WHEN '8003' THEN '先息后本-旧'
                      WHEN '8004' THEN '等本等息-融资租赁'
   ELSE a.repayment_mode END AS repayment_mode_name 
-- ,(CASE bo.loan_nature WHEN 0 THEN '新增' WHEN 1 THEN '结清再贷' WHEN 2 THEN '借新还旧' WHEN 3 THEN '先转等' WHEN 4 THEN '续借' ELSE NULL END) loan_nature_name      -- 借款性质（0：新增；1：结清再贷；2：借新还旧；3：先转等；4：续借）                                                                         
,(CASE yl.loan_nature WHEN 0 THEN '新增' WHEN 1 THEN '结清再贷' WHEN 2 THEN '借新还旧' WHEN 3 THEN '先转等' WHEN 4 THEN '续借' ELSE NULL END) as loan_nature_name      -- 借款性质（0：新增；1：结清再贷；2：借新还旧；3：先转等；4：续借）                                                                         
,DATE_FORMAT(a.due_date,'%Y-%m-%d') AS due_date    -- 应还日期 
,(a.bidding_money -IFNULL(yl.actual_bj,0)) AS left_prin -- 剩余本金（按单统计剩余本金）
,(CASE WHEN a.is_last_period=1 THEN '是' WHEN a.is_last_period=0 THEN '否' ELSE is_last_period END)    AS is_last_period   -- 是否展期（1 是 0 否）                                                                                                                                             
,(CASE WHEN a.is_the_settleup=1 THEN '是' WHEN a.is_the_settleup=0 THEN '否' ELSE is_the_settleup END) AS is_the_settleup  -- 是否结清（1 是 0 否）
,a.hx_batch_code                -- 核销批次码  
,a.hx_time                      -- 核销日期
,a.hx_money                     -- 核销总金额    
,a.loan_product
,NULL AS loan_product_name                
-- ,DATE_FORMAT(a.due_date,'%Y-%m-%d') AS due_date                       --  应还款日                                                                                     
,DATE_FORMAT(a.actual_date,'%Y-%m-%d') AS actual_date                  --  实际还款日 
,a.bj                           --  本金 
,a.lx                           --  利息                                                                                           
,a.gpsfwf                       --  GPS服务费 
,a.margin                       --  保证金 
,a.insurance AS platform_fee
,a.p2p_service                                      --  P2P服务费  
,a.zq_margin AS  zq_margin    -- 展期保证金
,a.pt_late_fee  AS  online_late_fee                                   -- 实还逾期滞纳金（线上） 平台滞纳金 
,a.yqfx         AS  offline_late_fee                                  -- 实还逾期滞纳金（线下） 逾期服务费(逾期罚息) 
,a.zjdtqjqwyj   AS  zjdtqjqwyj                           -- 资金端提前结清违约金
,a.pttqjqwyj                                             -- 实还资产端提前结清违约金(线上)         平台提前结清违约金
,a.tqhkywj      AS offline_tqhkywj                       -- 实还资产端提前结清违约金(线下) 提前还款违约金
-- ,ifnull(yra.roll_asset_fee,0) AS ht_service_fee       -- 鸿特服务费(展期资产端服务费) /*鸿特服务费是展期综合利率1.99%-展期利息-服务费-担保费*/ 
,a.towing_fee   AS  towing_fee           -- 法务清收费 
,a.yk                                    -- 余款
,(
    a.bj                   +      
    a.lx                   +      
    a.gpsfwf               +      
    a.margin               +      
    a.insurance            +      
    a.p2p_service          +      
    a.zq_margin            +      
    a.pt_late_fee          +      
    a.yqfx                 +      
    a.zjdtqjqwyj           +      
    a.pttqjqwyj            +      
    a.tqhkywj              +      
    a.towing_fee           +      
    a.yk                   +
    a.zq_lx                -- 展期利息                               
+ a.zq_insurance         -- 展期资产端服务费                       
+ a.zq_p2p_service       -- 展期P2P平台服务费(实还资金端展期服务费)
+ a.zq_dbf               -- 展期担保费 	
 ) AS actual_total                          --  实还款总额  
 ,a.repay_to_account                        --  还款到达账户
 ,dict.dict_key      AS refund_channel_name -- 还款渠道
 ,a.remark                                  -- 备注
 ,0 AS overdue_days                        -- 逾期天数
,a.bidding_platform                        --  发标平台 
,(CASE WHEN a.asset_side=1 THEN '鸿特' WHEN a.asset_side=2 THEN '一点' WHEN a.asset_side=3 THEN '德古' ELSE a.asset_side END ) AS asset_side_name -- 资产端（1：鸿特 2：一点）
, 'yidian' AS dw_src_sys
, 't_rp_repayment_detail,t_cfg_dictionary_detail,t_refund_period,t_cfg_organization,t_rp_business_open,dws_agr_yd_loan' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期 
,a.zq_lx                -- 展期利息                               
,a.zq_insurance         -- 展期资产端服务费                       
,a.zq_p2p_service       -- 展期P2P平台服务费(实还资金端展期服务费)
,a.zq_dbf               -- 展期担保费                             
,a.original_org_names   -- 原所属机构                                                                                    
FROM yidian.t_rp_repayment_detail a
-- LEFT JOIN yidian.t_rp_business_open bo  ON a.loan_id = bo.loan_id
LEFT JOIN (SELECT dict.`dict_key`,dict.`dict_value` FROM yidian.t_cfg_dictionary_detail dict WHERE dict.`parent_dict_code` IN ('hqjq')) dict ON a.`refund_channel` = dict.dict_value
LEFT JOIN DWS_DATA.dws_agr_yd_loan yl ON yl.loan_id = a.loan_id AND yl.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
where a.contract_number not like 'cdjk%'   -- add by caihl20181128 财务黄晓君确认需要排除掉CDJK的单
;





INSERT INTO dma_sal_achv_sub_comp_cg_tran_detail_tmp
SELECT 
 '1:交易数据' AS data_src
,xib.business_id  -- 业务编号：业务所属编号【标的文件-->标的ID  匹配 业务编号】
,tran.issue_id    -- 标的号：业务标的号【标的文件-->标的ID】
,xib.post_time    -- 上标时间：上标日期，【标的文件-->上标日期】
,xib.full_issue_date   -- 满标时间：满标时间【标的文件-->满标时间】
,xib.business_type_nm  -- 业务类型：业务所属类型，例如房速贷/车易贷/一点车贷等
,xib.customer_name     -- 借款人姓名：借款人姓名【标的文件-->借款人ID  匹配借款人名称】
,xib.full_borrow_money -- 满标金额：满标金额【标的文件-->借款金额】
,xib.borrow_limit      -- 借款期限：借款期限【标的文件-->借款期限】
,xib.loan_rate         -- 借款利率：借款利率【标的文件-->借款利率】
,tran.amount       AS tran_amount       -- 交易金额：接口数据【交易文件-->金额】、【提现文件-->到账金额】
,tran.account_time AS tran_time         -- 交易时间：接口数据，交易时间【交易文件-->发生时间】、【提现文件-->提现时间】
,tran.source_id                       -- 出款方：接口数据【交易文件-->出款方ID】 、【提现文件-->平台会员编码】
,dept1.full_branch_name AS out_name          -- 出款方全称：对应【交易文件-->出款方ID】 、【提现文件-->平台会员编码】的全称，可根据ID进行匹配
,tran.receive_id                     -- 入款方：接口数据【交易文件-->接收方ID】
,dept2.full_branch_name AS in_name          -- 入款方全称：对应全称
,tran.busi_type_nm                     -- 交易类型：【交易文件-->业务类型】，若为【提现文件】，则类型为提现
FROM ht_cams_cgt_transaction_tmp tran
LEFT JOIN dws_agr_issue_business_tmp  xib ON tran.issue_id = xib.issue_id
LEFT JOIN DIM_DATA.dim_org_full_branch_cfg  dept1       ON tran.source_id  = dept1.branch_id
LEFT JOIN DIM_DATA.dim_org_full_branch_cfg  dept2       ON tran.receive_id = dept2.branch_id
; 


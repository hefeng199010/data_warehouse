DROP TEMPORARY TABLE IF EXISTS ht_cams_cgt_transaction_tmp;
CREATE TEMPORARY TABLE `ht_cams_cgt_transaction_tmp` (
  `issue_id` VARCHAR(64) DEFAULT NULL COMMENT '标的ID',
  `source_id` VARCHAR(64) DEFAULT NULL COMMENT '出款方ID',
  `receive_id` VARCHAR(64) DEFAULT NULL COMMENT '接收方ID',
  `busi_type_nm` VARCHAR(7) DEFAULT NULL,
  `amount` DOUBLE(16,2) NOT NULL DEFAULT '0.00',
  `account_time` DATETIME DEFAULT NULL COMMENT '发生时间',
  KEY `idx_issue_id` (`issue_id`),
  KEY `idx_source_id` (`source_id`),
  KEY `idx_receive_id` (`receive_id`)
) ENGINE=INNODB DEFAULT CHARSET=utf8
;

INSERT INTO ht_cams_cgt_transaction_tmp
SELECT  
 tran.issue_id
,tran.source_id     -- 出款方ID
,tran.receive_id    -- 接收方ID
,(CASE tran.busi_type WHEN 1 THEN '代偿'  WHEN 2 THEN '还代偿' WHEN 3 THEN '分润' WHEN 4 THEN '代充值资金分发' ELSE tran.busi_type END) AS busi_type_nm
,(IFNULL(tran.amount,0)) AS  amount      -- 交易金额
,(tran.account_time)     AS account_time -- 发生时间
 FROM ODS_XD_TUANDAI_BM.ht_cams_cgt_transaction_ods tran
 WHERE tran.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
 -- AND tran.issue_id IS NOT NULL
 AND (EXISTS (SELECT 1 FROM DIM_DATA.dim_org_full_branch_cfg dept WHERE dept.branch_id = tran.source_id)
  OR EXISTS (SELECT 1 FROM DIM_DATA.dim_org_full_branch_cfg  dept WHERE dept.branch_id = tran.receive_id) -- 发现关联上都是接收方receive_id
    )
 ;


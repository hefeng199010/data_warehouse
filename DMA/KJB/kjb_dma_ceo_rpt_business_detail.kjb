 INSERT INTO DMA_CEO_RPT_DATA.dma_ceo_rpt_business_detail_his
 (
 stat_dt                               # 统计日期  
,business_id                          # 业务单号  
,area_nm                              # 业务区域  
,branch_nm                            # 分公司    
,operator_nm                          # 业务获取人
,type_nm                              # 类型      
,business_type_nm                     # 业务类型 
,product_type                         # 产品类型 新增字段  
,customer_nm                          # 客户名称  
,repay_type_nm                        # 还款方式 
,accounter_confirm_time               # 放款日期  
,borrow_limit                         # 借款期限  
,borrow_amt                           # 放款金额  
,dw_src_sys                           # 来源系统  
,dw_src_tbl                           # 来源表    
,dw_ins_tm                            # 插入时间  
,dw_upd_tm                            # 更新时间  
,dw_ins_usr                           # 插入用户  
,dw_upd_usr                           # 更新用户 
,part_dt
 )
  SELECT
  DATE_FORMAT('${pt_day}','%Y-%m-%d') AS stat_dt
,a.business_id         AS business_id                     -- 业务单号  
,IFNULL(a.business_area,'一点车贷')       AS area_nm                        -- 业务区域  
,a.branch_name         AS branch_nm                      -- 分公司    
,'无'                  AS operator_nm                    -- 业务获取人
, '新增'                     AS type_nm                        -- 类型 展期/新增业务 待确认     
,'一点车贷'            AS business_type_nm               -- 业务类型 
,NULL AS product_type                         # 产品类型 新增字段   
,cpi.customer_name     AS customer_nm                    -- 客户名称  
,CASE WHEN a.repayment_type IN (1,2) THEN '先息后本'
      WHEN a.repayment_type = 5      THEN '等额本息' ELSE a.repayment_type END  AS repay_type_nm           -- 还款方式	
	  
,DATE_FORMAT(IFNULL(ib.full_issue_date,'4712-12-31'),'%Y-%m-%d')     AS accounter_confirm_time         -- 放款日期  取线上满标时间 
,a.borrow_limit         AS borrow_limit                   -- 借款期限  
-- ,ifnull(wca.borrow_money,0)     AS borrow_amt                     -- 放款金额 取上标金额 
,IFNULL(a.borrow_money,0)     AS borrow_amt                     -- 放款金额 取基本信息表中borrow_money 
,'tuandai_bm' AS dw_src_sys                           # 来源系统  
,'tb_yidian_business_information_ods,tb_yidian_car_personal_information_ods,tb_yidian_withdrawals_loans_information_ods,tb_yidian_wind_control_approval_ods' AS dw_src_tbl                           # 来源表    
,NOW() AS dw_ins_tm                            # 插入时间  
,NOW() AS dw_upd_tm                            # 更新时间  
,'caihl' AS dw_ins_usr                           # 插入用户  
,'caihl' AS dw_upd_usr                           # 更新用户 
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期 
FROM tb_yidian_business_information_ods_tmp  a   
LEFT JOIN tb_yidian_car_personal_information_ods_tmp cpi
ON a.`business_id` = cpi.business_id
LEFT JOIN dws_agr_xd_issue_business_yidian_tmp ib
ON a.business_id = ib.business_id
;



 DROP TEMPORARY TABLE IF EXISTS `dws_agr_xd_business_tmp02`;
 
 CREATE TEMPORARY TABLE `dws_agr_xd_business_tmp02` (
	`business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务单号',
	`area_nm` VARCHAR(50) NULL DEFAULT NULL COMMENT '部门名称',
	`branch_nm` VARCHAR(50) NULL DEFAULT NULL COMMENT '部门名称',
	`USER_NAME` VARCHAR(50) NULL DEFAULT NULL COMMENT '[用户姓名]',
	`customer_name` VARCHAR(50) NULL DEFAULT NULL COMMENT '客户名称',
	`repay_way_nm` VARCHAR(50) NULL DEFAULT NULL COMMENT '还款方式名称',
	`apply_borrow_limit` INT(11) NULL DEFAULT NULL COMMENT '申请借款期限',
	`apply_borrow_money` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '申请借款金额',
	business_type VARCHAR(50) NULL DEFAULT NULL COMMENT '[业务类型]',
		INDEX `idx_business_id`(`business_id`)
)
COLLATE='utf8_general_ci'
ENGINE=INNODB
;

INSERT INTO  dws_agr_xd_business_tmp02  
 SELECT
 b.business_id                          # 业务单号  
,IFNULL(dep1.dept_name,'其他')   AS  area_nm                           # 业务区域  
,IFNULL(dep2.dept_name,'其他')   AS   branch_nm                         # 分公司     
,usr.USER_NAME                          # 业务获取人
,b.customer_name                           # 客户名称  
,IFNULL(rw.repay_way_nm ,'其他')                       # 还款方式   apply_repayment_type_id
-- ,dt.full_issue_date                          # 放款日期
,b.apply_borrow_limit                        # 借款期限  
,b.apply_borrow_money                        # 放款金额  
,b.business_type
  FROM dws_agr_xd_business_tmp b
  LEFT JOIN tb_department_ods_tmp1 dep1
  ON b.area_id = dep1.DEPT_ID
  LEFT JOIN tb_department_ods_tmp2 dep2
  ON b.branch_id = dep2.DEPT_ID
  LEFT JOIN tb_user_ods_tmp1  usr
  ON b.original_id = usr.USER_ID
  LEFT JOIN DIM_DATA.dim_evt_xd_repay_way rw -- 还款方式
  ON b.apply_repayment_type_id = rw.repay_way_id;
  
 
 -- 房速贷 车易贷 业务 + 一点车贷
 -- 如果是展期，放款日期（展期满标时间） 和 放款金额（展期放款时间） 和新增不同
 INSERT INTO DMA_CEO_RPT_DATA.dma_ceo_rpt_business_detail_his
 (
 stat_dt                               # 统计日期  
,business_id                          # 业务单号  
,area_nm                              # 业务区域  
,branch_nm                            # 分公司    
,operator_nm                          # 业务获取人
,type_nm                              # 类型      
,business_type_nm                     # 业务类型
,product_type                         # 产品类型 新增字段  
,customer_nm                          # 客户名称  
,repay_type_nm                        # 还款方式  
,accounter_confirm_time               # 放款日期  
,borrow_limit                         # 借款期限  
,borrow_amt                           # 放款金额  
,dw_src_sys                           # 来源系统  
,dw_src_tbl                           # 来源表    
,dw_ins_tm                            # 插入时间  
,dw_upd_tm                            # 更新时间  
,dw_ins_usr                           # 插入用户  
,dw_upd_usr                           # 更新用户 
,part_dt
 )
 SELECT 
 DATE_FORMAT('${pt_day}','%Y-%m-%d') AS stat_dt                               # 统计日期  
,b.business_id                          # 业务单号  
,b.area_nm    AS area_nm                              # 业务区域  
,b.branch_nm  AS branch_nm                            # 分公司    
,b.USER_NAME   AS operator_nm                          # 业务获取人
,CASE WHEN b.business_type IN ('T600','T500')  THEN '展期' ELSE '新增' END AS type_nm                              # 类型      
,CASE WHEN b.business_type IN ('T600','房速贷标准件','房速贷非标准件') THEN '房速贷' 
      WHEN b.business_type IN ('T500','车易贷') THEN '车易贷' END   AS business_type_nm                     # 业务类型  
,CASE WHEN  b.business_type IN ('T600','房速贷标准件','房速贷非标准件') AND aft.car_business_id IS NOT NULL THEN '信用加成类型' 
      WHEN  b.business_type IN ('T600','房速贷标准件','房速贷非标准件') AND aft.car_business_id IS  NULL THEN '纯抵押类型'
	ELSE NULL
END AS product_type # 产品类型 新增字段
,b.customer_name      AS customer_nm                          # 客户名称  
,b.repay_way_nm       AS repay_type_nm                        # 还款方式   apply_repayment_type_id
,DATE_FORMAT(dt.full_issue_date,'%Y-%m-%d')    AS accounter_confirm_time                             -- 放款日期 modified by caihl20180516 
,b.apply_borrow_limit AS  borrow_limit                         # 借款期限  
,IFNULL(dt.borrow_money,0)       AS  borrow_amt                           # 放款金额  modified by caihl 20180425
,'tuandai_bm' AS dw_src_sys                           # 来源系统  
,'dws_agr_xd_issue_business,dws_evt_xd_output_list,dws_agr_xd_business,tb_department_ods,tb_user_ods,dim_evt_xd_repay_way' AS dw_src_tbl                           # 来源表    
,NOW() AS dw_ins_tm                            # 插入时间  
,NOW() AS dw_upd_tm                            # 更新时间  
,'caihl' AS dw_ins_usr                           # 插入用户  
,'caihl' AS dw_upd_usr                           # 更新用户
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期   
  FROM  dws_agr_xd_business_tmp02   b
  INNER JOIN dws_agr_xd_issue_business_tmp01 dt   -- modified by caihl20180516 内关联，只取有出款的单
  ON b.business_id = dt.business_id
  LEFT JOIN tb_car_business_after_ods_tmp aft  -- 判断房速贷的产品类型
  ON b.business_id = aft.car_business_id
;


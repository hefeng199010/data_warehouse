INSERT INTO DMA_CEO_RPT_DATA.`dma_ceo_rpt_general_expense`
(
stat_dt                         
,statistical_time                
,company_code                    
,campany_name                    
,major_type                      
,sub_type                        
,rent_cost                       
,depreciation_amortization_fee   
,long_term_apportioned_cost      
,office_expenses                 
,travel_expenses                 
,communication_fee               
,advertising_fee                 
,vehicle_cost                    
,hospitality                     
,service_charge                  
,conference_fee                  
,low_value_consumables           
,commission_charge               
,other_expenses                  
,artificial_cost_aggregate       
,wages                           
,performance_promotion_and_bonus 
,social_security                 
,accumulation_fund               
,welfare_funds                   
,recruitment_costs               
,training_fee                    
,other                           
,value_added_tax                 
,additional_tax                  
,corporate_income_tax            
,general_expense_total                               
,STATUS                          
,dw_src_sys                      
,dw_src_tbl                      
,dw_ins_tm                       
,dw_upd_tm                       
,dw_ins_usr                      
,dw_upd_usr                      
)
SELECT 
 DATE_FORMAT('${pt_day}','%Y-%m-%d')  AS stat_dt                         -- 统计日期
,statistical_time                 -- 统计日期        
,company_code                     -- 公司代码        
,campany_name                     -- 公司名称        
,major_type                       -- 大类            
,sub_type                         -- 小类            
,IFNULL(rent_cost                       ,0) AS  rent_cost                      -- 房租费用        
,IFNULL(depreciation_amortization_fee   ,0) AS  depreciation_amortization_fee  -- 折旧摊销费      
,IFNULL(long_term_apportioned_cost      ,0) AS  long_term_apportioned_cost     -- 长期待摊费用    
,IFNULL(office_expenses                 ,0) AS  office_expenses                -- 办公费          
,IFNULL(travel_expenses                 ,0) AS  travel_expenses                -- 差旅费          
,IFNULL(communication_fee               ,0) AS  communication_fee              -- 通讯费          
,IFNULL(advertising_fee                 ,0) AS  advertising_fee                -- 广告宣传费      
,IFNULL(vehicle_cost                    ,0) AS  vehicle_cost                   -- 车辆费用        
,IFNULL(hospitality                     ,0) AS  hospitality                    -- 招待费          
,IFNULL(service_charge                  ,0) AS  service_charge                 -- 服务费          
,IFNULL(conference_fee                  ,0) AS  conference_fee                 -- 会议费          
,IFNULL(low_value_consumables           ,0) AS  low_value_consumables          -- 低值易耗品      
,IFNULL(commission_charge               ,0) AS  commission_charge              -- 手续费          
,IFNULL(other_expenses                  ,0) AS  other_expenses                 -- 其他费用        
,IFNULL(artificial_cost_aggregate       ,0) AS  artificial_cost_aggregate      -- 人工费合计      
,IFNULL(wages                           ,0) AS  wages                          -- 工资            
,IFNULL(performance_promotion_and_bonus ,0) AS  performance_promotion_and_bonus-- 绩效提成及奖金  
,IFNULL(social_security                 ,0) AS  social_security                -- 社保            
,IFNULL(accumulation_fund               ,0) AS  accumulation_fund              -- 公积金          
,IFNULL(welfare_funds                   ,0) AS  welfare_funds                  -- 福利费          
,IFNULL(recruitment_costs               ,0) AS  recruitment_costs              -- 招聘费用        
,IFNULL(training_fee                    ,0) AS  training_fee                   -- 培训费          
,IFNULL(other                           ,0) AS  other                          -- 其他            
,IFNULL(value_added_tax                 ,0) AS  value_added_tax                -- 增值税          
,IFNULL(additional_tax                  ,0) AS  additional_tax                 -- 附加税          
,IFNULL(corporate_income_tax            ,0) AS  corporate_income_tax           -- 企业所得税      
,IFNULL(general_expense_total           ,0) AS  general_expense_total          -- 日常费用合计
,STATUS 
, 'ht_cams' AS dw_src_sys
, 'tb_general_expense_ods' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr      
 FROM ODS_XD_TUANDAI_BM.tb_general_expense_ods t
 WHERE part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d') 
;


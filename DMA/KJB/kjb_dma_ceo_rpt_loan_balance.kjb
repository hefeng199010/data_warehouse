DROP  TEMPORARY TABLE IF EXISTS `tb_car_business_after_tmp1`;
CREATE TEMPORARY TABLE `tb_car_business_after_tmp1` (
  `business_id` VARCHAR(50) NOT NULL COMMENT '业务单号',
  `car_business_after_id` VARCHAR(50) DEFAULT NULL COMMENT '[当前还款期数]',
  `car_business_after_defer` VARCHAR(50) DEFAULT NULL COMMENT '[展期业务编号]',
  `car_business_defer_id` VARCHAR(50) DEFAULT NULL COMMENT '[当前展期期数]',
  `branch_id` VARCHAR(50) DEFAULT NULL COMMENT '业务所属分公司编号',
  `original_id` VARCHAR(50) DEFAULT NULL COMMENT '业务获取人编号',
  `business_type` VARCHAR(50) DEFAULT NULL COMMENT '业务类型',
  `customer_name` VARCHAR(100) DEFAULT NULL COMMENT '客户名称',
  `repayment_type_id` INT(11) DEFAULT NULL COMMENT '还款方式ID，1到期还本息，2每月付息到期还本，5每月等额本息,9是分期还本付息',
  `borrow_limit` INT(11) DEFAULT NULL COMMENT '借款期限',
  `borrow_money` DECIMAL(12,2) NOT NULL DEFAULT '0.00',
  `current_accrual` DECIMAL(12,2) NOT NULL DEFAULT '0.00',
  `fact_accrual` DECIMAL(12,2) NOT NULL DEFAULT '0.00',
  `current_Principa` DECIMAL(12,2) NOT NULL DEFAULT '0.00',
  `fact_principa` DECIMAL(12,2) NOT NULL DEFAULT '0.00',
  `due_unpaid_prin` DECIMAL(12,2) DEFAULT NULL,
  `due_unpaid_interest` DECIMAL(12,2) DEFAULT NULL,
  `car_business_after_type` VARCHAR(50) DEFAULT NULL COMMENT '[还款状态分类]：还款中，已还款，逾期',
  `reserve_2` VARCHAR(50) DEFAULT NULL COMMENT '[还款状态]还款中,逾期,还款待确认,还款登记被财务拒绝,催款中,已拖车登记,已移交法务,已还款,坏账,退租申请,全部结清',
  `borrow_date` DATETIME DEFAULT NULL COMMENT '[还款日期]',
  `fatct_replayDate` DATETIME DEFAULT NULL COMMENT '[实际还款日期]'
) ENGINE=INNODB DEFAULT CHARSET=utf8
;


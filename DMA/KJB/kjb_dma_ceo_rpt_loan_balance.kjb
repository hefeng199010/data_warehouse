INSERT INTO tb_car_business_after_tmp1 
SELECT tb.`business_id`
,aft.`car_business_after_id`
,aft.car_business_after_defer
,aft.`car_business_defer_id`
,tb.`branch_id`
,tb.original_id
,tb.`business_type`
,tb.`customer_name`
,tb.apply_repayment_type_id AS `repayment_type_id`
,tb.apply_borrow_limit      AS `borrow_limit`
,CAST((IFNULL(tb.`apply_borrow_money` ,0)) AS DECIMAL(12,2))      AS borrow_money    -- 借款金额不能进行汇总
-- ,(CASE WHEN aft.is_defer =1 THEN 0 ELSE CAST((IFNULL(aft.`current_accrual` ,0)) AS DECIMAL(12,2)) END) AS current_accrual    --  [本期应还利息] 3：对于展期的数据 应还利息需要排除展期那期（is_defer=1）
,(CASE WHEN aft.repayed_flag =6 THEN 0 ELSE CAST((IFNULL(aft.`current_accrual` ,0)) AS DECIMAL(12,2)) END) AS current_accrual    --  repayed_flag = 6 6:标识展期已还款
,CAST((IFNULL(aft.`fact_accrual` ,0)) AS DECIMAL(12,2))     AS fact_accrual     -- 实还利息 
,CAST((IFNULL(aft.`current_Principa` ,0)) AS DECIMAL(12,2)) AS current_Principa
,CAST((IFNULL(aft.`fact_principa` ,0)) AS DECIMAL(12,2))    AS fact_principa
,CASE WHEN aft.car_business_after_type = '逾期' THEN   CAST((IFNULL(aft.`current_Principa` ,0)) AS DECIMAL(12,2)) ELSE 0 END AS due_unpaid_prin                -- 到期未收本金 到期应还未还本金累计之和  
,CASE WHEN aft.car_business_after_type = '逾期' THEN   CAST((IFNULL(aft.`current_accrual` ,0)) AS DECIMAL(12,2)) ELSE 0 END AS due_unpaid_interest
,aft.car_business_after_type
,aft.`reserve_2`
,aft.borrow_date
,aft.fatct_replayDate
FROM DWS_DATA.`dws_agr_xd_business` tb
INNER JOIN ODS_XD_TUANDAI_BM.tb_car_business_after_ods aft
 ON tb.`business_id` = aft.`car_business_id`
AND aft.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
WHERE  tb.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
AND tb.business_type IN ('T600','房速贷标准件','房速贷非标准件','T500','车易贷')
AND tb.settle_way &lt;0  -- 未结清 add by caihl20180522 
;



INSERT INTO DMA_CEO_RPT_DATA.`dma_ceo_rpt_loan_balance`
(
stat_dt                        -- 统计日期    
,data_dt                       -- 数据日期    
,business_id                   -- 业务编号    
,area_nm                       -- 片区        
,branch_nm                     -- 分公司      
,business_type_nm              -- 业务类型    
,customer_nm                   -- 客户名称    
,repay_type_nm                 -- 还款方式    
,borrow_limit                  -- 借款期限    
,borrow_amt                    -- 放款金额    
,tot_receive_interest          -- 应收总利息  
,tot_actual_interest           -- 实收总利息  
,due_unpaid_prin               -- 到期未收本金
,due_unpaid_interest           -- 到期未收利息
,left_principa_amt             -- 剩余本金    
,left_interest_amt             -- 剩余利息    
,dw_src_sys                    -- 来源系统    
,dw_src_tbl                    -- 来源表      
,dw_ins_tm                     -- 插入时间    
,dw_upd_tm                     -- 更新时间    
,dw_ins_usr                    -- 插入用户    
,dw_upd_usr                    -- 更新用户    
,part_dt                       -- 分区时间   
)
 SELECT
 DATE_FORMAT('${pt_day}','%Y-%m-%d')  AS stat_dt                         -- 统计日期
,DATE_FORMAT('${pt_day}','%Y-%m-%d')  AS data_dt                       -- 数据日期 
,t.business_id   
,IFNULL(dep1.dept_name,'未知') area_nm                                         #区域            
,IFNULL(dep2.dept_name,'未知') branch_nm                                         #分公司 
,CASE WHEN t.business_type IN ('T600','房速贷标准件','房速贷非标准件') THEN '房速贷' 
      WHEN t.business_type IN ('T500','车易贷') THEN '车易贷' END   AS business_type_nm                     # 业务类型 
,t.customer_name                 AS customer_nm 								     #主借人
,IFNULL(rw.repay_way_nm,'未知')  AS repay_type_nm                                     #还款方式 
,borrow_limit    AS borrow_limit
,borrow_money    AS borrow_amt   -- 借款金额不能进行汇总
,current_accrual AS tot_receive_interest          -- 应收总利息  
,fact_accrual    AS tot_actual_interest           -- 实收总利息 
,due_unpaid_prin 
,due_unpaid_interest
,left_principa_amt              -- 剩余本金 根据单来计算  借款金额 - 已还本金   
,left_interest_amt              -- 剩余利息                总利息 - 已还利息
, 'tuandai_bm' AS dw_src_sys
, 'tb_business_ods,tb_car_business_after_ods,tb_department_ods' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期
  FROM tb_car_business_after_tmp2 t
 LEFT JOIN 
  (
   SELECT 
   dep.dept_id,
   dep.district_id AS area_id
  FROM  ODS_XD_TUANDAI_BM.tb_department_ods dep -- 部门信息表
   WHERE dep.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
  ) dep
   ON  t.branch_id = dep.dept_id
 LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')   -- 报表按小时跑批，部门表按天跑
  ) dep1 ON dep.area_id = dep1.DEPT_ID 
  LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')  
  ) dep2  ON t.branch_id = dep2.DEPT_ID 
 LEFT JOIN DIM_DATA.dim_evt_xd_repay_way rw 
  ON t.repayment_type_id = rw.repay_way_id
  ;

/*还款计划中获取逾期数据并且排除已经结清的数据*/
DROP TEMPORARY TABLE IF EXISTS dmb_fin_yidian_overdue_days_detail_tmp;
CREATE TEMPORARY TABLE dmb_fin_yidian_overdue_days_detail_tmp (
  log_id varchar(50) NOT NULL COMMENT 'id ',
  plan_id varchar(50) DEFAULT NULL COMMENT '还款计划ID ',
  repayment_id varchar(50) DEFAULT NULL COMMENT '还款ID ',
  business_id varchar(50) DEFAULT NULL COMMENT '业务单号 ',
  issue_id varchar(50) DEFAULT NULL COMMENT '标的ID ',
  business_after_id varchar(50) DEFAULT NULL COMMENT '期数 ',
  actual_repayment_date datetime DEFAULT NULL COMMENT '实还日期 ',
  actual_amount decimal(10,2) NOT NULL DEFAULT '0.00',
  actual_principal decimal(10,2) NOT NULL DEFAULT '0.00',
  actual_interest decimal(10,2) NOT NULL DEFAULT '0.00',
  receivable_repayment_date datetime DEFAULT NULL COMMENT '应收日期 ',
  receivable_amount decimal(10,2) NOT NULL DEFAULT '0.00',
  receivable_principal decimal(10,2) NOT NULL DEFAULT '0.00',
  receivable_interest decimal(10,2) NOT NULL DEFAULT '0.00',
  overdue_days int(7) DEFAULT NULL,
  overdue_stage varchar(10) null DEFAULT NULL,
  index idx_dmb_fin_yidian_overdue_days_detail_tmp_business_id(business_id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
;


insert into dmb_fin_yidian_overdue_days_detail_tmp 
SELECT 
 log_id                        
,plan_id                   
,repayment_id                
,business_id               
,issue_id                    
,business_after_id           
,actual_repayment_date     
,actual_amount                 
,actual_principal              
,actual_interest               
,receivable_repayment_date 
,receivable_amount             
,receivable_principal          
,receivable_interest           
,overdue_days                  
,overdue_stage 
FROM DMB_FIN_DATA.dmb_fin_yidian_overdue_days_detail  t 
WHERE t.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
  AND t.overdue_days>0  -- 逾期
  AND NOT EXISTS
       (SELECT 1 FROM ODS_XD_TUANDAI_BM.tb_yidian_repayment_actual_ods tt 
       WHERE tt.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
	    and  tt.clear_type IN (1,2,3) 
		AND  t.business_id = tt.business_id 
		) -- 排除已经结清数据
;


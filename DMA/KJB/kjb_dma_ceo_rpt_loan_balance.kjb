DROP TEMPORARY TABLE IF EXISTS dma_ceo_rpt_loan_balance_tmp1;
CREATE TEMPORARY TABLE dma_ceo_rpt_loan_balance_tmp1 (
  business_id varchar(50) DEFAULT NULL COMMENT '业务单号 ',
  area_nm varchar(50)  NULL DEFAULT '',
  branch_nm varchar(50)  NULL DEFAULT '',
  business_type_nm varchar(50)  NULL DEFAULT '',
  customer_nm varchar(50)  NULL DEFAULT '',
  repay_type_nm int(11) DEFAULT NULL COMMENT '还款方式（1:到期本息;2:每月付息;5等额本息） ',
  borrow_limit int(11) DEFAULT NULL COMMENT '借款期限 ',
  borrow_amt decimal(10,2) NOT NULL DEFAULT '0.00',
  tot_receive_interest decimal(32,2) DEFAULT NULL,
  tot_actual_interest decimal(32,2) DEFAULT NULL,
  left_principa_amt decimal(33,2) DEFAULT NULL,
  left_interest_amt decimal(33,2) DEFAULT NULL,
  index idx_dma_ceo_rpt_loan_balance_tmp1_business_id(business_id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
;


insert into dma_ceo_rpt_loan_balance_tmp1 
 SELECT
 t.business_id   
,IFNULL(t.business_area,'未知') area_nm                               --  区域            
,IFNULL(t.branch_name,'未知') branch_nm                               --  分公司 
,'一点车贷'  AS business_type_nm                                      --   业务类型 
,t.main_customer_name                 AS customer_nm                  --  主借人
,t.repayment_type  AS repay_type_nm                                   --  还款方式 
,t.borrow_limit    AS borrow_limit 
,IFNULL(t.borrow_money,0)    AS borrow_amt                             -- 借款金额不能进行汇总
,SUM(IFNULL(t.receivable_interest,0)) AS tot_receive_interest          -- 应收总利息  
,SUM(IFNULL(t.actual_interest,0))    AS tot_actual_interest            -- 实收总利息 
-- ,due_unpaid_prin 
-- ,due_unpaid_interest
,SUM(IFNULL(t.receivable_principal,0)) - SUM(IFNULL(t.actual_principal,0))    AS left_principa_amt              -- 剩余本金 根据单来计算  借款金额 - 已还本金   
,SUM(IFNULL(t.receivable_interest,0)) - SUM(IFNULL(t.actual_interest,0))      AS left_interest_amt              -- 剩余利息       总利息 - 已还利息
FROM DWS_DATA.dws_agr_yidian_repayment_basic_info t
WHERE t.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
AND NOT EXISTS 
     (SELECT 1 FROM ODS_XD_TUANDAI_BM.tb_yidian_repayment_actual_ods tt 
       WHERE tt.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
	    and  tt.clear_type IN (1,2,3) 
		AND  t.business_id = tt.business_id 
		) -- 排除已经结清数据
GROUP BY 
 t.business_id   
,IFNULL(t.business_area,'未知')                                        --  区域            
,IFNULL(t.branch_name,'未知')                                          --  分公司 
,t.main_customer_name             	                                   --  主借人
,t.repayment_type                                                      --  还款方式 
,t.borrow_limit     
,t.borrow_money   
; 


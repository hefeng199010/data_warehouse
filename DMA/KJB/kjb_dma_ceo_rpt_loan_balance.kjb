CREATE TEMPORARY TABLE `tb_car_business_after_tmp2` (
  `business_id` VARCHAR(50) NOT NULL COMMENT '业务单号',
  `branch_id` VARCHAR(50) DEFAULT NULL COMMENT '业务所属分公司编号',
  `business_type` VARCHAR(50) DEFAULT NULL COMMENT '业务类型',
  `customer_name` VARCHAR(100) DEFAULT NULL COMMENT '客户名称',
  `repayment_type_id` INT(11) DEFAULT NULL COMMENT '还款方式ID，1到期还本息，2每月付息到期还本，5每月等额本息,9是分期还本付息',
  `borrow_limit` INT(11) DEFAULT NULL COMMENT '借款期限',
  `borrow_money` DECIMAL(12,2) DEFAULT NULL,
  `current_accrual` DECIMAL(34,2) DEFAULT NULL,
  `fact_accrual` DECIMAL(34,2) DEFAULT NULL,
  `current_Principa` DECIMAL(34,2) DEFAULT NULL,
  `fact_principa` DECIMAL(34,2) DEFAULT NULL,
  `due_unpaid_prin` DECIMAL(34,2) DEFAULT NULL,
  `due_unpaid_interest` DECIMAL(34,2) DEFAULT NULL,
  `left_principa_amt` DECIMAL(35,2) DEFAULT NULL,
  `left_interest_amt` DECIMAL(35,2) DEFAULT NULL
) ENGINE=INNODB DEFAULT CHARSET=utf8
;

INSERT INTO tb_car_business_after_tmp2  
SELECT 
 business_id
,branch_id
,business_type
,customer_name
,repayment_type_id
,borrow_limit
,MAX(borrow_money) AS borrow_money    -- 借款金额不能进行汇总
,SUM(current_accrual) AS  current_accrual   --  [本期应还利息]
,SUM(fact_accrual)    AS  fact_accrual     -- 实还利息 
,SUM(current_Principa) AS current_Principa
,SUM(fact_principa)    AS fact_principa
,SUM(due_unpaid_prin)  AS due_unpaid_prin
,SUM(due_unpaid_interest) AS due_unpaid_interest
,(MAX(borrow_money) - SUM(fact_principa) ) AS left_principa_amt              -- 剩余本金 根据单来计算  借款金额 - 已还本金   
,(SUM(current_accrual) - SUM(fact_accrual) ) AS left_interest_amt              -- 剩余利息                总利息 - 已还利息
FROM tb_car_business_after_tmp1 t
GROUP BY 
 business_id
,branch_id
,business_type
,customer_name
,repayment_type_id
,borrow_limit
;


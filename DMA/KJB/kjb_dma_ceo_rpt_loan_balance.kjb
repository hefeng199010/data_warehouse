/*贷款余额*/
insert into DMA_CEO_RPT_DATA.dma_ceo_rpt_loan_balance 
(
stat_dt                        -- 统计日期    
,data_dt                       -- 数据日期    
,business_id                   -- 业务编号    
,area_nm                       -- 片区        
,branch_nm                     -- 分公司      
,business_type_nm              -- 业务类型    
,customer_nm                   -- 客户名称    
,repay_type_nm                 -- 还款方式    
,borrow_limit                  -- 借款期限    
,borrow_amt                    -- 放款金额    
,tot_receive_interest          -- 应收总利息  
,tot_actual_interest           -- 实收总利息  
,due_unpaid_prin               -- 到期未收本金
,due_unpaid_interest           -- 到期未收利息
,left_principa_amt             -- 剩余本金    
,left_interest_amt             -- 剩余利息    
,dw_src_sys                    -- 来源系统    
,dw_src_tbl                    -- 来源表      
,dw_ins_tm                     -- 插入时间    
,dw_upd_tm                     -- 更新时间    
,dw_ins_usr                    -- 插入用户    
,dw_upd_usr                    -- 更新用户    
,part_dt                       -- 分区时间   
)
SELECT 
 DATE_FORMAT('${pt_day}','%Y-%m-%d')  AS stat_dt                       -- 统计日期
,DATE_FORMAT('${pt_day}','%Y-%m-%d')  AS data_dt                       -- 数据日期 
,t.business_id                    
,t.area_nm                            
,t.branch_nm                          
,t.business_type_nm                   
,t.customer_nm  
,CASE WHEN t.repay_type_nm in (1,2) THEN '先息后本'
      WHEN t.repay_type_nm = 5      THEN '等额本息' END  AS repay_type_nm           -- 还款方式                              
,t.borrow_limit                   
,t.borrow_amt                         
,t.tot_receive_interest               
,t.tot_actual_interest                
,t.left_principa_amt                  
,t.left_interest_amt 
,IFNULL(tt.due_unpaid_prin,0)     AS due_unpaid_prin
,IFNULL(tt.due_unpaid_interest,0) AS due_unpaid_interest
, 'tuandai_bm' AS dw_src_sys
, 'dws_agr_yidian_repayment_basic_info tb_yidian_repayment_actual_ods dmb_fin_yidian_overdue_days_detail'     AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期
FROM dma_ceo_rpt_loan_balance_tmp1 t
LEFT JOIN 
(SELECT 
t.business_id,
SUM(t.receivable_principal) - SUM(t.actual_principal) AS due_unpaid_prin, -- 逾期应还本金减去实还本金
SUM(t.receivable_interest)  - SUM(t.actual_interest) AS  due_unpaid_interest -- 逾期应还利息减去实还利息
FROM dmb_fin_yidian_overdue_days_detail_tmp t 
 GROUP BY 
 t.business_id
 ) tt
ON t.business_id = tt.business_id
;


INSERT INTO dws_agr_xd_issue_business_tmp
SELECT
 xib.issue_id
,xib.business_id
,xib.full_time       -- 启标时间(上标时间)
,xib.full_issue_date  -- 满标时间
,xib.full_borrow_money -- 满标金额

);


INSERT INTO t_rp_business_open_tmp 
SELECT 
 '2:一点数据' AS src_data            -- 数据来源：1:房车数据 2:一点数据  
,t.runperiod  AS data_dt
,t.`loan_id`
,UPPER(t.`contract_number`) AS business_id         -- 业务编号                                  
,t.issue_id            -- 标号                                        
,t.bidding_platform  AS platform_id         -- 平台：团贷网和你我金融             
,NULL AS begin_issue_date  -- 上标时间                               
,DATE_FORMAT(t.full_scale_time,'%Y-%m-%d')  AS full_issue_dt       -- 满标日期                                  
-- ,t.business_type   as business_type       -- [业务类型名称]  
,'一点车贷' AS business_type       -- [业务类型名称]                            
,t.customer_name AS issue_cut_nm        -- 客户名称                                  
,t.bidding_money AS issue_borrow_amt    -- 上标金额                                  
,t.loan_period   AS borrow_limit        -- 期限（月）                               
,t.p2p_year_rate AS loan_rate           -- 利率(年化)                                
,t.assets_arrival AS one_time_fgsfwf_fee -- 分公司服务费（一次性收取）/资产端到账（放款）
,(SELECT tcdd.dict_key FROM yidian.t_cfg_dictionary_detail tcdd WHERE tcdd.dict_value = t.repayment_mode AND tcdd.parent_dict_code = '2') AS  repay_way_nm     	
FROM      yidian.t_rp_business_open t
;

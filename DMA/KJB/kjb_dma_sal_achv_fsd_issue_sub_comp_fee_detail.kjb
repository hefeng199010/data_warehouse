  `customer_name` VARCHAR(100) DEFAULT NULL COMMENT '客户名称',
  `customer_list` VARCHAR(2000) DEFAULT NULL COMMENT '所有客户姓名,已逗号隔开',
  `full_issue_date` VARCHAR(10) DEFAULT NULL,

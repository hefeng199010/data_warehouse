  data_src VARCHAR(50) NOT NULL DEFAULT '',
  `area_nm` VARCHAR(50) NOT NULL DEFAULT '',
  `branch_nm` VARCHAR(50) NOT NULL DEFAULT '',
  `type_nm` VARCHAR(50) DEFAULT NULL,
  `platform_nm` VARCHAR(50) DEFAULT NULL,

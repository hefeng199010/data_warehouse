/*CASE WHEN t.repayment_type_id=1000 分期还本付息10年 不需要核算利差 */
ALTER TABLE DMA_SAL_ACHV_DATA.dma_sal_achv_fsd_issue_sub_comp_fee_detail TRUNCATE PARTITION dma_sal_achv_fsd_issue_sub_comp_fee_detail_${pt_day};
INSERT INTO DMA_SAL_ACHV_DATA.dma_sal_achv_fsd_issue_sub_comp_fee_detail
 SELECT
 DATE_FORMAT('${pt_day}','%Y-%m-%d')  AS stat_dt -- 统计日期
,t.area_nm                                                                                                         -- 区域
,t.branch_nm                                                                                                       -- 分公司名称                             
,t.type_nm                                                                                                         -- 类型                                   
,t.platform_nm                                                                                                     -- 平台类型                               
,t.business_id                                                                                                     -- 业务编号                               
,t.customer_name                                                                                                   -- 借款人                                 
,t.customer_list                                                                                                   -- 共借人                                 
,t.full_issue_date                                                                                                 -- 满标日期                               
,t.full_borrow_money                                                                                               -- 满标金额                               
,IFNULL(tt.cut_interest    ,0)                                                  AS cut_interest                    -- 前置收入                               
,IFNULL(tt.one_tm_ssfy_fee ,0)                                                  AS one_tm_ssfy_fee                 -- 返佣费                                 
,(CASE WHEN t.repayment_type_id=1000 THEN IFNULL(tt.one_tm_fgsfw_fee,0) ELSE (t.full_borrow_money * t.fgsfw_one_fee_rate) END )                                  AS one_tm_fgsfw_fee_zb             -- 分公司服务费放款前一次性收取-总部标准值 分期还本付息10年 不需要核算利差默认给分公司定价
,IFNULL(tt.one_tm_fgsfw_fee,0)                                                  AS one_tm_fgsfw_fee_fgs            -- 分公司服务费放款前一次性收取-分公司定价
,(IFNULL(tt.one_tm_fgsfw_fee,0) - (CASE WHEN t.repayment_type_id=1000 THEN IFNULL(tt.one_tm_fgsfw_fee,0) ELSE (t.full_borrow_money * t.fgsfw_one_fee_rate) END ) )  AS one_tm_fgsfw_fee_diff           -- 分公司服务费放款前一次性收取-差异化    
,(CASE WHEN t.repayment_type_id=1000 THEN IFNULL(tt.month_fgsfw_fee,0) ELSE (t.full_borrow_money * t.fgsfw_mth_fee_rate)  END )                                 AS month_fgsfw_fee_zb              -- 分公司服务费每月收取-总部标准值  分期还本付息10年 不需要核算利差默认给分公司定价      
,IFNULL(tt.month_fgsfw_fee,0)                                                   AS month_fgsfw_fee_fgs             -- 分公司服务费每月收取-分公司定价        
,(IFNULL(tt.month_fgsfw_fee,0) - (CASE WHEN t.repayment_type_id=1000 THEN IFNULL(tt.month_fgsfw_fee,0) ELSE (t.full_borrow_money * t.fgsfw_mth_fee_rate)  END ) )   AS month_fgsfw_fee_diff            -- 分公司服务费每月收取-差异化    
,'tuandai_bm' AS dw_src_sys
,'dws_agr_xd_issue_business,tb_business_ods,tb_fee_config_ods,tb_department_ods,tb_business_apply_output_ods' AS dw_src_tbl
,CURRENT_TIMESTAMP AS dw_ins_tm
,CURRENT_TIMESTAMP AS dw_upd_tm
,'caihl'           AS dw_ins_usr
,'caihl'           AS dw_upd_usr
,DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期        
 FROM tb_business_ods_tmp t
 LEFT JOIN tb_business_apply_output_tmp1 tt ON t.business_id = tt.business_id
 ;

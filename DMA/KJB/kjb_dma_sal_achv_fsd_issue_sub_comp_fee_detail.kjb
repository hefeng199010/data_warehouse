/*非36期的数据*/
INSERT INTO tb_business_ods_tmp
 SELECT 
  '非36期的数据' AS  data_src
 ,IFNULL(dep1.dept_name,'未知')              AS area_nm                                         #区域            
 ,IFNULL(dep2.dept_name,'未知')              AS branch_nm                                       #分公司 
 ,(CASE WHEN t.business_type IN ('T600') THEN '展期' ELSE '新增' END )                    AS type_nm  -- 类型 
 ,(CASE WHEN t.platform_id =1 THEN '团贷网' WHEN t.platform_id = 2 THEN '你我金融' END )  AS platform_nm -- 平台类型 团贷网/你我金融
 ,t.business_id
 ,t.customer_name
 ,t.customer_list                                        -- 共借人
 ,DATE_FORMAT(ib.full_issue_date,'%Y-%m-%d')  AS full_issue_date -- 满标日期
 ,ib.full_borrow_money                                   -- 满标金额
 ,IFNULL(cfg.fee_value*0.01,1)  AS fgsfw_one_fee_rate  -- 分公司服务费一次性收取费率 百分比
 ,IFNULL(cfg1.fee_value*0.01,1) AS fgsfw_mth_fee_rate -- 分公司服务费月收取费率 百分比
 ,t.repayment_type_id -- CASE WHEN t.repayment_type_id=1000 分期还本付息10年 不需要核算利差
FROM ODS_XD_TUANDAI_BM.tb_business_ods t
INNER JOIN dws_agr_xd_issue_business_tmp ib  -- 上标
ON t.business_id = ib.business_id
LEFT JOIN (SELECT BUSINESS_ID,
             CASE WHEN SUM(IFNULL(BORROW_BALANCE,0))>0 THEN '二抵' ELSE '一抵' END AS dy_flg 
             FROM ODS_XD_TUANDAI_BM.tb_fsd_house_ods  WHERE part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')
             GROUP BY BUSINESS_ID 
          )fsd ON t.business_id = fsd.BUSINESS_ID                                                           
LEFT JOIN                                                                                   
 (SELECT  DISTINCT                                                                                  
    DEPT_ID                                                                                    
   ,business_type_detail -- 一抵/二抵                                                          
   ,repayment_type           -- 还款方式，1为到期还本息，2为每月付息到期还本，5为每月等额本息  
   ,loan_term               -- 借款期限                                                        
   ,fee_value                                                                                  
   FROM ODS_XD_TUANDAI_BM.tb_fee_config_ods t                                                                        
   WHERE t.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d') 
   AND t.fee_pid IN ('d1cdaf6e-d095-4d63-9138-9d6c8b270a25')                                
   AND t.IsSystem IS FALSE                                                                     
   AND t.fee_name IN ('分公司服务费（公司标准值）')           
  -- AND t.is_one_time_charge=2 -- 是否一次收取，1为按月收取，2为一次收取                        
  -- AND t.fee_charging_type =1  -- 费用收取方式，1为按比例，2为按金额  
   AND (t.DEPT_ID IS NOT NULL AND t.business_type_detail IS NOT NULL AND  t.repayment_type IS NOT NULL AND t.loan_term IS NOT NULL)                         
 ) cfg   ON t.branch_id = cfg.DEPT_ID   AND fsd.dy_flg =  cfg.business_type_detail AND t.repayment_type_id = cfg.repayment_type AND t.borrow_limit = cfg.loan_term 
 LEFT JOIN                                                                                   
 (SELECT DISTINCT                                                                                    
    DEPT_ID                                                                                    
   ,business_type_detail     -- 一抵/二抵                                                          
   ,repayment_type           -- 还款方式，1为到期还本息，2为每月付息到期还本，5为每月等额本息  
   ,loan_term               -- 借款期限                                                        
   ,fee_value                                                                                  
   FROM ODS_XD_TUANDAI_BM.tb_fee_config_ods t                                                                        
  WHERE t.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d') 
   AND  t.fee_pid IN ('d1cdaf6e-d095-4d63-9138-9d6c8b270a25')                                
   AND t.IsSystem IS FALSE                                                                     
   AND t.fee_name IN ('月收分公司服务费（公司标准值）')           
  -- AND t.is_one_time_charge=1  -- 1为按月收取                       
  -- AND t.fee_charging_type =1  -- 费用收取方式，1为按比例，2为按金额  
   AND (t.DEPT_ID IS NOT NULL AND t.business_type_detail IS NOT NULL AND  t.repayment_type IS NOT NULL AND t.loan_term IS NOT NULL)                         
 ) cfg1   ON t.branch_id = cfg1.DEPT_ID   AND fsd.dy_flg =  cfg1.business_type_detail AND t.repayment_type_id = cfg1.repayment_type AND t.borrow_limit = cfg1.loan_term    
LEFT JOIN 
  (
   SELECT 
   dep.dept_id,
   dep.district_id AS area_id
  FROM  ODS_XD_TUANDAI_BM.tb_department_ods dep -- 部门信息表
   WHERE dep.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
  ) dep
   ON  t.branch_id = dep.dept_id
 LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')   
  ) dep1 ON dep.area_id = dep1.DEPT_ID 
  LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')  
  ) dep2  ON t.branch_id = dep2.DEPT_ID 
WHERE t.part_dt= DATE_FORMAT('${pt_day}','%Y-%m-%d')
AND   t.business_type IN ('房速贷标准件','房速贷非标准件','T600') 
AND   ib.full_issue_date >= DATE_FORMAT('2018-07-01','%Y-%m-%d')  -- 2018/07/01（含）后上标数据(满标)
AND   IFNULL(t.borrow_limit,1) &lt;&gt;36
;
  
 

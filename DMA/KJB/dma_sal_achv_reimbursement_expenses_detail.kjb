      <sql>

SET SESSION tmp_table_size=1024*1024*1024;
SET SESSION max_heap_table_size=1024*1024*1024;
SET SESSION sql_mode='NO_ENGINE_SUBSTITUTION';


 -- 费用报销表
INSERT INTO dma_sal_achv_reimbursement_expenses_detail(
accounting_time,
business_id,
AREA,
province,
business_company_name,
law_offices,
business_type_name,
apply_user,
customer_name,
fee_term,
payee,
fee_money,
dw_src_sys,
dw_src_tbl,
dw_ins_tm,
dw_upd_tm,
dw_ins_usr,
dw_upd_usr,
part_dt
)
SELECT  DISTINCT
b.`accounting_time`,-- 报销时间  
b.`business_id`,-- 业务编号
CASE WHEN a.business_type='一点车贷' THEN
		(SELECT t1.`area` FROM ODS_XD_TUANDAI_BM.`business_company_area_ods` t1 JOIN ODS_XD_TUANDAI_BM.`business_company_yd_ods` t2 ON t1.province_name=t2.province_name WHERE t2.business_company_name=a.business_company_name LIMIT 1)
	     ELSE 
	         (SELECT t1.`area` FROM ODS_XD_TUANDAI_BM.`business_company_area_ods` t1 JOIN ODS_XD_TUANDAI_BM.`business_company_ht_ods` t2 ON t1.province_name=t2.province_name WHERE t2.business_company_name=a.business_company_name LIMIT 1)
	END AREA,
	CASE WHEN a.business_type='一点车贷' THEN
		(SELECT tt.`province_name` FROM ODS_XD_TUANDAI_BM.`business_company_yd_ods` tt WHERE tt.business_company_name=a.business_company_name LIMIT 1)
	     ELSE 
	         (SELECT tt.`province_name` FROM ODS_XD_TUANDAI_BM.`business_company_ht_ods` tt WHERE tt.business_company_name=a.business_company_name LIMIT 1) 
	END province_name,
a.business_company_name,-- 分公司
d.law_office,-- 律所
a.business_type_name,-- 业务类型 
(SELECT c.`apply_user` FROM `ODS_XD_TUANDAI_BM`.`tb_litigation_action_plan_lawsuit_ods` c WHERE  c.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d') AND c.business_id=a.business_id ) apply_user,-- 申请人/原告
a.`customer_name`, -- 客户名称
(SELECT DISTINCT a.param_name FROM `ODS_XD_TUANDAI_BM`.`tb_parameter_lawsuit_ods` a WHERE a.param_type IN('feeTermExpenditure','feeTermIncome','feeTermOther') AND a.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d') AND a.param_int_value=b.fee_term ) param_name,-- 费用项
(SELECT t.payee FROM ODS_XD_TUANDAI_BM.tb_litigation_fee_register_lawsuit_ods t WHERE t.`fee_type`=2 AND t.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d') AND t.business_id=a.business_id AND t.fee_term=b.fee_term AND t.id=b.id) payee, -- 收款单位
b.fee_money, -- 金额
'ODS_XD_TUANDAI_BM' AS dw_src_sys,
'tb_litigation_fee_register,tb_litigation,tb_litigation_attorney_info,tb_litigation_action_plan' AS dw_src_tbl,
NOW() AS dw_ins_tm,
NOW() AS dw_upd_tm,
'hefeng' AS dw_ins_usr,
'hefeng' AS dw_upd_usr,
DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期
FROM
ODS_XD_TUANDAI_BM.tb_litigation_lawsuit_ods a 
JOIN 
ODS_XD_TUANDAI_BM.tb_litigation_fee_register_lawsuit_ods b
ON a.`business_id`=b.`business_id`
LEFT JOIN  (SELECT GROUP_CONCAT(t.`law_office`) law_office,t.`business_id` FROM ODS_XD_TUANDAI_BM.tb_litigation_attorney_info_lawsuit_ods t WHERE t.`type`=1 GROUP BY t.`business_id` AND t.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d')) d
ON a.`business_id`=d.business_id
WHERE 
  b.`fee_type`=2  
 AND b.accounting_time IS NOT NULL
 AND a.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d')
 AND b.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d')</sql>

ALTER TABLE DMA_SAL_ACHV_DATA.dma_sal_achv_fin_settle_detail_his TRUNCATE PARTITION dma_sal_achv_fin_settle_detail_his_${pt_day};
INSERT INTO DMA_SAL_ACHV_DATA.dma_sal_achv_fin_settle_detail_his
SELECT
 '1:房车线上数据' AS src_data
,DATE_FORMAT('${pt_day}','%Y%m%d') AS data_dt
,xb.business_id  -- 业务单号
,b.issue_id      -- 标的
,b.full_time     AS begin_issue_date  -- 上标时间
,b.full_issue_date                    -- 满标时间
,xb.business_type -- 业务类型
,xb.platform_nm   -- 上标平台
,xb.customer_name -- 借款人姓名
,b.full_borrow_money                  -- 满标金额
,xb.apply_borrow_limit                -- 借款期限
-- ,xb.apply_repayment_type_id
,xb.repay_way_nm                      -- 还款方式
,xb.loan_rate                         -- 借款利率
,xb.fatct_replayDate                  -- 最后一期实际还款日期
,xb.finance_confirmed_date            -- 最后一期财务确认还款日期
,xb.settle_way                        -- 结清状态
,xb.is_settled                              -- 是否结清 add by caihl20181120
,ti.BranchCompanyId AS contract_branch_id   -- 合同主体分公司id
,rt.branch_name     AS contract_branch_name -- 合同主体分公司名称
, 'tuandai_bm' AS dw_src_sys
, 'dws_agr_xd_business,dim_evt_xd_repay_way,dim_prd_xd_business_type,dws_agr_xd_issue_business' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期     
FROM dws_agr_xd_business_tmp xb
INNER JOIN dws_agr_xd_issue_business_tmp b ON xb.business_id = b.business_id
LEFT JOIN resl_temp0 rt                    ON xb.business_id = rt.business_id
LEFT JOIN (SELECT  ti.IssueId, ti.BranchCompanyId FROM ODS_XD_TUANDAI_BM.tb_issue_ods ti WHERE ti.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')) ti ON b.issue_id = ti.IssueId
;


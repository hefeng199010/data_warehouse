-- 税务会计结清明细表
DROP TEMPORARY TABLE IF EXISTS dws_agr_xd_business_tmp;
CREATE TEMPORARY TABLE dws_agr_xd_business_tmp (
  business_id VARCHAR(50) NOT NULL DEFAULT '' COMMENT '业务单号',
  business_type VARCHAR(50) DEFAULT NULL COMMENT '业务类型',
  platform_nm VARCHAR(4) DEFAULT NULL,
  customer_name VARCHAR(50) DEFAULT NULL COMMENT '客户名称',
  apply_borrow_limit INT(11) DEFAULT NULL COMMENT '申请借款期限',
  apply_repayment_type_id INT(11) DEFAULT NULL COMMENT '线下出款则为null，还款方式ID，1到期还本息，2每月付息到期还本，5每月等额本息',
  repay_way_nm VARCHAR(50) DEFAULT NULL COMMENT '还款方式名称',
  loan_rate DECIMAL(10,4) DEFAULT NULL COMMENT '年化利率',
  finance_confirmed_date DATETIME DEFAULT NULL COMMENT '财务实际结清时间',
  fatct_replayDate DATETIME DEFAULT NULL COMMENT '实际结清时间',
  settle_way VARCHAR(50) DEFAULT NULL COMMENT '结清方式，1表示正常结清，2表示逾期结清，3表示提前结清',
  is_settled VARCHAR(50) DEFAULT NULL COMMENT '是否结清',
  KEY idx_business_id (business_id)
) ENGINE=INNODB DEFAULT CHARSET=utf8
;

INSERT INTO dws_agr_xd_business_tmp
SELECT 
 t.business_id
,typ.business_type_nm AS business_type
,(CASE WHEN t.platform_id=1 THEN '团贷网' WHEN t.platform_id=2 THEN '你我金融' ELSE platform_id END) AS  platform_nm  -- [平台编号 1：团贷网 2：你我金融]
,t.customer_name
,t.apply_borrow_limit      -- 借款期限 (case when tb.business_type ='T600' then ifnull(bec.extension_period,tb.borrow_limit) else tb.borrow_limit end)
,t.apply_repayment_type_id -- 还款方式
,rw.repay_way_nm
,t.loan_rate
,t.fin_act_settle_dt AS finance_confirmed_date   -- 财务实际结清时间
,t.act_settle_dt     AS fatct_replayDate          -- 实际还款日期
,(CASE WHEN t.settle_way&lt;0 THEN '未结清' when t.settle_way=1 THEN '正常结清' WHEN  t.settle_way=2 THEN '逾期结清' WHEN  t.settle_way=3 THEN '提前结清' ELSE t.settle_way END) AS settle_way
,(CASE WHEN t.settle_way>0 THEN '结清' ELSE '未结清' END) AS is_settled -- 是否结清
FROM DWS_DATA.dws_agr_xd_business t
LEFT JOIN DIM_DATA.dim_evt_xd_repay_way  rw ON t.apply_repayment_type_id = rw.repay_way_id
LEFT JOIN DIM_DATA.dim_prd_xd_business_type typ ON t.business_type = typ.business_type
WHERE t.part_dt=DATE'${pt_day}'
-- AND t.settle_way>0  
;

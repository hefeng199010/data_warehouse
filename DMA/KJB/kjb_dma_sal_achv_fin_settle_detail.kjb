/*part2:一点车贷数据*/     
INSERT INTO DMA_SAL_ACHV_DATA.dma_sal_achv_fin_settle_detail_his
SELECT
 '2:一点车贷数据' AS src_data
,tl.runperiod AS data_dt
-- ,DATE_FORMAT('${pt_day}','%Y%m%d') AS data_dt
,UPPER(tl.contract_number)       AS business_id                   
,IF(tl.has_disassembled = '0',lb.issue_id,
     (SELECT tlbb.issue_id FROM yidian.t_loan_bidding tlbb 
     WHERE tlbb.loan_id IN (SELECT tll.id FROM yidian.t_loan tll WHERE tll.disassembled_id = tl.id AND tll.has_main = '1' AND tll.status = 1 ))
     ) issue_id            -- 拆标标的处理
,lb.create_time AS begin_issue_date  -- 上标时间（暂时这样取）
,lb.full_scale_time        --  满标时间 
,'一点车贷' AS business_type -- 业务类型  
-- ,(SELECT tcdd.dict_key FROM yidian.t_cfg_dictionary_detail tcdd WHERE tcdd.dict_value = tl.loan_product AND tcdd.parent_dict_code = '1') as business_type -- 业务类型  
,tl.bidding_platform  AS platform_nm    -- 上标平台     
,thcl.customer_name AS customer_name  -- 借款人姓名   
,tl.bidding_money     AS  full_borrow_money     -- 满标金额
,tl.loan_period       AS apply_borrow_limit     -- 借款期限
,(SELECT tcdd.dict_key FROM yidian.t_cfg_dictionary_detail tcdd WHERE tcdd.dict_value = tl.repayment_mode AND tcdd.parent_dict_code = '2') AS  repay_way_nm   -- 还款方式(等本等息:8000、先息后本:8001)
,(SELECT thlf.fee_value FROM yidian.t_hi_loan_fee thlf WHERE thlf.loan_id = tr.loan_id AND fee_name = '月利率') AS loan_rate -- jkylilv  
,tr.settle_date  AS fatct_replayDate              -- 结清日期  
,(SELECT IF(trra.refund_channel = '1' OR trra.refund_channel = '2' OR trra.refund_channel = '7' OR trra.refund_channel = '8'
        ,(SELECT DATE_FORMAT(trd.daikou_time,'%Y-%m-%d %H:%i:%s') FROM yidian.t_refund_daikou trd WHERE trra.hx_batch_code = trd.hx_batch_code AND trra.loan_id = trd.loan_id AND trd.status IN ('1','3'))
        ,IF(trra.refund_channel = '3'
            ,(SELECT DATE_FORMAT(trt.approval_time,'%Y-%m-%d %H:%i:%s') FROM yidian.t_refund_transfer trt WHERE trra.hx_batch_code = trt.hx_batch_code AND trra.loan_id = trt.loan_id AND trt.approve_type = '1')
            ,IF(trra.refund_channel = '6'
                ,(SELECT DATE_FORMAT(tyca.create_time,'%Y-%m-%d %H:%i:%s') FROM yidian.t_yk_charge_against tyca WHERE trra.hx_batch_code = tyca.hx_batch_code AND trra.loan_id = tyca.loan_id)
                ,DATE_FORMAT(trra.create_time,'%Y-%m-%d %H:%i:%s')
              )
          )) FROM yidian.t_refund_actual trra WHERE trra.loan_id = tr.loan_id  ORDER BY trra.create_time DESC LIMIT 1) AS finance_confirmed_date -- hx_date   
,(CASE WHEN tr.settle_type='1' THEN '正常结清' WHEN tr.settle_type='2' THEN '提前结清' ELSE '未结清' END)                 AS settle_way                        -- 结清状态
,(CASE WHEN tr.settle_type>0 THEN '结清' ELSE '未结清' END) AS  is_settled           -- 是否结清 add by caihl20181120
,tl.org_depository_account AS contract_branch_id   -- 合同主体分公司id
,cda.org_name              AS contract_branch_name -- 合同主体分公司名称  
, 'yidian'  AS dw_src_sys
, 't_refund,t_loan_bidding,t_hi_customer_loan,t_cfg_dictionary_detail,t_hi_loan_fee,t_refund_daikou,t_refund_transfer,t_yk_charge_against,t_refund_actual' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr 
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期           
 FROM yidian.t_loan tl
 INNER JOIN yidian.t_refund tr              ON tl.id = tr.loan_id  -- loan_id 唯一
 LEFT JOIN yidian.t_loan_bidding lb         ON tr.loan_id=lb.loan_id
 LEFT JOIN yidian.t_hi_customer_loan thcl   ON thcl.loan_id = tr.loan_id
 LEFT JOIN yidian.t_cfg_capital_deposit_account cda ON cda.depository_account=tl.org_depository_account -- 资金端存管账号 
WHERE tl.status='1' -- 正常 add by caihl20181120
;



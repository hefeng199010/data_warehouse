DROP TEMPORARY TABLE IF EXISTS `dma_fsd_tra_detail_tmp1`;
CREATE TEMPORARY TABLE `dma_fsd_tra_detail_tmp1` (
	`business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务编号',
	`is_extension` INT(11) NULL DEFAULT NULL COMMENT '是否展期 1：是 0：否',
	`issue_type` INT(11) NULL DEFAULT NULL COMMENT '发标类型0:线下放款, 1:P2p,2:定期理财',
	`customer_list` VARCHAR(2000) NULL DEFAULT NULL COMMENT '所有客户姓名,已逗号隔开',
	`input_time` DATETIME NULL DEFAULT NULL COMMENT '受理时间',
	`process_modify_time` DATETIME NULL DEFAULT NULL COMMENT '流程修改时间',
	`audit_borrow_limit` INT(11) NULL DEFAULT NULL COMMENT '审批借款期限',
	`audit_borrow_money` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '审批借款金额',
	`approve_rate` VARCHAR(50) NULL DEFAULT NULL COMMENT '最终借款利率',
	`repay_status_subtype` INT(11) NULL DEFAULT NULL COMMENT '还款状态类别，详见dim_data.dim_evt_xd_repay_status_subtype',
	`area_id` VARCHAR(51) NULL DEFAULT NULL COMMENT '区域编号',
	`branch_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务所属分公司编号',
	`type_name` VARCHAR(100) NULL DEFAULT NULL COMMENT '业务类型',
	`plan_settle_dt` DATETIME NULL DEFAULT NULL COMMENT '计划结清时间',
	`act_settle_dt` DATETIME NULL DEFAULT NULL COMMENT '计划结清时间',
	`user_name` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务获取人编号',
	`apply_repayment_type_id` INT(11) NULL DEFAULT NULL COMMENT '线下出款则为null，还款方式ID，1到期还本息，2每月付息到期还本，5每月等额本息',
KEY `dma_fsd_tra_detail_tmp1_business_id`(business_id),
KEY `dma_fsd_tra_detail_tmp1_area_id`(area_id),
KEY `dma_fsd_tra_detail_tmp1_branch_id`(branch_id),
KEY `dma_fsd_tra_detail_tmp1_apply_repayment_type_id`(apply_repayment_type_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8;

INSERT INTO dma_fsd_tra_detail_tmp1
select   t1.business_id,
		t1.is_extension,
		t1.issue_type,
		t1.customer_list,
		t1.input_time,
		t1.process_modify_time,
		t1.apply_borrow_limit as audit_borrow_limit, -- update 20180411
		t1.apply_borrow_money as audit_borrow_money, -- update 20180411
		t1.approve_rate,
		t1.repay_status_subtype,
		t1.area_id,
		t1.branch_id,
		t2.type_name,
		t1.plan_settle_dt,
		t1.act_settle_dt,
		a4.user_name,
		t1.apply_repayment_type_id
from (select t1.business_id,
		t1.is_extension,
		t1.issue_type,
		t1.customer_list,
		t1.input_time,
		t1.process_modify_time,
		t1.apply_borrow_limit,
		t1.apply_borrow_money,
		t1.approve_rate,
		t1.repay_status_subtype,
		t1.area_id,
		t1.branch_id,
		t1.plan_settle_dt,
		t1.act_settle_dt,
		t1.apply_repayment_type_id,
		 t1.business_type,
		 t1.original_id
        from DWS_DATA.dws_agr_xd_business t1
       where t1.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
        and t1.business_type in ('房速贷标准件','房速贷非标准件','T600') 
  )t1
left join ODS_XD_TUANDAI_BM.tb_business_type_ods t2
       on t1.business_type = t2.TYPE_ID
	  and t2.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
left join ODS_XD_TUANDAI_BM.tb_user_ods a4
      ON t1.original_id=a4.user_id
   AND a4.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')  ;



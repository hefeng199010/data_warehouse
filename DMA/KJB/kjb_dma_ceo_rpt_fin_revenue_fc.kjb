DROP  TEMPORARY TABLE IF EXISTS `tb_business_ods_tmp`;
CREATE TEMPORARY TABLE `tb_business_ods_tmp` (
  `area_nm` VARCHAR(50) NOT NULL DEFAULT '',
  `branch_nm` VARCHAR(50) NOT NULL DEFAULT '',
  `business_type_nm` VARCHAR(3) DEFAULT NULL,
  `borrow_limit` INT(11) DEFAULT NULL COMMENT '借款期限',
  `repay_type_nm` VARCHAR(50) NOT NULL DEFAULT '',
  `customer_nm` VARCHAR(100) DEFAULT NULL COMMENT '客户名称',
  `business_id` VARCHAR(50) NOT NULL COMMENT '业务单号',
  `borrow_amt` DECIMAL(12,2) DEFAULT NULL COMMENT '借款金额',
  INDEX tb_business_ods_tmp_business_id(business_id)
) ENGINE=INNODB DEFAULT CHARSET=utf8
;

INSERT INTO tb_business_ods_tmp 
SELECT
IFNULL(dep1.dept_name,'未知') area_nm                                         #区域            
,IFNULL(dep2.dept_name,'未知') branch_nm                                         #分公司 
,CASE WHEN t.business_type IN ('T600','房速贷标准件','房速贷非标准件') THEN '房速贷' 
      WHEN t.business_type IN ('T500','车易贷') THEN '车易贷' END   AS business_type_nm                     # 业务类型 
,t.borrow_limit    AS borrow_limit
,IFNULL(rw.repay_way_nm,'未知')  AS repay_type_nm                                     #还款方式 
,t.customer_name                 AS customer_nm 
,t.business_id  								     #主借人
,borrow_money    AS borrow_amt   -- 借款金额不能进行汇总
  FROM ODS_XD_TUANDAI_BM.tb_business_ods t
 LEFT JOIN 
  (
   SELECT 
   dep.dept_id,
   dep.district_id AS area_id
  FROM  ODS_XD_TUANDAI_BM.tb_department_ods dep -- 部门信息表
   WHERE dep.part_dt = DATE_FORMAT('${p_day}','%Y-%m-%d') 
  ) dep
   ON  t.branch_id = dep.dept_id
 LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${p_day}','%Y-%m-%d')   -- 报表按小时跑批，部门表按天跑
  ) dep1 ON dep.area_id = dep1.DEPT_ID 
  LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${p_day}','%Y-%m-%d')  
  ) dep2  ON t.branch_id = dep2.DEPT_ID 
 LEFT JOIN DIM_DATA.dim_evt_xd_repay_way rw 
  ON t.repayment_type_id = rw.repay_way_id
WHERE t.part_dt = DATE_FORMAT('${p_day}','%Y-%m-%d')
 AND  t.business_type IN ('T600','房速贷标准件','房速贷非标准件','T500','车易贷')
 AND  t.business_id NOT LIKE '%ZQ%' -- 排除展期数据
  ;
/* 获取原业务单号的区域等维度信息  end*/



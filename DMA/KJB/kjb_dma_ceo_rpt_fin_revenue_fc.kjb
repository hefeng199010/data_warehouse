,extsn_nofsh_srv_amt
,extsn_charge_amt
,extsn_nofsh_other_amt
,current_Breach
,deferred_pay_amt
,day_visit_amt
,fsh_stop_amt
,day_lawyer_amt
,day_adv_settle_amt
,day_Litigation_amt
,day_trail_amt
,court_accpt_amt
,day_ovd_brh_srv_amt
,overdue_acal
,day_ovd_income_amt
,other_money
,day_margin_amt
,dw_src_sys
,dw_src_tbl
,dw_ins_tm
,dw_upd_tm
,dw_ins_usr
,dw_upd_usr
,part_dt
)
SELECT 
 DATE_FORMAT('${p_day}','%Y-%m-%d')  AS stat_dt                         -- 统计日期
,DATE_FORMAT(IFNULL(ad.actual_repayment_date,'4712-12-31'),'%Y-%m-%d') AS actual_repayment_date
,t.area_nm                        -- 片区                           
,t.branch_nm                      -- 分公司                         
,t.business_type_nm               -- 业务类型                       
,t.borrow_limit                   -- 借款期限                       
,t.repay_type_nm                  -- 还款方式                       
,t.business_id                    -- 业务编号                       
,t.customer_nm                    -- 客户名称                       
,t.borrow_amt                     -- 放款额                         
,IFNULL(ad.comp_mon_srv_amt,0) AS comp_mon_srv_amt                      -- 分公司费用:月收服务费(金额)    
,IFNULL(ad.comp_one_tm_srv_amt,0) AS comp_one_tm_srv_amt                -- 分公司费用:一次性服务费（金额）
,IFNULL(ad.mon_srv_pct_amt,0) AS mon_srv_pct_amt                        -- 分公司费用:月收服务费(比例)    
,IFNULL(ad.agency_amt,0) AS agency_amt                                  -- 分公司费用:中介费              
,IFNULL(ad.admin_amt,0) AS admin_amt                                    -- 分公司费用:行政综合费          
,IFNULL(ad.repay_evaluate_money,0) AS repay_evaluate_money              -- 分公司费用:评估费              
,IFNULL(ad.gps_cost_amt,0) AS gps_cost_amt                              -- 分公司费用:GPS费用             
,IFNULL(ad.comp_stop_amt,0) AS comp_stop_amt                            -- 分公司费用:停车费              
,IFNULL(ad.acct_mgr_amt,0) AS acct_mgr_amt                              -- 分公司费用:账户管理费          
,IFNULL(ad.consult_file_amt,0) AS consult_file_amt                      -- 分公司费用:查档费              
,IFNULL(ad.plat_mon_srv_amt,0) AS plat_mon_srv_amt                      -- 团贷网平台费用:月收平台费      
,IFNULL(ad.plat_one_tm_srv_amt,0) AS plat_one_tm_srv_amt                -- 团贷网平台费用:一次性收平台费  
,IFNULL(ad.one_tm_db_fee,0) AS one_tm_db_fee                            -- 担保费                         
,IFNULL(ad.consult_amt,0) AS consult_amt                                -- 咨询费                         
,IFNULL(ad.maintenamce_amt,0) AS maintenamce_amt                        -- 保全费                         
,IFNULL(ad.extsn_nofsh_srv_amt,0) AS extsn_nofsh_srv_amt                -- 展期费用:未结清服务费          
,IFNULL(ad.extsn_charge_amt,0) AS extsn_charge_amt                      -- 展期费用:手续费                
,IFNULL(ad.extsn_nofsh_other_amt,0) AS extsn_nofsh_other_amt            -- 展期费用:未结清其他费用        
,IFNULL(ad.current_Breach,0) AS current_Breach                          -- 滞纳金                         
,IFNULL(ad.deferred_pay_amt,0) AS deferred_pay_amt                      -- 延迟履行金                     
,IFNULL(ad.day_visit_amt,0) AS day_visit_amt                            -- 结清固定费用:上门费            
,IFNULL(ad.fsh_stop_amt,0) AS fsh_stop_amt                              -- 结清固定费用:停车费            
,IFNULL(ad.day_lawyer_amt,0) AS day_lawyer_amt                          -- 结清固定费用:律师费            
,IFNULL(ad.day_adv_settle_amt,0) AS day_adv_settle_amt                  -- 结清固定费用:提前结清违约金    
,IFNULL(ad.day_Litigation_amt,0) AS day_Litigation_amt                  -- 结清固定费用:诉讼费            
,IFNULL(ad.day_trail_amt,0) AS day_trail_amt                            -- 结清固定费用:拖车费            
,IFNULL(ad.court_accpt_amt,0) AS court_accpt_amt                        -- 结清固定费用:案件受理费        
,IFNULL(ad.day_ovd_brh_srv_amt,0) AS day_ovd_brh_srv_amt                -- 结清固定费用:逾期分公司服务费  
,IFNULL(ad.overdue_acal,0) AS overdue_acal                              -- 结清固定费用:逾期利息          
,IFNULL(ad.day_ovd_income_amt,0) AS day_ovd_income_amt                  -- 结清固定费用:逾期收入          
,IFNULL(ad.other_money,0) AS other_money                                -- 结清固定费用:其他费用          
,IFNULL(ad.day_margin_amt,0) AS day_margin_amt                          -- 利差  
, 'tuandai_bm' AS dw_src_sys
, 'tb_business_ods,tb_car_business_after_detail_ods,tb_department_ods' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr
, DATE_FORMAT('${p_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期                          
 FROM tb_business_ods_tmp t
inner  JOIN tb_car_business_after_detail_tmp ad
ON t.`business_id` = ad.`business_id`
;
/* 生成目标表his  end*/


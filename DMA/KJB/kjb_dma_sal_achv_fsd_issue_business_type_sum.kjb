INSERT INTO tb_business_ods_tmp1
SELECT 
 tb.business_id
,tb.branch_id
,tb.repay_type_nm                                   #还款方式
,fh.dy_flg 
,DATE_FORMAT(ib.full_issue_date,'%Y-%m-%d') AS full_issue_date
,IFNULL(ib.borrow_money,0) AS borrow_money          #满标金额
,tb.borrow_limit

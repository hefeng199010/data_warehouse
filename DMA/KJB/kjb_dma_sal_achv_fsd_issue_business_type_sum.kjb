DROP TEMPORARY TABLE IF EXISTS tb_car_business_after_detail_tmp2;
CREATE TEMPORARY TABLE tb_car_business_after_detail_tmp2 (
  business_id VARCHAR(50) DEFAULT NULL COMMENT '业务编号',
  business_after_id VARCHAR(50) DEFAULT NULL COMMENT '期数',
  plan_repayment_date VARCHAR(10) DEFAULT NULL,
  actual_repayment_date VARCHAR(10) DEFAULT NULL,
  fee_name VARCHAR(50) DEFAULT NULL COMMENT '费用名称',
  plan_fee_value DECIMAL(12,2) DEFAULT NULL COMMENT '[本期应还金额]',
  actual_fee_value DECIMAL(12,2) DEFAULT NULL COMMENT '[本期实还金额]',
  branch_id VARCHAR(50) DEFAULT NULL COMMENT '业务所属分公司编号',
  recv_month_pt_fee DECIMAL(12,2) DEFAULT NULL,
  fact_month_pt_fee DECIMAL(12,2) DEFAULT NULL,
  recv_month_fgsfw_fee DECIMAL(12,2) DEFAULT NULL,
  fact_month_fgsfw_fee DECIMAL(12,2) DEFAULT NULL,
  recv_month_interest_fee DECIMAL(12,2) DEFAULT NULL,
  fact_month_interest_fee DECIMAL(12,2) DEFAULT NULL,
  KEY idx_business_id (business_id)
) ENGINE=INNODB DEFAULT CHARSET=utf8;

INSERT INTO tb_car_business_after_detail_tmp2
SELECT 
 tab_detail.business_id
,tab_detail.business_after_id
,DATE_FORMAT(tab_detail.plan_repayment_date,'%Y-%m-%d')  AS plan_repayment_date   -- 应收日期
,DATE_FORMAT(tab_detail.actual_repayment_date,'%Y-%m-%d')  AS actual_repayment_date -- 实收日期
,tab_detail.fee_name
,tab_detail.plan_fee_value
,tab_detail.actual_fee_value
,t1.branch_id
,CASE WHEN  tab_detail.fee_name LIKE '%分公司服务费%' AND tab_detail.fee_name NOT IN ('提前结清分公司服务费违约金','逾期分公司服务费') THEN IFNULL(tab_detail.plan_fee_value,0) ELSE 0 END AS recv_month_pt_fee    -- 应收月收平台服务费
,CASE WHEN  tab_detail.fee_name LIKE '%分公司服务费%' AND tab_detail.fee_name NOT IN ('提前结清分公司服务费违约金','逾期分公司服务费') THEN IFNULL(tab_detail.actual_fee_value,0) ELSE 0 END AS fact_month_pt_fee    -- 应收月收平台服务费 
,CASE WHEN  tab_detail.fee_name LIKE '%平台服务费%' THEN IFNULL(tab_detail.plan_fee_value,0) ELSE 0 END   AS recv_month_fgsfw_fee  -- 应收月分公司服务费
,CASE WHEN  tab_detail.fee_name LIKE '%平台服务费%' THEN IFNULL(tab_detail.actual_fee_value,0) ELSE 0 END AS fact_month_fgsfw_fee    -- 应收月收平台服务费 
,CASE WHEN  tab_detail.fee_name IN ('利息') THEN IFNULL(tab_detail.plan_fee_value,0) ELSE 0 END   AS recv_month_interest_fee  -- 应收利息
,CASE WHEN  tab_detail.fee_name IN ('利息') THEN IFNULL(tab_detail.actual_fee_value,0) ELSE 0 END AS fact_month_interest_fee    -- 实收利息   
 FROM ODS_XD_TUANDAI_BM.tb_business_ods t1
 INNER JOIN tb_car_business_after_detail_tmp1 tab_detail ON tab_detail.business_id=t1.business_id 
WHERE t1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d') 
AND   t1.business_type IN ('房速贷标准件','房速贷非标准件') 
AND  EXISTS (SELECT 1 FROM tb_car_business_after_ods_tmp b WHERE  t1.business_id = b.car_business_id  ) -- 排除已经结清
;



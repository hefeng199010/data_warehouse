,0 AS 48mon_debx_yd_cnt
,0 AS 36mon_debx_ed_cnt
,0 AS 48mon_debx_ed_cnt
,0 AS hbfx5y_yd_cnt
,0 AS hbfx5y_ed_cnt
,0 AS hbfx10y_yd_cnt
,0 AS hbfx10y_ed_cnt
,0 AS 1mon_xxhb_yd_amt
,0 AS 3mon_xxhb_yd_amt
,0 AS 6mon_xxhb_yd_amt
,0 AS 12mon_xxhb_yd_amt
,0 AS 1mon_xxhb_ed_amt
,0 AS 3mon_xxhb_ed_amt
,0 AS 6mon_xxhb_ed_amt
,0 AS 12mon_xxhb_ed_amt
,0 AS 36mon_debx_yd_amt
,0 AS 48mon_debx_yd_amt
,0 AS 36mon_debx_ed_amt
,0 AS 48mon_debx_ed_amt
,0 AS hbfx5y_yd_amt
,0 AS hbfx5y_ed_amt
,0 AS hbfx10y_yd_amt
,0 AS hbfx10y_ed_amt
  ,(one_tm_pt_fee + one_tm_fgsfw_fee + one_tm_db_fee  ) AS  cut_interest        -- 砍头息 
  ,(t.one_tm_fd_syqz_fee + t.one_tm_ssfy_fee)             AS  one_tm_fd_syqz_ssfy_fee    -- 返点-应收剩余前置服务费
  ,0 AS recv_service_fee
  ,0 AS fact_service_fee
 FROM (
       SELECT 
        t.business_id,
        tb.branch_id,
	SUM(CASE WHEN fee_item_id IN ('c5114e6d-fb97-11e7-a149-94c69109b34a','ee9f418f-fb97-11e7-a149-94c69109b34a','fcf15cb8-fb97-11e7-a149-94c69109b34a') THEN fee_value ELSE 0 END) AS one_tm_fd_syqz_fee,
	SUM(CASE WHEN fee_item_name ='返佣费' AND is_one_time_charge = 2 THEN fee_value ELSE 0 END) AS one_tm_ssfy_fee,
		  
        SUM(CASE WHEN fee_type_id = '38456370-3b68-41d3-a16b-ec294c1a334c' AND is_one_time_charge = 2  THEN fee_value ELSE 0 END) AS one_tm_pt_fee,
        SUM(CASE WHEN fee_type_id = 'd5aef8af-0277-4514-a4cc-cdb095db30f4' AND is_one_time_charge = 2  THEN fee_value ELSE 0 END) AS one_tm_fgsfw_fee,
        SUM(CASE WHEN fee_item_name ='担保费' AND is_one_time_charge = 2 THEN fee_value ELSE 0 END) AS one_tm_db_fee,
        SUM(CASE WHEN fee_item_id='0b7bec91-722e-4f3c-9677-6620ea61526d' THEN fee_value ELSE 0 END) AS one_tm_fd_dc_fee,
        
        SUM(CASE WHEN fee_item_name ='行政综合费' AND is_one_time_charge = 2 THEN fee_value ELSE 0 END) AS one_tm_xzzh_fee, -- 行政综合费（确定是否是在砍头息里面）
       
        SUM(CASE WHEN fee_type_id = '38456370-3b68-41d3-a16b-ec294c1a334c' AND is_one_time_charge = 1  THEN fee_value ELSE 0 END) AS recv_month_pt_fee,    -- 应收月收平台服务费
        SUM(CASE WHEN fee_type_id = 'd5aef8af-0277-4514-a4cc-cdb095db30f4' AND is_one_time_charge = 1  THEN fee_value ELSE 0 END) AS recv_month_fgsfw_fee, -- 应收月分公司服务费
        SUM(CASE WHEN fee_item_name = '利息' AND is_one_time_charge = 1  THEN fee_value ELSE 0 END)                               AS recv_month_interest_fee, -- 应收利息
        SUM(CASE WHEN fee_type_id = '38456370-3b68-41d3-a16b-ec294c1a334c' AND is_one_time_charge = 1  THEN fact_fee_value ELSE 0 END) AS fact_month_pt_fee,      -- 实收月收平台服务费
        SUM(CASE WHEN fee_type_id = 'd5aef8af-0277-4514-a4cc-cdb095db30f4' AND is_one_time_charge = 1  THEN fact_fee_value ELSE 0 END) AS fact_month_fgsfw_fee,   -- 实收月收平台服务费
        SUM(CASE WHEN fee_item_name = '利息' AND is_one_time_charge = 1  THEN fact_fee_value ELSE 0 END)                               AS fact_month_interest_fee -- 实收月收平台服务费          
         FROM ODS_XD_TUANDAI_BM.tb_business_apply_output_ods t
         INNER JOIN ( SELECT tb.business_id,tb.branch_id FROM ODS_XD_TUANDAI_BM.tb_business_ods tb WHERE tb.part_dt= DATE_FORMAT('${pt_day}','%Y-%m-%d') AND tb.business_type IN ('房速贷标准件','房速贷非标准件') ) tb -- 取房速贷展期业务
         ON  tb.business_id = t.business_id
        WHERE t.part_dt= DATE_FORMAT('${pt_day}','%Y-%m-%d')    
       GROUP BY t.business_id,tb.branch_id
       ) t
    INNER JOIN dws_agr_xd_issue_business_tmp ib  -- 满标日期
    ON  t.business_id = ib.business_id
   ;


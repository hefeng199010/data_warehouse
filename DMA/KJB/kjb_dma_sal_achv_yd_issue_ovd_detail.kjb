INSERT INTO DMA_SAL_ACHV_DATA.dma_sal_achv_yd_issue_ovd_detail
SELECT
 tt.loan_id                 --  子标借据号     
,tt.src_loan_id             --  母标借据号     
,tt.issue_id                --  标的号           
,tt.period                  --  期数              
,tt.contract_number         --  合同号           
,tt.src_contract_number     --  母标合同号     
,tt.customer_name           --  客户名           
,tt.id_card                 --  身份证号码     
,tt.mobile_phone            --  手机号           
,tt.company_type            --  企业或个人     
,tt.business_type           --  业务类型        
,tt.full_scale_time         --  满标时间        
,tt.contract_bidding_money  --  母标发标金额  
,tt.issue_bidding_money     --  子标发标金额  
,tt.ovd_bj                  --  逾期本金        
,tt.ovd_lx                  --  逾期利息        
,tt.first_ovd_due_date      --  应还款日        
,tt.max_ovd_days            --  最大逾期天数  
,tt.cur_ovd_days            --  当前逾期天数 
,(
  	   CASE WHEN IFNULL(tt.cur_ovd_days,0)&lt;=30 THEN '1-30天'
	        WHEN IFNULL(tt.cur_ovd_days,0)&lt;=60 THEN '31-60天'
	        WHEN IFNULL(tt.cur_ovd_days,0)&lt;=90 THEN '61-90天'
	        WHEN IFNULL(tt.cur_ovd_days,0)&lt;=120 THEN '91-120天'
	        WHEN IFNULL(tt.cur_ovd_days,0) >120 THEN '121天及以上' END )   AS ovd_stage  
,tt.dw_src_sys              --  来源系统        
,tt.dw_src_tbl              --  来源表           
,tt.dw_ins_tm               --  插入时间        
,tt.dw_upd_tm               --  更新时间        
,tt.dw_ins_usr              --  插入用户        
,tt.dw_upd_usr              --  更新用户        
,tt.part_dt                 --  分区时间 
FROM(
SELECT 
 t.loan_id           AS loan_id
,src_con.src_loan_id AS src_loan_id
,t.issue_id  AS issue_id 
,pd.period   AS period
 ,UPPER(tl.contract_number) AS contract_number
 ,UPPER(src_con.src_contract_number) AS src_contract_number
,t.customer_name AS customer_name
,CONCAT("'",cus.id_card) AS id_card
,CONCAT("'",cus.mobile_phone)  AS mobile_phone
,CASE WHEN t.has_main=2 THEN '企业' ELSE '个人' END AS company_type
,'一点车贷'          AS business_type
,t.full_scale_time    AS full_scale_time -- 出款时间
,tl_rel.bidding_money AS contract_bidding_money -- 业务单所有标的的满标金额之和 找到对应目标的满标金额
,t.bidding_money      AS issue_bidding_money
,(IFNULL(trp1.yh_bj,0))     AS ovd_bj
,(IFNULL(trp1.yh_lx,0))     AS ovd_lx
,trp.due_date      AS first_ovd_due_date
,trp.overdue_days  AS max_ovd_days
,(SELECT SUM(cur_ovd.overdue_days) FROM yidian.`t_refund_period` cur_ovd WHERE cur_ovd.overdue_days>0 AND cur_ovd.loan_id = t.loan_id AND cur_ovd.period >= pd.period) AS cur_ovd_days
/*,(
  	   case when ifnull(pd.overdue_days,0)&lt;=30 then '1-30天'
	        when ifnull(pd.overdue_days,0)&lt;=60 then '31-60天'
	        when ifnull(pd.overdue_days,0)&lt;=90 then '61-90天'
	        when ifnull(pd.overdue_days,0)&lt;=120 then '91-120天'
	        when ifnull(pd.overdue_days,0) >120 then '121天及以上' end )   as 当期逾期阶段
	        */
, 'yidian' AS dw_src_sys
, 't_rp_business_open..' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr
, DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期
 FROM yidian.t_rp_business_open  t 
 LEFT JOIN yidian.t_loan tl ON t.loan_id = tl.id AND tl.status='1'
 LEFT JOIN yidian.t_hi_customer_loan cus ON t.loan_id=cus.loan_id
 LEFT JOIN (
        SELECT 
tl.id AS loan_id
,tl.contract_number
,IFNULL(tl1.contract_number,tl.contract_number) AS src_contract_number
,IFNULL(tl.disassembled_id,tl.id) AS src_loan_id  -- 母标的借据号
,tl.disassembled_id
,tl.has_disassembled
FROM yidian.t_loan tl
LEFT JOIN yidian.t_loan tl1 ON tl.disassembled_id=tl1.id AND tl1.status='1'
WHERE tl.status='1' 
 ) src_con  ON t.loan_id = src_con.loan_id 
LEFT JOIN yidian.t_loan tl_rel ON src_con.src_loan_id= tl_rel.id 
 LEFT JOIN (
SELECT 
             trp.loan_id
             ,SUM(IFNULL(trp.overdue_days,0)) AS overdue_days
             ,MIN(trp.due_date) AS due_date
             FROM yidian.t_refund_period trp
             WHERE trp.refund_status IN (3,4)
               AND trp.overdue_days>0
             GROUP BY trp.loan_id  
  ) trp ON t.loan_id=trp.loan_id
 LEFT JOIN (
SELECT 
             trp.loan_id
             ,trp.period
             ,IFNULL(trp.overdue_days,0) AS overdue_days
             FROM yidian.t_refund_period trp
             WHERE trp.refund_status IN (3,4) 
             AND trp.overdue_days>0
 ) pd ON t.loan_id = pd.loan_id
 LEFT JOIN (  SELECT   trp.loan_id
             ,trp.period
            ,(IFNULL(trp.bj,0))AS yh_bj
            ,(IFNULL(trp.lx,0)) AS yh_lx
             FROM yidian.t_refund_period trp
             WHERE trp.refund_status IN (3,4)
             AND trp.overdue_days>0
            -- AND trp.due_date &lt;=DATE'2018-11-15'
             ) trp1 ON pd.loan_id =trp1.loan_id AND pd.period=trp1.period
WHERE 1=1  
AND  tl.bidding_platform ='团贷网'
AND  trp.overdue_days>0
ORDER BY  tl.contract_number  
         ,pd.period 
) tt           
;

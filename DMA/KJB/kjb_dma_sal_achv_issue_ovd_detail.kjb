LEFT JOIN (SELECT * FROM ODS_XD_TUANDAI_BM.tb_repayment_proj_plan_dh_ods t3 WHERE part_dt =  DATE_FORMAT('${pt_day}','%Y-%m-%d') ) t3 ON t2.`proj_plan_id` = t3.`proj_plan_id`

	join issue_ovd_detail_tmp2 t2 on t1.business_id = t2.business_id and t1.car_business_after_id = t2.business_after_id
	where IFNULL(t2.`actual_repayment_date`,'') = ''
	  and t2.`fee_name` IN ('本金');


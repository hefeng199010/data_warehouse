DROP TEMPORARY TABLE IF EXISTS `issue_ovd_detail_tmp8`;
CREATE TEMPORARY TABLE `issue_ovd_detail_tmp8` (
	`stat_dt` VARCHAR(8) NULL DEFAULT NULL COMMENT '统计日期',
	`out_dt` VARCHAR(10) NULL DEFAULT NULL COMMENT '出款日期',
	`business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务编号',
	`issue_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '标的号',
	`customer_nm` VARCHAR(200) NULL DEFAULT NULL COMMENT '客户名称',
	`customer_type` VARCHAR(10) NULL DEFAULT NULL COMMENT '客户类型',
	`ovd_days` INT(10) NULL DEFAULT NULL COMMENT '逾期天数',
	`ovd_stage` VARCHAR(50) NULL DEFAULT NULL COMMENT '逾期阶段',
	`is_Principa_ovd` VARCHAR(10) NULL DEFAULT NULL COMMENT '是否本金逾期',
	`is_interest_ovd` VARCHAR(10) NULL DEFAULT NULL COMMENT '是否利息逾期',
	`business_out_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '业务出款金额',
	`issue_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '上标金额',
	`ovd_principa_left_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '逾期借款本金余额',
	`dw_src_sys` VARCHAR(50) NULL DEFAULT NULL COMMENT '来源系统',
	`dw_src_tbl` VARCHAR(500) NULL DEFAULT NULL COMMENT '来源表',
	`dw_ins_tm` DATETIME NULL DEFAULT NULL COMMENT '插入时间',
	`dw_upd_tm` DATETIME NULL DEFAULT NULL COMMENT '更新时间',
	`dw_ins_usr` VARCHAR(50) NULL DEFAULT NULL COMMENT '插入用户',
	`dw_upd_usr` VARCHAR(50) NULL DEFAULT NULL COMMENT '更新用户'
)ENGINE=MEMORY DEFAULT CHARSET=utf8;	

insert into issue_ovd_detail_tmp8
select substring('${p_day}',1,8) as stat_dt	,-- 统计日期      
       t3.out_dt,
       t3.business_id,
	   t1.issue_id,
	   t3.RealName as customer_nm,
	   t3.cust_type as  customer_type,
	   greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0)) ovd_days,
	   case when greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0)) >0 and greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0))&lt;=30 then '1-30天'
	        when greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0)) >30 and greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0))&lt;=60 then '31-60天'
	        when greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0)) >60 and greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0))&lt;=90 then '61-90天'
	        when greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0)) >90 and greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0))&lt;=120 then '91-120天'
	        when greatest(ifnull(t1.principl_ovd_days,0),ifnull(t1.interest_ovd_days,0)) >120  then '121天及以上' end as ovd_stage,
	   case when t1.principl_ovd_amt >0 then '是' else '否' end is_Principa_ovd,
	   case when t1.interest_ovd_amt >0 then '是' else '否' end is_interest_ovd,
	   t4.OUTPUT_MONEY as business_out_amt,
	   t3.full_borrow_money as issue_amt,
	   t2.total_principl_amt - t2.actual_principl_amt as ovd_principa_left_amt ,
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'ODS_XD_TUANDAI_BM.tb_car_business_after_ods..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	--	更新用户
from issue_ovd_detail_tmp3 t1
left join issue_ovd_detail_tmp4 t2 on t1.issue_id = t2.issue_id
join issue_ovd_detail_tmp6 t3 on t1.issue_id = t3.issue_id

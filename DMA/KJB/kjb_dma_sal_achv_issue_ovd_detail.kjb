select t2.issue_id,
		   t2.business_id,
		   t2.business_after_id,  
		   TO_DAYS(DATE_FORMAT('${pt_day}','%Y-%m-%d')) - TO_DAYS(t2.plan_repayment_date) as principl_ovd_days, 

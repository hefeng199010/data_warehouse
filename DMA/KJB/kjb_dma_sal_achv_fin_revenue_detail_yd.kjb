CREATE TEMPORARY TABLE `dma_sal_achv_fin_revenue_detail_yd_tmp` (
   data_src VARCHAR(100) DEFAULT NULL COMMENT '数据来源',  
  `runperiod` VARCHAR(50) DEFAULT NULL COMMENT '显示原表数据是否是最新的数据',
  `data_dt` VARCHAR(10) DEFAULT NULL COMMENT '数据日期',
  `loan_id` INT(11) NOT NULL COMMENT '借款ID',
  `contract_number` VARCHAR(50) DEFAULT NULL COMMENT '合同号',
  `org_address_province` VARCHAR(20) DEFAULT NULL COMMENT '机构地址省',
  `org_name` VARCHAR(50) DEFAULT NULL COMMENT '机构名',
  `loan_product_name` VARCHAR(50) DEFAULT NULL COMMENT '借款类型',
  `loan_date` VARCHAR(100) DEFAULT NULL COMMENT '合同日期',
  `full_scale_date` VARCHAR(100) DEFAULT NULL COMMENT '满标时间',
  `customer_name` VARCHAR(50) DEFAULT NULL COMMENT '客户名',
  `is_issue` VARCHAR(1) NOT NULL DEFAULT ''  COMMENT '是否上标',
  `bidding_platform` VARCHAR(50) DEFAULT NULL COMMENT '发标平台',
  `loan_period` TINYINT(4) DEFAULT NULL COMMENT '审核期限',
  `bdnhlv` VARCHAR(10) DEFAULT NULL COMMENT '投资人标的利率',
  `actual_period` TINYINT(4) DEFAULT NULL COMMENT '实际还款期数',
  `loan_money` DECIMAL(12,2) DEFAULT NULL COMMENT '审核金额',
  `bidding_money` DECIMAL(12,2) DEFAULT NULL COMMENT '发标金额',
  `sub_comp_out_service_fee` DECIMAL(12,2) NOT NULL DEFAULT '0.00'  COMMENT '分公司服务费(出款)',
  `sub_comp_month_service_fee` DECIMAL(12,2) DEFAULT 0.00 COMMENT '分公司月服务费',
  `zq_margin` DECIMAL(12,2) DEFAULT '0.00' COMMENT '展期保证金',
  `sh_zq_zcdfwf` DECIMAL(14,2)   DEFAULT '0.00' COMMENT '资产端展期服务费',
  `yqfx` DECIMAL(12,2) DEFAULT '0.00' COMMENT '逾期罚息',
  `sh_pttqjqwyj` DECIMAL(12,2) DEFAULT '0.00' COMMENT '资产端提前结清违约金(线上)(也叫平台提前结清违约金)',
  `sh_tqhkywj` DECIMAL(12,2) DEFAULT '0.00' COMMENT '资产端提前结清违约金(线下)(也叫提前还款违约金)',
  `yk` DECIMAL(12,2) DEFAULT '0.00' COMMENT '其他收入(也叫余款)',
  `out_ptfwf` DECIMAL(14,2) DEFAULT '0.00' COMMENT '平台服务费(出款)即资金端服务费+仲裁费',
  `p2p_service` DECIMAL(12,2) DEFAULT '0.00' COMMENT 'P2P服务费',
  `sh_zq_zjdfwf` DECIMAL(14,2) DEFAULT '0.00' COMMENT '资金端展期服务费',
  `pt_late_fee` DECIMAL(12,2) DEFAULT '0.00' COMMENT '平台滞纳金',
  `zjdtqjqwyj` DECIMAL(12,2) DEFAULT '0.00' COMMENT '资金端提前结清违约金',
  `out_guarantee` DECIMAL(12,2)  DEFAULT '0.00' COMMENT '担保费(出款)',
  `sh_zq_guarantee` DECIMAL(12,2) DEFAULT '0.00' COMMENT '展期担保费',
  `asset_side_name` VARCHAR(11) DEFAULT NULL
) ENGINE=INNODB DEFAULT CHARSET=utf8
; 

  
INSERT INTO dma_sal_achv_fin_revenue_detail_yd_tmp
 SELECT
 't_rp_repayment_detail' AS data_src
 ,a.runperiod
,DATE_FORMAT(IFNULL(a.hx_time,a.actual_date),'%Y-%m-%d')   AS data_dt -- 数据日期(实还日期)
,a.loan_id  
,UPPER(a.contract_number)             AS contract_number -- 业务编号     
,org.org_address_province-- 省份                 
,a.org_name            -- 所属机构             
,prod.loan_product_name-- 借款类型             
,a.loan_date           -- 合同日期             
,a.full_scale_time AS full_scale_date   -- 满标日期             
,a.customer_name     -- 客户名称             
,(CASE WHEN a.full_scale_time IS NULL THEN '否' ELSE '是' END ) AS is_issue -- 是否上标             
,a.bidding_platform-- 发标平台             
,a.loan_period     -- 借款期限             
,a.bdnhlv            -- 标的年化             
-- ,(CASE WHEN a.is_last_period=1 THEN  '是' WHEN IFNULL(a.is_last_period,0) =0 THEN '否' ELSE is_last_period END)       AS is_zq_period   -- 是否展期（1 是 0 否）                                                                                                                                             
-- ,(CASE WHEN a.is_the_settleup=1 THEN '是' WHEN IFNULL(a.is_the_settleup,0)=0 THEN '否' ELSE is_the_settleup END)      AS is_jq_period   -- 是否结清（1 是 0 否）            
,a.actual_period
,a.loan_money        -- 审批金额             
,a.bidding_money     -- 审批金额
/*分公司收入*/
,0 AS sub_comp_out_service_fee -- 14、分公司服务费（出款）：出款时收取的一次性分公司服务费即放款明细表.资产端到账（放款）
,(a.gpsfwf + a.margin + a.insurance) AS sub_comp_month_service_fee -- 15、分公司月服务费：月GPS服务费+月保证金+月平台服务费 
,a.zq_margin      -- 展期保证金
,(a.zq_insurance) AS sh_zq_zcdfwf      -- 实还资产端展期服务费
,a.yqfx              -- 逾期罚息 
,a.pttqjqwyj AS  sh_pttqjqwyj -- 实还资产端提前结清违约金（线上）
,a.tqhkywj   AS  sh_tqhkywj   -- 实还资产端提前结清违约金（线下）
,a.yk                         -- 余款 和huangxiaojun确认统计时只取还款明细表中已结清客户的余款 
/*平台费*/
,0 AS out_ptfwf   -- 平台服务费（出款）
,a.p2p_service                -- P2P月服务费（是否就是 p2p_service p2p服务费）
,(a.zq_p2p_service) AS sh_zq_zjdfwf -- 24、资金端展期服务费(展期p2p平台服务费)
,a.pt_late_fee     -- 25、平台滞纳金：逾期还款收取的平台滞纳金
,a.zjdtqjqwyj      -- 26、资金端提前结清违约金 ：提前结清平台违约金
/*担保费*/
,0 AS out_guarantee   -- 27、担保费（出款）：出款时收取的担保费(放款明细表.担保费)
,a.zq_dbf AS sh_zq_guarantee -- 28、展期担保费：展期收取的担保费(还款明细（按期）表.展期担保费)
,(CASE WHEN a.asset_side=1 THEN '鸿特' WHEN a.asset_side=2 THEN '一点' WHEN a.asset_side=3 THEN '德古' ELSE a.asset_side END ) AS asset_side_name -- 资产端（1：鸿特 2：一点）
 FROM  yidian.t_rp_repayment_detail a
 LEFT JOIN (SELECT prod.dict_value,prod.dict_key AS loan_product_name FROM yidian.t_cfg_dictionary_detail  prod  WHERE prod.parent_dict_code IN ('1')) prod  ON a.loan_product = prod.dict_value
 LEFT JOIN  yidian.t_cfg_organization org  ON a.org_id = org.id
 ;
 

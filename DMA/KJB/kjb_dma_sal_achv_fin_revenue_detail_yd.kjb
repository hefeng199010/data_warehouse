 INSERT INTO dma_sal_achv_fin_revenue_detail_yd_tmp
SELECT 
  't_rp_loan_detail' AS data_src
,a.runperiod
,DATE_FORMAT(IFNULL(a.full_scale_time,a.loan_date),'%Y-%m-%d')   AS data_dt -- 数据日期(应还日期)  对于部分合同没有满标日期（星享融平台的）取合同日期即借款起始日
,a.loan_id  
,UPPER(a.contract_number)             AS contract_number -- 业务编号       
,org.org_address_province-- 省份                 
,a.org_name            -- 所属机构             
,prod.loan_product_name-- 借款类型             
,a.loan_date           -- 合同日期             
,a.full_scale_time AS full_scale_date   -- 满标日期             
,a.customer_name     -- 客户名称             
,(CASE WHEN a.full_scale_time IS NULL THEN '否' ELSE '是' END ) AS is_issue -- 是否上标             
,a.bidding_platform-- 发标平台             
,a.loan_period     -- 借款期限             
,a.p2p_year_rate AS bdnhlv            -- 标的年化
-- ,IF(a.period > tl.loan_period,'是','否') is_zq_period  
-- IF(tr.refund_status != 3,'0',IF(((SELECT trra.actual_period FROM t_refund_actual trra WHERE trra.loan_id = trp.loan_id ORDER BY trra.create_time DESC LIMIT 1) = trp.period),'1','0')) is_jq_period           
-- ,(CASE WHEN a.is_zq_period=1 THEN  '是' WHEN IFNULL(a.is_zq_period,0) =0 THEN '否' ELSE is_zq_period END)    AS is_zq_period   -- 是否展期（1 是 0 否）                                                                                                                                             
-- ,(CASE WHEN a.is_jq_period=1 THEN '是' WHEN IFNULL(a.is_jq_period,0)=0 THEN '否' ELSE is_jq_period END)      AS is_jq_period   -- 是否结清（1 是 0 否）
,0 AS actual_period -- 由于为出款时候一次性收取费用 故设定为第0期      
,a.loan_money        -- 审批金额             
,a.bidding_money     -- 合同金额 
 /*分公司收入*/
,(a.assets_arrival + a.subsidies_fee) AS sub_comp_out_service_fee -- 14、分公司服务费（出款）：出款时收取的一次性分公司服务费即放款明细表.资产端到账（放款） + 补贴金
,0 AS sub_comp_month_service_fee -- 15、分公司月服务费：月GPS服务费+月保证金+月平台服务费 
,0 AS zq_margin      -- 展期保证金
,0 AS sh_zq_zcdfwf      -- 实还资产端展期服务费
,0 AS yqfx              -- 逾期罚息 
,0 AS sh_pttqjqwyj -- 实还资产端提前结清违约金（线上）
,0 AS sh_tqhkywj   -- 实还资产端提前结清违约金（线下）
,0 AS yk                         -- 余款 和huangxiaojun确认统计时只取还款明细表中已结清客户的余款 
/*平台费*/
,(a.p2p_service_fee + a.td_warbitration_fee) AS out_ptfwf   -- 平台服务费（出款）
,0 AS p2p_service                -- P2P月服务费（是否就是 p2p_service p2p服务费）
,0 AS sh_zq_zjdfwf -- 24、资金端展期服务费 ：展期收取的资金端服务费(还款明细（按期）表.资金端展期服务费 )
,0 AS pt_late_fee     -- 25、平台滞纳金：逾期还款收取的平台滞纳金
,0 AS zjdtqjqwyj      -- 26、资金端提前结清违约金 ：提前结清平台违约金
/*担保费*/
,a.guarantee_fee AS out_guarantee   -- 27、担保费（出款）：出款时收取的担保费(放款明细表.担保费)
,0 AS sh_zq_guarantee -- 28、展期担保费：展期收取的担保费(还款明细（按期）表.展期担保费)
,(CASE WHEN a.asset_side=1 THEN '鸿特' WHEN a.asset_side=2 THEN '一点' WHEN a.asset_side=3 THEN '德古' ELSE a.asset_side END ) AS asset_side_name -- 资产端（1：鸿特 2：一点） 
FROM yidian.t_rp_loan_detail a -- 放款明细报表
 LEFT JOIN (SELECT prod.dict_value,prod.dict_key AS loan_product_name FROM yidian.t_cfg_dictionary_detail  prod  WHERE prod.parent_dict_code IN ('1')) prod  ON a.loan_product = prod.dict_value
 LEFT JOIN  yidian.t_cfg_organization org  ON a.org_id = org.id
;

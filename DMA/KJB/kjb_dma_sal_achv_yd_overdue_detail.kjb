alter table DMA_SAL_ACHV_DATA.dma_sal_achv_yd_overdue_detail_his truncate partition dma_sal_achv_yd_overdue_detail_his_${pt_day};
INSERT INTO DMA_SAL_ACHV_DATA.dma_sal_achv_yd_overdue_detail_his
(
stat_dt                            -- 跑批日期            
,data_dt                           -- 数据日期（应收日期）
,area_nm                           -- 片区                
,branch_nm                         -- 分公司              
,customer_nm                       -- 客户名称            
,business_id                       -- 业务编号            
,business_type_nm                  -- 业务类型            
,type_nm                           -- 类型                
,out_dt                            -- 出款日期            
,repay_type_nm                     -- 还款方式            
,cur_repay_period                  -- 当前期数
,borrow_limit	                   -- 借款期限            
,borrow_amt                        -- 借款金额            
,left_principa_amt                 -- 剩余本金            
,max_ovd_days                      -- 最大逾期天数        
,cur_ovd_days                      -- 当期逾期天数        
,asset_type                        -- 资产分类            
,overdue_stage                     -- 逾期阶段            
,receive_repayment_date            -- 应收日期            
,receive_principal                 -- 应收本金（元）      
,receive_interest                  -- 应收利息（元）      
,`receive_gps_month_service` 
,`receive_month_cash_deposi` 
,`receive_month_platform_service`
,`receive_p2p_platform_service`
,receive_platform_delaying_payment   
,receive_little                    -- 应收小计（本息）    
,receive_total                     -- 应收合计（元）      
,actual_repayment_date             -- 实收日期            
,actual_principal                  -- 实收本金（元）      
,actual_interest                   -- 实收利息（元）     
  ,`actual_gps_month_service`
 , `actual_month_cash_deposi` 
 , `actual_month_platform_service` 
 , `actual_p2p_platform_service` 
 , `actual_delaying_interest` 
 , `actual_prepayment_contract`
 , `actual_surplus`
,actual_delaying_payment           -- 实收滞纳金（元）    
,actual_little                     -- 实收小计（本息）    
,actual_total                      -- 实收合计（元）      
,dw_src_sys                        -- 来源系统            
,dw_src_tbl                        -- 来源表              
,dw_ins_tm                         -- 插入时间            
,dw_upd_tm                         -- 更新时间            
,dw_ins_usr                        -- 插入用户            
,dw_upd_usr                        -- 更新用户            
,part_dt                           -- 分区时间            
)
SELECT 
DATE_FORMAT('${pt_day}','%Y-%m-%d') AS stat_dt
,DATE_FORMAT(t.receive_repayment_date,'%Y-%m-%d')  AS data_dt                          --  获取应收日期
,t.area_nm                          -- 片区                
,t.branch_nm                        -- 分公司              
,t.customer_nm                      -- 客户名称            
,t.business_id                      -- 业务编号            
,t.business_type_nm                 -- 业务类型            
,t.type_nm                          -- 类型                
,DATE_FORMAT(t.out_dt,'%Y-%m-%d')  AS out_dt                          -- 出款日期            
,CASE WHEN t.repay_type_nm in (1,2) THEN '先息后本'
      WHEN t.repay_type_nm = 5      THEN '等额本息' else t.repay_type_nm END  AS repay_type_nm           -- 还款方式 	  
,t.cur_repay_period                 -- 当前期数       
,t.borrow_limit                     -- 借款期限     
,t.borrow_amt                       -- 借款金额            
,IFNULL(prin.left_principa_amt,0) AS left_principa_amt                          -- 剩余本金            
,ovd.max_ovd_days                     -- 最大逾期天数        
,ovd.cur_ovd_days                     -- 当期逾期天数        
,'--' AS asset_type                   -- 资产分类            
,ovd.overdue_stage                    -- 逾期阶段            
,DATE_FORMAT(t.receive_repayment_date,'%Y-%m-%d') AS receive_repayment_date      -- 应收日期            
,t.receive_principal                -- 应收本金（元）      
,t.receive_interest                 -- 应收利息（元）      
,IFNULL(plan.receive_gps_month_service,0)                 AS receive_gps_month_service                  -- 应收月GPS服务费
,IFNULL(plan.receive_month_cash_deposi,0)                 AS receive_month_cash_deposi                  -- 应收月保证金
,IFNULL(plan.receive_month_platform_service,0)                 AS receive_month_platform_service                  -- 应收月平台服务费
,IFNULL(plan.receive_p2p_platform_service,0)                 AS receive_p2p_platform_service                  -- P2P平台服务费
,IFNULL(plan.receive_platform_delaying_payment,0) AS receive_platform_delaying_payment  -- 应收滞纳金 add by caihl20180530
,t.receive_little                                                                       -- 应收小计（本息）    
,(IFNULL(t.receive_little,0) + IFNULL(plan.receive_gps_month_service,0) + IFNULL(plan.receive_month_cash_deposi,0)+IFNULL(plan.receive_month_platform_service,0)+ IFNULL(plan.receive_p2p_platform_service,0)) AS receive_total                    -- 应收合计（元）      
,DATE_FORMAT(t.actual_repayment_date,'%Y-%m-%d')                 AS  actual_repayment_date           -- 实收日期            
,t.actual_principal                 -- 实收本金（元）      
,t.actual_interest                  -- 实收利息（元）      
,IFNULL(actual.actual_gps_month_service,0    ) AS  actual_gps_month_service                  -- 实收GPS月收服务费  
,IFNULL(actual.actual_month_cash_deposi ,0  ) AS  actual_month_cash_deposi                  -- 实收月保证金    
,IFNULL(actual.actual_month_platform_service,0 ) AS  actual_month_platform_service                  -- 实收月平台服务费    
,IFNULL(actual.actual_p2p_platform_service ,0) AS  actual_p2p_platform_service                  -- 实收P2P平台服务费    
,IFNULL(actual.actual_delaying_interest,0) AS  actual_delaying_interest                 -- 实收逾期利息    
,IFNULL(actual.actual_prepayment_contract,0   ) AS  actual_prepayment_contract                  -- 实收提前还款违约金    
,IFNULL(actual.actual_surplus  ,0) AS  actual_surplus                  -- 实收实收结余    
,IFNULL(actual.actual_delaying_payment,0)  AS  actual_delaying_payment          -- 实收滞纳金（元）    
,(IFNULL(t.actual_principal,0) + IFNULL(t.actual_interest,0) ) AS  actual_little                    -- 实收小计（本息）    
-- ,(t.actual_principal + t.actual_interest + actual.actual_other_fee  + actual.actual_delaying_payment) AS actual_total                     -- 实收合计（元）
,IFNULL(t.actual_total,0) AS actual_total                                                 -- 实收合计（元）
 ,'tuandai_bm' AS dw_src_sys
 ,'dws_agr_yidian_repayment_basic_info...' AS dw_src_tbl
 ,CURRENT_TIMESTAMP AS dw_ins_tm
 ,CURRENT_TIMESTAMP AS dw_upd_tm
 ,'caihl' AS dw_ins_usr
 ,'caihl' AS dw_upd_usr
 ,DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期 
FROM dma_ceo_rpt_overdue_detail_tmp01 t
  LEFT JOIN dma_ceo_rpt_overdue_detail_tmp04  ovd
  ON t.business_id = ovd.business_id
  AND t.cur_repay_period = ovd.business_after_id
  LEFT JOIN dma_ceo_rpt_overdue_detail_tmp05 plan
    ON t.business_id = plan.business_id
  AND t.cur_repay_period = plan.business_after_id
  LEFT JOIN dma_ceo_rpt_overdue_detail_tmp06 actual
     ON t.business_id = actual.business_id
  AND t.cur_repay_period = actual.business_after_id
  LEFT JOIN left_principa_amt_tmp1 prin
    ON t.business_id = prin.business_id
;


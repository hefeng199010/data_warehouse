  `business_id` varchar(50) DEFAULT NULL COMMENT '业务单号 ',
  `business_after_id` varchar(50) DEFAULT NULL COMMENT '期数 ',
  `cur_ovd_days` int(7) DEFAULT NULL,
  `max_ovd_days` int(7) DEFAULT NULL,
  `overdue_stage` varchar(3) DEFAULT NULL,
  KEY `idx_business_id` (`business_id`,`business_after_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
;

INSERT INTO dma_ceo_rpt_overdue_detail_tmp04
SELECT 
    t.business_id,
    t.business_after_id,
    t.cur_ovd_days,
    tt.max_ovd_days,
    (CASE WHEN t.cur_ovd_days  &lt;= 0 THEN 'M0'
      WHEN t.cur_ovd_days &lt; 31  THEN 'M1'
      WHEN t.cur_ovd_days &lt; 61  THEN 'M2'
      WHEN t.cur_ovd_days &lt; 91  THEN 'M3'
      WHEN t.cur_ovd_days >= 91  THEN 'M3+'
 END)  AS  overdue_stage 
FROM dma_ceo_rpt_overdue_detail_tmp02 t 
  LEFT JOIN 
    dma_ceo_rpt_overdue_detail_tmp03 tt 
ON t.business_id = tt.business_id 
;


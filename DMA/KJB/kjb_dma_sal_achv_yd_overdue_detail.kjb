/*增加应收滞纳金费用*/
DROP TEMPORARY TABLE IF EXISTS dma_ceo_rpt_overdue_detail_tmp01;
CREATE TEMPORARY TABLE dma_ceo_rpt_overdue_detail_tmp01 (
  area_nm varchar(50) DEFAULT NULL COMMENT '业务区域，例如，华南',
  branch_nm varchar(50) DEFAULT NULL COMMENT '业务所属分公司名',
  customer_nm varchar(100) DEFAULT NULL,
  business_id varchar(50) DEFAULT NULL COMMENT '业务单号 ',
  business_type_nm varchar(10)  NULL DEFAULT '',
  type_nm varchar(10)  NULL DEFAULT '',
  out_dt datetime DEFAULT NULL,
  repay_type_nm int(11) DEFAULT NULL COMMENT '还款方式（1:到期本息;2:每月付息;5等额本息） ',
  cur_repay_period varchar(50) DEFAULT NULL COMMENT '期数 ',
  borrow_limit INT(11) DEFAULT NULL COMMENT '借款期限 ',
  borrow_amt decimal(10,2) DEFAULT NULL,
  receive_repayment_date datetime DEFAULT NULL COMMENT '应收日期 ',
  receive_principal decimal(32,2) DEFAULT NULL,
  receive_interest decimal(32,2) DEFAULT NULL,
  receive_little decimal(33,2) DEFAULT NULL,
  actual_repayment_date datetime DEFAULT NULL COMMENT '实还日期 ',
  actual_principal decimal(32,2) DEFAULT NULL,
  actual_interest decimal(32,2) DEFAULT NULL,
  actual_total decimal(32,2) DEFAULT NULL,
  KEY idx_business_id (business_id,cur_repay_period)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
;

INSERT INTO dma_ceo_rpt_overdue_detail_tmp01
SELECT 
 '一点车贷' AS area_nm                           -- 片区                
,t.branch_name   AS branch_nm                         -- 分公司              
,t.main_customer_name AS customer_nm                       -- 客户名称            
,t.business_id                       -- 业务编号            
,'一点车贷' AS business_type_nm                  -- 业务类型            
,'新增'     AS type_nm                           -- 类型                
,MIN(t.full_issue_date) AS out_dt                            -- 出款日期            
,t.repayment_type  AS repay_type_nm                     -- 还款方式            
,t.business_after_id AS cur_repay_period                  -- 当前期数           
,t.borrow_limit AS borrow_limit                  -- 借款期限            
,MAX(IFNULL(t.borrow_money ,0) )AS borrow_amt                        -- 借款金额    add by caihl20180528 改为max               
,MIN(t.receivable_repayment_date) AS receive_repayment_date            -- 应收日期            
,SUM(t.receivable_principal )     AS receive_principal                 -- 应收本金（元）      
,SUM(t.receivable_interest )      AS receive_interest                  -- 应收利息（元）  
,SUM(t.receivable_principal + t.receivable_interest) AS receive_little                    -- 应收小计（本息）          
,MIN(t.actual_repayment_date) AS actual_repayment_date             -- 实收日期            
,SUM(t.actual_principal) AS actual_principal                  -- 实收本金（元）      
,SUM(t.actual_interest)  AS actual_interest                   -- 实收利息（元）         
,SUM(t.actual_amount) AS actual_total                         -- 实收合计（元）  取tb_yidian_repayment_actual.amount add by caihl20180528
FROM DWS_DATA.dws_agr_yidian_repayment_basic_info t               -- 还款计划基本信息 tb_yidian_repayment_plan_tmp
WHERE t.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
AND NOT EXISTS 
     (SELECT 1 FROM ODS_XD_TUANDAI_BM.tb_yidian_repayment_actual_ods tt 
       WHERE tt.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
	    and  tt.clear_type IN (1,2,3) 
		AND  t.business_id = tt.business_id 
		) -- 排除已经结清数据
GROUP BY 
 t.business_area             
,t.branch_name            
,t.main_customer_name          
,t.business_id               
,t.repayment_type            
,t.business_after_id 
;

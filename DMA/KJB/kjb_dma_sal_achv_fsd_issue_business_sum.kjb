CREATE TEMPORARY TABLE dma_sal_achv_fsd_issue_business_detail_tmp1 (
  data_dt VARCHAR(10) DEFAULT NULL,
  area_nm VARCHAR(50)  NULL DEFAULT '',
  branch_nm VARCHAR(50)  NULL DEFAULT '',
  business_id VARCHAR(50) NOT NULL COMMENT '业务单号',
  is_new_customer INT(11) NOT NULL DEFAULT '0',
  new_cust_amt DECIMAL(12,2) NOT NULL DEFAULT '0.00',
  is_jqzd_customer INT(11) DEFAULT NULL,
  jqzd_cust_amt DECIMAL(12,2) DEFAULT NULL,
  is_tuandai_loan INT(11) DEFAULT NULL,
  tuandai_loan_amt DECIMAL(12,2) DEFAULT NULL,
  is_nwjr_loan INT(11) DEFAULT NULL,
  nwjr_loan_amt DECIMAL(12,2) DEFAULT NULL,
    begin_issue_amount DECIMAL(12,2) NULL DEFAULT NULL COMMENT '启标金额',
	begin_issue_date date NULL DEFAULT NULL,
  INDEX idx_business_id(business_id)
) ENGINE=INNODB DEFAULT CHARSET=utf8
;
 
INSERT INTO dma_sal_achv_fsd_issue_business_detail_tmp1
 SELECT 
 DATE_FORMAT(ib.full_issue_date,'%Y-%m-%d')  AS data_dt -- 满标日期
 ,IFNULL(dep1.dept_name,'未知')              AS area_nm                                         #区域            
 ,IFNULL(dep2.dept_name,'未知')              AS branch_nm                                       #分公司 
 ,t.business_id
 ,CASE WHEN IFNULL(t.customer_class,0)=0  THEN 1 ELSE 0 END AS is_new_customer  -- 是否新增业务单
 ,CASE WHEN IFNULL(t.customer_class,0)=0  THEN IFNULL(ib.borrow_money,0) ELSE 0 END AS new_cust_amt  -- 新增业务放款金额
 ,CASE WHEN t.customer_class          =1  THEN 1 ELSE 0 END AS is_jqzd_customer -- 是否结清再贷业务单
 ,CASE WHEN t.customer_class          =1  THEN IFNULL(ib.borrow_money,0) ELSE 0 END AS jqzd_cust_amt -- 结清再贷放款金额
 ,CASE WHEN t.platform_id          =1  THEN 1 ELSE 0 END AS is_tuandai_loan     -- 是否团贷网放款
 ,CASE WHEN t.platform_id          =1  THEN IFNULL(ib.borrow_money,0) ELSE 0 END AS tuandai_loan_amt     -- 团贷网放款金额
 ,CASE WHEN t.platform_id          =2  THEN 1 ELSE 0 END AS is_nwjr_loan        -- 是否你我金融放款
 ,CASE WHEN t.platform_id          =2  THEN IFNULL(ib.borrow_money,0) ELSE 0 END AS nwjr_loan_amt        -- 你我金融放款金额
 ,ib.begin_issue_amount
 ,DATE_FORMAT(ib.begin_issue_date,'%Y-%m-%d') as begin_issue_date
FROM ODS_XD_TUANDAI_BM.tb_business_ods t
INNER JOIN dws_agr_xd_issue_business_tmp ib -- 上标
ON t.business_id = ib.business_id
LEFT JOIN 
  (
   SELECT 
   dep.dept_id,
   dep.district_id AS area_id
  FROM  ODS_XD_TUANDAI_BM.tb_department_ods dep -- 部门信息表
   WHERE dep.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
  ) dep
   ON  t.branch_id = dep.dept_id
 LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')   
  ) dep1 ON dep.area_id = dep1.DEPT_ID 
  LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')  
  ) dep2  ON t.branch_id = dep2.DEPT_ID 
WHERE t.part_dt= DATE_FORMAT('${pt_day}','%Y-%m-%d')
AND   t.business_type IN ('房速贷标准件','房速贷非标准件') -- 房速贷新增业务（去掉展期）
/*
AND   t.issue_type =1 -- 上标
AND EXISTS
 ( SELECT 1 

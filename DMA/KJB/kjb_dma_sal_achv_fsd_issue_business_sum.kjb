;

-- P2P业务实际满标金额累计之和，线下出款以财务登记出款为准
-- 线上:tb_issue_business.full_borrow_money
-- 线下:tb_output_list.OUTPUT_MONEY
 INSERT INTO dws_agr_xd_issue_business_tmp 
-- 线上  
  SELECT '1' AS is_issue,business_id,SUM(IFNULL(full_borrow_money,0)) AS borrow_money, MIN(t.full_issue_date) AS full_issue_date ,
  SUM(IFNULL(begin_issue_amount,0))         AS begin_issue_amount,  -- 启标金额
  MIN(t.full_time)                          AS begin_issue_date     -- 启标时间
  FROM DWS_DATA.dws_agr_xd_issue_business t
WHERE  part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
AND (IFNULL(t.full_issue_date,'')!='' or IFNULL(t.full_time,'')!='') -- add by caihl 
GROUP BY business_id
;



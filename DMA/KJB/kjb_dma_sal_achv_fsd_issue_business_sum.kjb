INSERT INTO DMA_SAL_ACHV_DATA.dma_sal_achv_fsd_issue_business_sum_his
 SELECT 
 DATE_FORMAT('${pt_day}','%Y-%m-%d')                   AS stat_dt                 -- 统计日期 
,DATE_FORMAT(IFNULL(t.data_dt,'4712-12-31'),'%Y-%m-%d') AS data_dt
,IFNULL(t.area_nm,'未知')                      AS area_nm                      -- 片区            
,IFNULL(t.branch_nm,'未知')                    AS branch_nm                 -- 分公司             
,SUM(new_apl_cnt)                              AS  new_apl_cnt                 -- 申请单数        
,SUM(new_pass_cnt)                             AS  new_pass_cnt               -- 通过单数 
,CASE WHEN SUM(new_apl_cnt) =0 THEN 0 ELSE CAST(SUM(new_pass_cnt)/SUM(new_apl_cnt) AS DECIMAL(10,4)) END AS new_pass_cnt_pct             -- 通过率   通过单数/申请单数               
,SUM(new_sign_cnt)                             AS new_sign_cnt                 -- 签约单数  
,CASE WHEN SUM(new_pass_cnt)=0 THEN 0 ELSE CAST(SUM(new_sign_cnt)/SUM(new_pass_cnt) AS DECIMAL(10,4)) END AS new_sign_cnt_pct             -- 签约率   签约单数/通过单数               
,SUM(new_output_cnt)                           AS   new_output_cnt           -- 新增放款单数   
-- ,CASE WHEN SUM(new_pass_cnt) =0 THEN 0 ELSE CAST(SUM(new_output_cnt)/SUM(new_pass_cnt) AS DECIMAL(10,4)) END AS new_output_cnt_pct           -- 放款率 放款单数/通过单数   
,CASE WHEN SUM(new_sign_cnt) =0 THEN 0 ELSE CAST(SUM(new_begin_issue_cnt)/SUM(new_sign_cnt) AS DECIMAL(10,4)) END AS new_output_cnt_pct           -- 放款率  上标单数/签约单数，最终结果以百分比显示，保留两位小数点                     
,SUM(new_output_amt)                           AS new_output_amt             -- 新增放款金额  
,SUM(is_new_customer)                          AS is_new_customer          -- 新增业务单  
,SUM(new_cust_amt)                             AS new_cust_amt             -- 新增业务放款金额 
,SUM(is_jqzd_customer)                         AS is_jqzd_customer         -- 是否结清再贷业务单   
,SUM(jqzd_cust_amt)                            AS  jqzd_cust_amt           -- 结清再贷放款金额
,SUM(is_tuandai_loan)                          AS  is_tuandai_loan         -- 是否团贷网放款
,SUM(tuandai_loan_amt)                         AS  tuandai_loan_amt        -- 团贷网放款金额
,CASE WHEN SUM(new_output_amt) =0 THEN 0 ELSE CAST(SUM(tuandai_loan_amt)/SUM(new_output_amt) AS DECIMAL(10,4)) END AS tuandai_loan_amt_pct         -- 团贷网放款占比
,SUM(is_nwjr_loan)                             AS  is_nwjr_loan            -- 是否你我金融放款
,SUM(nwjr_loan_amt)                            AS  nwjr_loan_amt           -- 你我金融放款金额
,CASE WHEN SUM(new_output_amt) =0 THEN 0 ELSE CAST(SUM(nwjr_loan_amt)/SUM(new_output_amt) AS DECIMAL(10,4)) END    AS nwjr_loan_amt_pct            -- 你我金融放款占比
,SUM(t.extension_apl_cnt)                      AS extension_apl_cnt           --  展期业务申请单数        
,SUM(t.extension_pass_cnt)                     AS extension_pass_cnt          --  展期业务通过单数        
,SUM(t.extension_output_cnt)                   AS extension_output_cnt        --  展期业务放款单数        
,SUM(t.extension_output_amt)                   AS extension_output_amt        --  展期业务放款金额  
,SUM(t.cut_interest)                           AS cut_interest
,SUM(t.one_tm_fd_syqz_ssfy_fee)                AS one_tm_fd_syqz_ssfy_fee 
,SUM(t.new_begin_issue_cnt)                    AS new_begin_issue_cnt  -- 新增上标单数
,SUM(t.begin_issue_amount)                     AS begin_issue_amount   -- 新增上标金额 
,'tuandai_bm' AS dw_src_sys
,'dws_agr_xd_issue_business,dws_evt_xd_output_list,tb_business_ods,tb_department_ods,tb_car_business_after_ods,tb_business_apply_output_ods,dma_ceo_rpt_business_sum' AS dw_src_tbl
,CURRENT_TIMESTAMP AS dw_ins_tm
,CURRENT_TIMESTAMP AS dw_upd_tm
,'caihl'           AS dw_ins_usr
,'caihl'           AS dw_upd_usr
,DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期
FROM DMA_SAL_ACHV_DATA.dma_sal_achv_fsd_issue_business_detail t
GROUP BY 
 IFNULL(t.data_dt,'4712-12-31')
,IFNULL(t.area_nm,'未知')                     -- 片区            
,IFNULL(t.branch_nm,'未知')                   -- 分公司   
;


/*砍头息和返点费用计算  -- 贷后业务 -- 展期跟踪--操作--业务明细*/  



DROP TEMPORARY TABLE IF EXISTS tb_business_apply_output_tmp1;
CREATE TEMPORARY TABLE tb_business_apply_output_tmp1 (
  business_id VARCHAR(50) DEFAULT NULL COMMENT '业务单号',
  one_tm_fd_syqz_fee DECIMAL(32,2) DEFAULT NULL,
  one_tm_ssfy_fee DECIMAL(32,2) DEFAULT NULL,
  cut_interest DECIMAL(34,2) DEFAULT NULL,
  full_issue_date DATETIME DEFAULT NULL,
  branch_id VARCHAR(50) DEFAULT NULL COMMENT '业务所属分公司编号',
  KEY idx_business_id (business_id)
) ENGINE=INNODB DEFAULT CHARSET=utf8
;

INSERT INTO tb_business_apply_output_tmp1
SELECT 
   t.business_id
  ,t.one_tm_fd_syqz_fee        -- 返点-应收剩余前置服务费
  ,t.one_tm_ssfy_fee           --  返点-应收实时返佣费
  ,(one_tm_pt_fee + one_tm_fgsfw_fee + one_tm_db_fee  ) AS  cut_interest -- 砍头息 one_tm_pt_fee   + one_tm_fgsfw_fee + one_tm_db_fee + one_tm_fd_dc_fee + one_tm_xzzh_fee
  ,ib.full_issue_date
  ,t.branch_id
 FROM (
       SELECT 
        t.business_id,
        tb.branch_id,
	SUM(CASE WHEN fee_item_id IN ('c5114e6d-fb97-11e7-a149-94c69109b34a','ee9f418f-fb97-11e7-a149-94c69109b34a','fcf15cb8-fb97-11e7-a149-94c69109b34a') THEN fee_value ELSE 0 END) AS one_tm_fd_syqz_fee,
	SUM(CASE WHEN fee_item_name ='返佣费' AND is_one_time_charge = 2 THEN fee_value ELSE 0 END) AS one_tm_ssfy_fee,
		  
         SUM(CASE WHEN fee_type_id = '38456370-3b68-41d3-a16b-ec294c1a334c' AND is_one_time_charge = 2  THEN fee_value ELSE 0 END) AS one_tm_pt_fee,
         SUM(CASE WHEN fee_type_id = 'd5aef8af-0277-4514-a4cc-cdb095db30f4' AND is_one_time_charge = 2  THEN fee_value ELSE 0 END) AS one_tm_fgsfw_fee,
         SUM(CASE WHEN fee_item_name ='担保费' AND is_one_time_charge = 2 THEN fee_value ELSE 0 END) AS one_tm_db_fee,
         SUM(CASE WHEN fee_item_id='0b7bec91-722e-4f3c-9677-6620ea61526d' THEN fee_value ELSE 0 END) AS one_tm_fd_dc_fee,
         
         SUM(CASE WHEN fee_item_name ='行政综合费' AND is_one_time_charge = 2 THEN fee_value ELSE 0 END) AS one_tm_xzzh_fee, -- 行政综合费（确定是否是在砍头息里面）
       
         SUM(CASE WHEN fee_type_id = '38456370-3b68-41d3-a16b-ec294c1a334c' AND is_one_time_charge = 1  THEN fee_value ELSE 0 END) AS month_pt_fee,
         SUM(CASE WHEN fee_type_id = 'd5aef8af-0277-4514-a4cc-cdb095db30f4' AND is_one_time_charge = 1  THEN fee_value ELSE 0 END) AS month_fgsfw_fee
         FROM ODS_XD_TUANDAI_BM.tb_business_apply_output_ods t
         INNER JOIN ( SELECT tb.business_id,tb.branch_id FROM ODS_XD_TUANDAI_BM.tb_business_ods tb WHERE tb.part_dt= DATE_FORMAT('${pt_day}','%Y-%m-%d') AND tb.business_type IN ('T600') ) tb -- 取房速贷展期业务
         ON  tb.business_id = t.business_id
        WHERE t.part_dt= DATE_FORMAT('${pt_day}','%Y-%m-%d') 
        /*AND EXISTS 
              (SELECT 1 FROM ODS_XD_TUANDAI_BM.tb_issue_business_ods ib  WHERE ib.part_dt= DATE_FORMAT('${pt_day}','%Y-%m-%d') AND  tb.business_id = ib.business_id) -- 只取上标的数据
          */    
       GROUP BY t.business_id,tb.branch_id
       ) t
    INNER JOIN dws_agr_xd_issue_business_tmp ib
    ON  t.business_id = ib.business_id
   ;

 

INSERT INTO DMA_SAL_ACHV_DATA.dma_sal_achv_fsd_issue_business_detail
SELECT 
'3砍头息和返点费用' AS data_src
 ,DATE_FORMAT(t.full_issue_date,'%Y-%m-%d')  AS data_dt -- 满标日期
 ,t.business_id
 ,IFNULL(dep1.dept_name,'未知')              AS area_nm                                         #区域            
 ,IFNULL(dep2.dept_name,'未知')              AS branch_nm                                       #分公司 
,0  AS new_apl_cnt           --  新增业务申请单数                                                                      
,0  AS new_pass_cnt          --  新增业务通过单数                                                                       
,0  AS new_sign_cnt          --  新增业务签约单数                                                                      
,0  AS new_output_cnt        --  新增业务放款单数                                                                     
,0  AS new_output_amt        --  新增业务放款金额
,0  AS is_new_customer
,0  AS new_cust_amt
,0  AS is_jqzd_customer
,0  AS jqzd_cust_amt
,0  AS is_tuandai_loan
,0  AS tuandai_loan_amt
,0  AS is_nwjr_loan
,0  AS nwjr_loan_amt 
,0 AS extension_apl_cnt           --  展期业务申请单数        
,0 AS extension_pass_cnt          --  展期业务通过单数        
,0 AS extension_output_cnt        --  展期业务放款单数        
,0 AS extension_output_amt        --  展期业务放款金额  
,t.cut_interest                                                          -- 展期首笔代扣费用(出款砍头息)
,(t.one_tm_fd_syqz_fee + t.one_tm_ssfy_fee) AS one_tm_fd_syqz_ssfy_fee  -- 展期线下收取(剩余前置服务费+返佣费)
,'tuandai_bm' AS dw_src_sys
,'dma_sal_achv_fsd_issue_business_detail_tmp1' AS dw_src_tbl
,CURRENT_TIMESTAMP AS dw_ins_tm
,CURRENT_TIMESTAMP AS dw_upd_tm
,'caihl'           AS dw_ins_usr
,'caihl'           AS dw_upd_usr
,0 as new_begin_issue_cnt -- 新增上标单数
,0 as begin_issue_amount     -- 新增上标金额
FROM tb_business_apply_output_tmp1 t
LEFT JOIN 
  (
   SELECT 
   dep.dept_id,
   dep.district_id AS area_id
  FROM  ODS_XD_TUANDAI_BM.tb_department_ods dep -- 部门信息表
   WHERE dep.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') 
  ) dep
   ON  t.branch_id = dep.dept_id
 LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')   
  ) dep1 ON dep.area_id = dep1.DEPT_ID 
  LEFT JOIN 
  (
  SELECT a1.DEPT_ID,dept_name
  FROM ODS_XD_TUANDAI_BM.tb_department_ods a1
  WHERE a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')  
  ) dep2  ON t.branch_id = dep2.DEPT_ID 
;  
 

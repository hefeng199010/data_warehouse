	is_issue VARCHAR(1) NOT NULL DEFAULT '',
	business_id VARCHAR(50) NULL DEFAULT NULL COMMENT '业务单号',
     borrow_money DECIMAL(12,2) NULL DEFAULT NULL COMMENT '借款金额',
	full_issue_date DATETIME NULL DEFAULT NULL,

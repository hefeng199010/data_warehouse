INSERT INTO dma_sal_achv_issue_ovd_detail_v2_yd_tmp
SELECT
SUBSTRING('${p_day}',1,8) AS stat_dt	-- 统计日期      
,DATE_FORMAT(t.full_scale_time,'%Y-%m-%d')  AS out_dt
,t.contract_number AS business_id
,t.issue_id
,t.period        AS business_after_id
,t.customer_name AS customer_nm
,t.customer_type AS customer_type
,hlm.mortgage_name AS creditor_or_lender_name   -- 抵押权人
,lb.guarantee_name AS TuanDai_Guarantee_Company -- 对应团贷网担保公司
,t.ovd_days
,(
  	  CASE WHEN IFNULL(t.cur_ovd_days,0)&lt;=30 THEN '1-30天'
	        WHEN IFNULL(t.cur_ovd_days,0)&lt;=60 THEN '31-60天'
	        WHEN IFNULL(t.cur_ovd_days,0)&lt;=90 THEN '61-90天'
	        WHEN IFNULL(t.cur_ovd_days,0)&lt;=120 THEN '91-120天'
	        WHEN IFNULL(t.cur_ovd_days,0) >120 THEN '121天及以上' END )   AS ovd_stage
,CASE WHEN t.principl_ovd_amt >0 THEN '是' ELSE '否' END is_Principa_ovd
,CASE WHEN t.interest_ovd_amt >0 THEN '是' ELSE '否' END is_interest_ovd
,t.busi_bidding_money AS business_out_amt
,t.bidding_money      AS issue_amt
,t.bidding_money - IFNULL(tra.sh_bj,0) AS ovd_principa_left_amt 
,t.bidding_money - IFNULL(tra.sh_lx,0) AS ovd_interest_left_amt 
,t.principl_ovd_amt
,t.interest_ovd_amt
,'yidian'	AS	dw_src_sys	--	来源系统
,'t_rp_business_open,t_loan,t_hi_customer_loan,t_refund_period'	AS	dw_src_tbl	--	来源表
,NOW()	AS	dw_ins_tm	--	插入时间
,NOW()	AS	dw_upd_tm	--	更新时间
,'caihl'	AS	dw_ins_usr	--	插入用户
,'caihl'	AS	dw_upd_usr	--	更新用户
,2 AS src_data -- 一点车贷系统数据
,t.business_type -- 业务类型编号
FROM dma_sal_achv_issue_ovd_detail_v2_yd_tmp1 t
LEFT JOIN 
(SELECT tra.loan_id,SUM(tra.bj) AS sh_bj,SUM(tra.lx) AS sh_lx FROM yidian.t_refund_actual tra GROUP BY tra.loan_id) tra ON t.loan_id = tra.loan_id
LEFT JOIN yidian.t_hi_loan_mortgage hlm ON t.loan_id  = hlm.loan_id
LEFT JOIN yidian.t_loan_bidding lb      ON t.loan_id  = lb.loan_id
;

	issue_id VARCHAR(50) NULL DEFAULT NULL COMMENT '业务编号',
	business_id VARCHAR(50) NULL DEFAULT NULL COMMENT '业务编号',
	business_after_id VARCHAR(50) NULL DEFAULT NULL COMMENT '业务编号',
	principl_ovd_days INT(11) NULL DEFAULT NULL COMMENT '业务类型',
	interest_ovd_days INT(11) NULL DEFAULT NULL COMMENT '业务类型',
	principl_ovd_amt DECIMAL(12,2) NULL DEFAULT NULL COMMENT '出款金额（线上指提现金额，线下指出款总金额）',
	interest_ovd_amt DECIMAL(12,2) NULL DEFAULT NULL COMMENT '出款金额（线上指提现金额，线下指出款总金额）',

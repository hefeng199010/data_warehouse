insert into issue_ovd_detail_tmp8
select substring('${p_day}',1,8) as stat_dt	,-- 统计日期      
       t3.out_dt,
       t3.business_id,
	   t1.issue_id,
	   t1.business_after_id,
	   t3.RealName as customer_nm,
	   t3.cust_type as  customer_type,
	   t6.creditor_or_lender_name,
	   t6.TuanDai_Guarantee_Company,
	   greatest(ifnull(t5.principl_ovd_days,0),ifnull(t5.interest_ovd_days,0)) ovd_days,
	   case when greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0)) >0 and greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0))&lt;=30 then '1-30天'
	        when greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0)) >30 and greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0))&lt;=60 then '31-60天'
	        when greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0)) >60 and greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0))&lt;=90 then '61-90天'
	        when greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0)) >90 and greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0))&lt;=120 then '91-120天'
	        when greatest(ifnull(t1.period_principl_ovd_days,0),ifnull(t1.period_interest_ovd_days,0)) >120  then '121天及以上' end as ovd_stage,
	   case when t1.principl_ovd_amt >0 then '是' else '否' end is_Principa_ovd,
	   case when t1.interest_ovd_amt >0 then '是' else '否' end is_interest_ovd,
	   t4.OUTPUT_MONEY as business_out_amt,
	   t3.full_borrow_money as issue_amt,
	   t2.total_principl_amt - t2.actual_principl_amt as ovd_principa_left_amt ,  -- 标的剩余本金
	   t2.total_interest_amt - t2.actual_interest_amt as ovd_interest_left_amt ,  -- 标的剩余利息
	   t1.principl_ovd_amt,
	   t1.interest_ovd_amt,
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'ODS_XD_TUANDAI_BM.tb_car_business_after_ods..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	,--	更新用户
		1 as src_data, -- 小贷系统数据
		bt.business_big_type_nm as business_type  -- add 20181221
from issue_ovd_detail_tmp33 t1
left join issue_ovd_detail_tmp4 t2 on t1.issue_id = t2.issue_id
join issue_ovd_detail_tmp6 t3 on t1.issue_id = t3.issue_id
left join issue_ovd_detail_tmp5 t4 on t4.business_id = t1.business_id
left join  issue_ovd_detail_tmp3 t5 on t1.issue_id = t5.issue_id
left join issue_ovd_detail_tmp7 t6 on t2.first_business_id = t6.BUSINESS_ID
left join DWS_DATA.dws_agr_xd_business xb        on t1.business_id=xb.business_id and xb.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
left join DIM_DATA.dim_prd_xd_business_type bt on xb.business_type = bt.business_type

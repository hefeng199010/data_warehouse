INSERT INTO issue_ovd_detail_tmp4 
select t1.issue_id,
      t1.business_id,
	  t2.first_business_id,
    SUM(CASE WHEN IFNULL(t2.issue_split_type,0) = 1 AND  t1.fee_name = '本金' THEN ROUND(IFNULL(plan_fee_value,0)) 
         WHEN IFNULL(t2.issue_split_type,0) &lt;&gt; 1 AND  t1.fee_name = '本金' THEN IFNULL(plan_fee_value,0) ELSE 0 END) AS  total_principl_amt,
    SUM(CASE WHEN IFNULL(t2.issue_split_type,0) = 1 AND  t1.fee_name = '本金' THEN ROUND(IFNULL(actual_fee_value,0))
         WHEN IFNULL(t2.issue_split_type,0) &lt;&gt;  1 AND  t1.fee_name = '本金' THEN IFNULL(actual_fee_value,0) ELSE 0 END) AS  actual_principl_amt,
    SUM(CASE WHEN IFNULL(t2.issue_split_type,0) = 1 AND  t1.fee_name = '利息' THEN ROUND(IFNULL(plan_fee_value,0)) 
         WHEN IFNULL(t2.issue_split_type,0) &lt;&gt; 1 AND  t1.fee_name = '利息' THEN IFNULL(plan_fee_value,0) ELSE 0 END) AS  total_interest_amt,
    SUM(CASE WHEN IFNULL(t2.issue_split_type,0) = 1 AND  t1.fee_name = '利息' THEN ROUND(IFNULL(actual_fee_value,0)) 
         WHEN IFNULL(t2.issue_split_type,0) &lt;&gt; 1 AND  t1.fee_name = '利息' THEN IFNULL(actual_fee_value,0) ELSE 0 END) AS  actual_interest_amt
from issue_ovd_detail_tmp2 t1
left join (SELECT a.business_id,a.issue_split_type,first_business_id FROM DWS_DATA.dws_agr_xd_business a WHERE a.part_dt =DATE_FORMAT('${pt_day}','%Y-%m-%d'))t2
       on t1.business_id = t2.business_id
where t1.fee_name in ( '本金','利息')
group by t1.issue_id,t1.business_id,t2.first_business_id;
  

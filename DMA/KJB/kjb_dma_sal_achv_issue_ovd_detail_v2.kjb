CREATE TEMPORARY TABLE dma_sal_achv_issue_ovd_detail_v2_yd_tmp1 (
  loan_id INT(11) DEFAULT NULL COMMENT '借款ID',
  src_loan_id INT(11) DEFAULT NULL,
  issue_id VARCHAR(50) DEFAULT NULL,
  period TINYINT(4) DEFAULT NULL COMMENT '期数',
  contract_number VARCHAR(50) DEFAULT NULL,
  src_contract_number VARCHAR(50) DEFAULT NULL,
  customer_name VARCHAR(50) DEFAULT NULL COMMENT '客户名',
  id_card VARCHAR(20) DEFAULT NULL COMMENT '身份证',
  mobile_phone VARCHAR(20) DEFAULT NULL COMMENT '手机号',
  customer_type VARCHAR(10) DEFAULT NULL,
  business_type VARCHAR(10) NOT NULL DEFAULT '',
  full_scale_time DATETIME DEFAULT NULL COMMENT '满标时间',
  busi_bidding_money DECIMAL(12,2) DEFAULT NULL COMMENT '发标金额',
  bidding_money DECIMAL(12,2) DEFAULT NULL COMMENT '发标金额',
  principl_ovd_amt DECIMAL(12,2) NOT NULL DEFAULT '0.00',
  interest_ovd_amt DECIMAL(12,2) NOT NULL DEFAULT '0.00',
  first_ovd_due_dt DATE DEFAULT NULL COMMENT '应还款日',
  ovd_days DECIMAL(32,0) DEFAULT NULL,
  cur_ovd_days DECIMAL(32,0) DEFAULT NULL,
  key idx_loan_id(loan_id)
) ENGINE=INNODB DEFAULT CHARSET=utf8
;

INSERT INTO dma_sal_achv_issue_ovd_detail_v2_yd_tmp1
SELECT 
 t.loan_id           -- AS 子标借据号
,src_con.src_loan_id -- AS 母标借据号
,t.issue_id          -- AS 平台对应项目ID 
,pd.period           -- AS 期次
 ,UPPER(tl.contract_number) AS contract_number -- 业务单号
 ,UPPER(src_con.src_contract_number) AS src_contract_number -- 源业务单号
,t.customer_name -- AS 客户名称
,cus.id_card      --  as 身份证号码
,cus.mobile_phone --  AS 手机号
,CASE WHEN t.has_main=2 THEN '企业' ELSE '个人' END AS customer_type -- "类型（企业/自然人）"
,'一点车贷'        AS business_type -- 业务类型编号
,t.full_scale_time                  -- AS 满标时间 
,tl_rel.bidding_money  AS busi_bidding_money -- AS 业务出款金额 -- 业务单所有标的的满标金额之和 找到对应目标的满标金额
,t.bidding_money                             -- AS 上标金额
,(IFNULL(trp1.yh_bj,0))  AS principl_ovd_amt -- 逾期本金
,(IFNULL(trp1.yh_lx,0))  AS interest_ovd_amt -- 逾期利息
,trp.due_date            AS first_ovd_due_dt  -- "逾期开始时间（首次逾期开始日）"
,trp.overdue_days        AS ovd_days          -- "当前逾期天数（最长逾期天数）"
,(SELECT SUM(cur_ovd.overdue_days) FROM yidian.t_refund_period cur_ovd WHERE cur_ovd.overdue_days>0 AND cur_ovd.loan_id = t.loan_id AND cur_ovd.period >= pd.period) AS cur_ovd_days
 FROM yidian.t_rp_business_open  t 
 LEFT JOIN yidian.t_loan tl ON t.loan_id = tl.id AND tl.status='1'
 LEFT JOIN yidian.t_hi_customer_loan cus ON t.loan_id=cus.loan_id
 LEFT JOIN (
        SELECT 
tl.id AS loan_id
,tl.contract_number
,IFNULL(tl1.contract_number,tl.contract_number) AS src_contract_number
,IFNULL(tl.disassembled_id,tl.id) AS src_loan_id  -- 母标的借据号
,tl.disassembled_id
,tl.has_disassembled
FROM yidian.t_loan tl
LEFT JOIN yidian.t_loan tl1 ON tl.disassembled_id=tl1.id AND tl1.status='1'
WHERE tl.status='1' 
 ) src_con  ON t.loan_id = src_con.loan_id 
LEFT JOIN yidian.t_loan tl_rel ON src_con.src_loan_id= tl_rel.id 
 LEFT JOIN (
       SELECT   
             trp.loan_id
             ,SUM(IFNULL(trp.overdue_days,0)) AS overdue_days
             ,MIN(trp.due_date) AS due_date
             FROM yidian.t_refund_period trp
             WHERE trp.refund_status IN (3,4)
               AND trp.overdue_days>0
             GROUP BY trp.loan_id  
            ) trp ON t.loan_id=trp.loan_id
 LEFT JOIN (
       SELECT 
             trp.loan_id
             ,trp.period
             ,IFNULL(trp.overdue_days,0) AS overdue_days
             FROM yidian.t_refund_period trp
             WHERE trp.refund_status IN (3,4) 
             AND trp.overdue_days>0
           ) pd ON t.loan_id = pd.loan_id
 LEFT JOIN (  SELECT   
              trp.loan_id
             ,trp.period
            ,(IFNULL(trp.bj,0))AS yh_bj
            ,(IFNULL(trp.lx,0)) AS yh_lx
             FROM yidian.t_refund_period trp
             WHERE trp.refund_status IN (3,4)
             AND trp.overdue_days>0
             GROUP BY trp.loan_id 
			 ) trp1 ON pd.loan_id =trp1.loan_id AND pd.period=trp1.period
WHERE 1=1  
AND  tl.bidding_platform ='团贷网'
AND  trp.overdue_days>0
ORDER BY  tl.contract_number  
         ,pd.period   
;

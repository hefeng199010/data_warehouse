insert into issue_ovd_detail_tmp23
select t2.issue_id,
		   t2.business_id,
		   t2.business_after_id,  
		   TO_DAYS(DATE_FORMAT('${pt_day}','%Y-%m-%d')) - TO_DAYS(t2.plan_repayment_date) as principl_ovd_days, 
		   0 as  interest_ovd_days,
		   t2.plan_fee_value as principl_ovd_amt,
		   0 as interest_ovd_amt
	from issue_ovd_detail_tmp1 t1
	join issue_ovd_detail_tmp2 t2 on t1.business_id = t2.business_id and t1.car_business_after_id = t2.business_after_id
	where IFNULL(t2.actual_repayment_date,'') = ''
	  and t2.fee_name IN ('本金');

insert into issue_ovd_detail_tmp23
  select t2.issue_id,
	   t2.business_id,
	   t2.business_after_id,  
	   0 as principl_ovd_days, 
	   TO_DAYS(DATE_FORMAT('${pt_day}','%Y-%m-%d')) - TO_DAYS(t2.plan_repayment_date)  as  interest_ovd_days,
	   0 as principl_ovd_amt,
	   t2.plan_fee_value as interest_ovd_amt
from issue_ovd_detail_tmp1 t1
join issue_ovd_detail_tmp2 t2 on t1.business_id = t2.business_id and t1.car_business_after_id = t2.business_after_id
where IFNULL(t2.actual_repayment_date,'') = ''
  and t2.fee_name IN ('利息');


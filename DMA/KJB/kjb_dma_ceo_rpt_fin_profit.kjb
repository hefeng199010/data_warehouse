DROP TEMPORARY TABLE IF EXISTS dma_ceo_rpt_fin_revenue_tmp;
CREATE TEMPORARY TABLE dma_ceo_rpt_fin_revenue_tmp (
  period_id VARCHAR(7) DEFAULT NULL,
  area_nm VARCHAR(100) DEFAULT NULL,
  branch_nm VARCHAR(100) DEFAULT NULL,
  business_type_nm VARCHAR(50) DEFAULT NULL,
  revenue_amt DECIMAL(65,2) DEFAULT 0.00,
 asset_income_amt DECIMAL(65,2) DEFAULT 0.00, -- 资产端收入
 plat_income_amt DECIMAL(65,2) DEFAULT 0.00, -- 团贷网平台收入
 db_income_amt  DECIMAL(65,2) DEFAULT 0.00 -- 担保收入
) ENGINE=INNODB DEFAULT CHARSET=utf8
;

INSERT INTO dma_ceo_rpt_fin_revenue_tmp 
SELECT 
DATE_FORMAT(actual_repayment_date,'%Y-%m') AS period_id
,area_nm
,branch_nm
,(CASE WHEN business_type_nm IN ('房速贷','车易贷') THEN '房贷'
      WHEN business_type_nm IN ('一点车贷') THEN '车贷'
    ELSE business_type_nm END) AS business_type_nm
,SUM(IFNULL(comp_mon_srv_amt       ,0) +    
IFNULL(comp_one_tm_srv_amt    ,0) +
IFNULL(mon_srv_pct_amt        ,0) +
IFNULL(agency_amt             ,0) +
IFNULL(admin_amt              ,0) +
IFNULL(repay_evaluate_money   ,0) +
IFNULL(gps_cost_amt           ,0) +
IFNULL(comp_stop_amt          ,0) +
IFNULL(acct_mgr_amt           ,0) +
IFNULL(consult_file_amt       ,0) +
IFNULL(plat_mon_srv_amt       ,0) +
IFNULL(plat_one_tm_srv_amt    ,0) +
IFNULL(one_tm_db_fee          ,0) +
IFNULL(consult_amt            ,0) +
IFNULL(maintenamce_amt        ,0) +
IFNULL(extsn_nofsh_srv_amt    ,0) +
IFNULL(extsn_charge_amt       ,0) +
IFNULL(extsn_nofsh_other_amt  ,0) +
IFNULL(current_Breach         ,0) +
IFNULL(deferred_pay_amt       ,0) +
IFNULL(day_visit_amt          ,0) +
IFNULL(fsh_stop_amt           ,0) +
IFNULL(day_lawyer_amt         ,0) +
IFNULL(day_adv_settle_amt     ,0) +
IFNULL(day_Litigation_amt     ,0) +
IFNULL(day_trail_amt          ,0) +
IFNULL(court_accpt_amt        ,0) +
IFNULL(day_ovd_brh_srv_amt    ,0) +
IFNULL(overdue_acal           ,0) +
IFNULL(day_ovd_income_amt     ,0) +
IFNULL(other_money            ,0) +
IFNULL(day_margin_amt         ,0) ) AS revenue_amt -- 收入合计
,SUM(comp_mon_srv_amt+comp_one_tm_srv_amt+mon_srv_pct_amt+agency_amt+admin_amt+repay_evaluate_money+gps_cost_amt+comp_stop_amt+acct_mgr_amt+consult_file_amt+
          consult_amt+maintenamce_amt+extsn_nofsh_srv_amt+extsn_charge_amt+extsn_nofsh_other_amt+current_Breach+deferred_pay_amt+day_visit_amt+fsh_stop_amt+
          day_lawyer_amt+day_adv_settle_amt+day_Litigation_amt+day_trail_amt+court_accpt_amt+day_ovd_brh_srv_amt+overdue_acal+day_ovd_income_amt+other_money+day_margin_amt) AS asset_income_amt -- 资产端收入
,SUM(plat_mon_srv_amt+plat_one_tm_srv_amt) AS plat_income_amt -- 团贷网平台收入
,SUM(one_tm_db_fee) AS db_income_amt -- 担保收入
FROM DMA_CEO_RPT_DATA.dma_ceo_rpt_fin_revenue_fc t
WHERE t.actual_repayment_date&lt;&gt;'4712-12-31'
GROUP BY
DATE_FORMAT(actual_repayment_date,'%Y-%m')
,area_nm
,branch_nm
,(CASE WHEN business_type_nm IN ('房速贷','车易贷') THEN '房贷'
      WHEN business_type_nm IN ('一点车贷') THEN '车贷'
    ELSE business_type_nm END)
UNION ALL
SELECT 
DATE_FORMAT(actual_repayment_date,'%Y-%m') AS period_id
,area_nm
,branch_nm
,(CASE WHEN business_type_nm IN ('房速贷','车易贷') THEN '房贷'
      WHEN business_type_nm IN ('一点车贷') THEN '车贷'
    ELSE business_type_nm END) AS business_type_nm
,SUM(IFNULL(comp_srv_amt                    ,0) + 
IFNULL(gps_cost_amt                    ,0) + 
IFNULL(guarantee_amt                   ,0) + 
IFNULL(actual_month_platform_service   ,0) + 
IFNULL(actual_p2p_platform_service     ,0) + 
IFNULL(one_tm_db_fee                   ,0) + 
IFNULL(ode_acal_amt                    ,0) + 
IFNULL(current_Breach                  ,0) + 
IFNULL(trail_amt                       ,0) + 
IFNULL(day_adv_settle_amt              ,0)  ) AS revenue_amt
,SUM(comp_srv_amt+gps_cost_amt+guarantee_amt+actual_month_platform_service+ode_acal_amt+current_Breach+trail_amt+day_adv_settle_amt) AS asset_income_amt -- 资产端收入
,SUM(actual_p2p_platform_service) AS plat_income_amt -- 团贷网平台收入
,SUM(one_tm_db_fee) AS db_income_amt -- 担保收入
FROM DMA_CEO_RPT_DATA.dma_ceo_rpt_fin_revenue_yd t
WHERE t.actual_repayment_date&lt;&gt;'4712-12-31'
GROUP BY
DATE_FORMAT(actual_repayment_date,'%Y-%m')
,area_nm
,branch_nm
,(CASE WHEN business_type_nm IN ('房速贷','车易贷') THEN '房贷'
      WHEN business_type_nm IN ('一点车贷') THEN '车贷'
    ELSE business_type_nm END)
;


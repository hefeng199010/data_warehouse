CREATE TEMPORARY TABLE dma_ceo_rpt_fin_profit_tmp (
  period_id VARCHAR(7) NOT NULL DEFAULT '',
  area_nm VARCHAR(100) DEFAULT NULL,
  branch_nm VARCHAR(100) DEFAULT NULL COMMENT '分公司全称',
  business_type_nm VARCHAR(50) DEFAULT NULL,
   asset_income_amt DECIMAL(65,2) DEFAULT 0.00, -- 资产端收入
   plat_income_amt DECIMAL(65,2) DEFAULT 0.00, -- 团贷网平台收入
   db_income_amt  DECIMAL(65,2) DEFAULT 0.00 ,-- 担保收入
  revenue_amt DECIMAL(65,2) DEFAULT NULL,
  daily_expenses_amt DECIMAL(62,2) DEFAULT NULL,
  business_expenses_amt DECIMAL(62,2) DEFAULT NULL,
  guar_comp_share_amt DECIMAL(62,2) DEFAULT NULL,
  tot_expenses_amt DECIMAL(62,2) DEFAULT NULL,
  profit_amt DECIMAL(65,2) DEFAULT NULL
) ENGINE=INNODB DEFAULT CHARSET=utf8
;

INSERT INTO dma_ceo_rpt_fin_profit_tmp
SELECT
 conf.period_id                   -- 统计日期（按月）            
,(CASE WHEN conf.business_type IN ('车贷') THEN '一点车贷' ELSE rev.area_nm END) AS area_nm         --   片区  (由于一点车贷的区域无法获取所以统一给‘一点车贷’)                                  
,conf.branch_name   AS branch_nm   
,(CASE WHEN conf.business_type IN ('房贷') THEN '房速贷'
       WHEN conf.business_type IN ('车贷') THEN '一点车贷'
   ELSE conf.business_type END ) AS business_type_nm        --  业务类型
,SUM(IFNULL(rev.asset_income_amt,0) ) AS asset_income_amt  
,SUM(IFNULL(rev.plat_income_amt,0) )  AS plat_income_amt  
,SUM(IFNULL(rev.db_income_amt,0) )    AS db_income_amt                        
,SUM(IFNULL(rev.revenue_amt,0) ) AS revenue_amt              -- 收入合计                   
,SUM(IFNULL(exps.daily_expenses_amt,0))    AS daily_expenses_amt         -- 支出-日常费用支出          
,SUM(IFNULL(exps.business_expenses_amt,0)) AS business_expenses_amt      -- 支出-业务费用支出          
,SUM(IFNULL(exps.guar_comp_share_amt,0))   AS guar_comp_share_amt        -- 支出-担保公司分摊          
,SUM(IFNULL(exps.tot_expenses_amt,0))      AS tot_expenses_amt           -- 支出合计                   
,(SUM(IFNULL(rev.revenue_amt,0)) -  SUM(IFNULL(exps.tot_expenses_amt,0))) AS profit_amt                 -- 利润                       
FROM DIM_DATA.dim_fin_profit_branch_month_config conf
LEFT JOIN 
    (
    SELECT 
     t.period_id
    ,t.area_nm
    ,IFNULL(br.full_branch_name,t.branch_nm) AS branch_nm
    ,t.business_type_nm
    ,t.asset_income_amt
    ,t.plat_income_amt
    ,t.db_income_amt
    ,t.revenue_amt
    FROM dma_ceo_rpt_fin_revenue_tmp t
    LEFT JOIN tb_department_ods_tmp br
    ON t.branch_nm = br.DEPT_NAME
    ) rev
 ON  conf.period_id     = rev.period_id
AND conf.dept_name     = rev.branch_nm
AND conf.business_type = rev.business_type_nm
LEFT JOIN dma_ceo_rpt_fin_expense_tmp2  exps
 ON  conf.period_id     = exps.period_id
AND conf.dept_name     = exps.campany_name
AND conf.business_type = exps.sub_type
GROUP BY
 conf.period_id                -- 统计日期（按月）           
,(CASE WHEN conf.business_type IN ('车贷') THEN '一点车贷' ELSE rev.area_nm END)         --   片区  (由于一点车贷的区域无法获取所以统一给‘一点车贷’)                                  
,conf.branch_name 
,(CASE WHEN conf.business_type IN ('房贷') THEN '房速贷'
       WHEN conf.business_type IN ('车贷') THEN '一点车贷'
   ELSE conf.business_type END )    --  业务类型   
;


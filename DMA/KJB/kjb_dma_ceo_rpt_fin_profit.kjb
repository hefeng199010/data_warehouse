INSERT INTO dma_ceo_rpt_fin_expense_tmp2    
 SELECT 
 exps.period_id
,exps.campany_name
,exps.sub_type
,SUM(IFNULL(CASE WHEN exps.expense_type IN ('dma_ceo_rpt_general_expense') THEN exps.expense_total ELSE 0 END ,0)) AS daily_expenses_amt         -- 支出-日常费用支出          
,SUM(IFNULL(CASE WHEN exps.expense_type IN ('dma_ceo_rpt_business_expense') THEN exps.expense_total ELSE 0 END ,0))AS business_expenses_amt      -- 支出-业务费用支出          
,SUM(IFNULL(CASE WHEN exps.expense_type IN ('dma_ceo_rpt_bonding_company_expense') THEN exps.expense_total ELSE 0 END ,0)) AS guar_comp_share_amt        -- 支出-担保公司分摊          
,SUM(IFNULL(CASE WHEN exps.expense_type IN ('dma_ceo_rpt_general_expense','dma_ceo_rpt_business_expense','dma_ceo_rpt_bonding_company_expense') THEN exps.expense_total ELSE 0 END ,0)) tot_expenses_amt           -- 支出合计                    
  FROM dma_ceo_rpt_fin_expense_tmp1  exps 
  GROUP BY
 exps.period_id
,exps.campany_name
,exps.sub_type
;  

 

INSERT INTO DMA_CEO_RPT_DATA.dma_ceo_rpt_fin_profit_his
(
 stat_dt                 --   统计日期（按月）                  
,area_nm                 --   片区                                    
,branch_nm               --   分公司 
,business_type_nm  
,asset_income_amt
,plat_income_amt
,db_income_amt                               
,month_revenue_amt       --   月收入合计                                                     
,daily_expenses_amt      --   支出-日常费用支出                 
,business_expenses_amt   --   支出-业务费用支出                 
,guar_comp_share_amt     --   支出-担保公司分摊                 
,month_tot_expenses_amt  --   月支出合计                                                     
,month_profit_amt        --   月利润合计                           
,dw_src_sys              --   来源系统                              
,dw_src_tbl              --   来源表                                 
,dw_ins_tm               --   插入时间                              
,dw_upd_tm               --   更新时间                              
,dw_ins_usr              --   插入用户                              
,dw_upd_usr              --   更新用户                              
,part_dt                 --   分区日期  
)
SELECT 
 CONCAT(t.period_id,'-01') AS stat_dt                    -- 统计日期（按月）           
,t.area_nm         --   片区  (由于一点车贷的区域无法获取所以统一给‘一点车贷’)                                  
,(CASE WHEN (t.branch_nm IN ('汽车金融事业部','房产金融事业部') OR t.business_type_nm ='职能部门') THEN '广东鸿特信息咨询有限公司' ELSE t.branch_nm END ) AS branch_nm               -- 职能部门处理    
,t.business_type_nm        --  业务类型  
,t.asset_income_amt
,t.plat_income_amt
,t.db_income_amt                              
,t.revenue_amt AS month_revenue_amt       --   月收入合计                                                    
,t.daily_expenses_amt      --   支出-日常费用支出                 
,t.business_expenses_amt   --   支出-业务费用支出                 
,t.guar_comp_share_amt     --   支出-担保公司分摊                 
,t.tot_expenses_amt AS month_tot_expenses_amt  --   月支出合计                                               
,t.profit_amt AS month_profit_amt        --   月利润合计                           
, 'tuandai_bm' AS dw_src_sys
, 'tb_department_ods	dma_ceo_rpt_fin_revenue_fc	dma_ceo_rpt_fin_revenue_yd	dma_ceo_rpt_general_expense	dma_ceo_rpt_business_expense	dma_ceo_rpt_bonding_company_expense' AS dw_src_tbl
, CURRENT_TIMESTAMP AS dw_ins_tm
, CURRENT_TIMESTAMP AS dw_upd_tm
, 'caihl' AS dw_ins_usr
, 'caihl' AS dw_upd_usr
, DATE_FORMAT('${pt_day}','%Y-%m-%d') AS part_dt
FROM dma_ceo_rpt_fin_profit_tmp t
WHERE  (t.revenue_amt&lt;&gt;0  OR  t.tot_expenses_amt &lt;&gt; 0 OR t.profit_amt &lt;&gt; 0 )  
-- WHERE t.business_type_nm &lt;&gt; '职能部门'
;

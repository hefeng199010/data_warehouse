DROP TEMPORARY TABLE IF EXISTS dma_ceo_rpt_fin_expense_tmp1;
CREATE TEMPORARY TABLE `dma_ceo_rpt_fin_expense_tmp1` (
  `period_id` VARCHAR(7) DEFAULT NULL,
  `campany_name` VARCHAR(100) DEFAULT NULL,
  `sub_type` VARCHAR(100) DEFAULT NULL,
  `expense_type` VARCHAR(35) NOT NULL DEFAULT '',
  `expense_total` DECIMAL(40,2) DEFAULT NULL
) ENGINE=INNODB DEFAULT CHARSET=utf8
;
INSERT INTO dma_ceo_rpt_fin_expense_tmp1
SELECT 
        period_id
        ,CASE WHEN t.sub_type NOT IN ('房贷','车贷') THEN t.sub_type ELSE campany_name END AS campany_name  -- 对支出的非房贷 车贷的数据特殊处理 方便和配置表关联
        ,CASE WHEN t.sub_type IN ('总裁办'
                                 ,'行政中心'
                                 ,'人力资源中心'
                                 ,'财务中心'
                                 ,'审计监察中心'
                                 ,'战略发展中心'
                                 ,'资金管理中心'
                                 ,'贷后管理中心(担保公司) '
                                 ,'贷后管理中心(档案组)'
                                 ,'信息技术中心'
                                 ,'法律合规中心'
                                 ,'消费金融事业部'
                                 ,'小微及供应链金融事业部'
                                 ) THEN '职能部门' 
              WHEN sub_type = '房产金融事业部' THEN '房贷'
              WHEN sub_type = '汽车金融事业部' THEN '车贷'
              WHEN sub_type = '房贷（东莞）'   THEN '房贷'
              WHEN sub_type = '房贷（塘厦）'   THEN '房贷'  
              ELSE sub_type END AS sub_type
        ,expense_type
        ,expense_total
        FROM dma_ceo_rpt_fin_expense_tmp t
        ;
 

    <connection>DWS_DATA</connection>
    <sql>select  distinct 
        substring('${p_day}',1,8) as stat_dt	,-- 统计日期
		t1.business_id			as business_id,--	业务编号
		t2.dept_name			as area_nm,--	区域名称
		IFNULL(t3.dept_name,'未知')			as branch_nm,--	分公司名称
		IFNULL(t4.user_name ,'未知')		    as operator_nm,--	经办客户经理名称
		IFNULL(t6.repay_way_nm,'其他')  as repay_type_nm,--	还款方式
		case when t1.is_extension = 0 then '净新增业务' when t1.is_extension &lt;&gt; 0 then '展期业务' end  as type_nm,--	类型
		case when t1.business_type ='T600' then '房速贷展期业务' else  t1.business_type end  as business_type_nm,--	业务类型
		t1.customer_name		as customer_nm,--	借款人
		t1.customer_list		as customer_list_nm,--	借款人
		t5.process_name 	    as phase_nm,--	流程阶段
		t1.apply_borrow_limit	as borrow_limit,--	借款期限（月）
		t1.apply_borrow_money	as borrow_amt,--	借款金额
		DATE_FORMAT(t1.input_time,'%Y-%m-%d')	    	as input_dt,--	受理日期
		DATE_FORMAT(t1.process_modify_time,'%Y-%m-%d')	as update_dt,--	更新日期
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'dws_data.dws_agr_xd_business,dwd_data.dwd_emp_xd_department,dwd_data.dwd_evt_xd_business_approve..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	--	更新用户
from   
(select t1.business_id,t1.is_extension, t1.business_type,t1.customer_name,t1.customer_list,t1.apply_borrow_limit,t1.apply_borrow_money,t1.input_time,t1.process_modify_time,
        t1.area_id,t1.branch_id,t1.operator_id,t1.process_id,t1.apply_repayment_type_id
from DWS_DATA.dws_agr_xd_business t1 
where t1.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
  and FIND_IN_SET(t1.business_type,'房速贷标准件,房速贷非标准件,T600')
) t1 -- 业务基本信息事实表
left join  ODS_XD_TUANDAI_BM.tb_department_ods t2 -- 部门信息表
	   on t1.area_id = t2.dept_id 
	  and t2.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
left join  ODS_XD_TUANDAI_BM.tb_department_ods t3 -- 部门信息表
	   on t1.branch_id = t3.dept_id 
	  and t3.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
left join ODS_XD_TUANDAI_BM.tb_user_ods t4
       on t4.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
	  and t1.operator_id = t4.user_id
left join ODS_XD_TUANDAI_BM.tb_process_list_ods t5
       on t5.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
	  and t1.process_id = t5.process_id
left join DIM_DATA.dim_evt_xd_repay_way t6
       on t1.apply_repayment_type_id = t6.repay_way_id</sql>

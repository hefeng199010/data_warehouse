    <connection>DMA_SAL_ACHV_DATA</connection>
    <sql>SELECT 交易时间,分公司ID,分公司,汇总表期末余额,余额文件账户余额   FROM (
SELECT DATE_FORMAT(a.account_time,'%Y-%m-%d %H:%m:%s') '交易时间',a.branch_id '分公司ID',b.full_branch_name '分公司',b.qm_bal '汇总表期末余额',a.amount '余额文件账户余额' FROM 
(
  SELECT a.`branch_id` branch_id,t.`amount`,t.`account_time` FROM `ODS_XD_TUANDAI_BM`.`ht_cams_cgt_allbalance_ods` t JOIN DIM_DATA.dim_org_full_branch_cfg a ON t.`user_id`=a.branch_id 
  WHERE t.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d')  AND t.`account_time`>='2018-10-01'  AND t.`account_time`&lt;=DATE_ADD(DATE_FORMAT('${pt_day}','%Y-%m-%d'),INTERVAL -1 DAY)
) a
LEFT JOIN 
(
  SELECT a.`branch_id`,a.`tran_time`,a.`full_branch_name`,a.`qm_bal` FROM `DMA_SAL_ACHV_DATA`.`dma_sal_achv_sub_comp_cg_tran_sum_detail` a 
  WHERE  a.`tran_time`>='2018-10-01' AND a.`tran_time`&lt;=DATE_ADD(DATE_FORMAT('${pt_day}','%Y-%m-%d'),INTERVAL -1 DAY)
) b
ON a.branch_id=b.branch_id AND a.account_time=b.tran_time
WHERE b.branch_id IS NULL
UNION
SELECT DATE_FORMAT(a.tran_time,'%Y-%m-%d %H:%m:%s') '交易时间',a.branch_id '分公司ID',a.full_branch_name '分公司',a.qm_bal '汇总表期末余额',b.amount '余额文件账户余额' FROM 
(
  SELECT a.`branch_id`,a.`tran_time`,a.`full_branch_name`,a.`qm_bal` FROM `DMA_SAL_ACHV_DATA`.dma_sal_achv_sub_comp_cg_tran_sum_detail a 
  WHERE  a.`tran_time`>='2018-10-01' AND a.`tran_time`&lt;=DATE_ADD(DATE_FORMAT('${pt_day}','%Y-%m-%d'),INTERVAL -1 DAY)
) a
LEFT JOIN 
(
  SELECT a.`branch_id` branch_id,t.`amount`,t.`account_time` FROM `ODS_XD_TUANDAI_BM`.`ht_cams_cgt_allbalance_ods` t JOIN DIM_DATA.dim_org_full_branch_cfg a ON t.`user_id`=a.branch_id 
  WHERE t.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d')  AND t.`account_time`>='2018-10-01'  AND t.`account_time`&lt;=DATE_ADD(DATE_FORMAT('${pt_day}','%Y-%m-%d'),INTERVAL -1 DAY)
) b
ON a.branch_id=b.branch_id AND a.tran_time=b.account_time
WHERE b.branch_id IS NULL
UNION
SELECT DATE_FORMAT(a.tran_time,'%Y-%m-%d %H:%m:%s') '交易时间',a.branch_id '分公司ID',a.full_branch_name '分公司',a.qm_bal '汇总表期末余额',b.amount '余额文件账户余额' FROM 
(
  SELECT a.`branch_id`,a.`tran_time`,a.`full_branch_name`,a.`qm_bal` FROM `DMA_SAL_ACHV_DATA`.dma_sal_achv_sub_comp_cg_tran_sum_detail a  
  WHERE  a.`tran_time`>='2018-10-01' AND a.`tran_time`&lt;=DATE_ADD(DATE_FORMAT('${pt_day}','%Y-%m-%d'),INTERVAL -1 DAY)
) a
 JOIN 
(
  SELECT a.`branch_id`,t.`amount`,t.`account_time` FROM `ODS_XD_TUANDAI_BM`.`ht_cams_cgt_allbalance_ods` t JOIN DIM_DATA.dim_org_full_branch_cfg a ON t.`user_id`=a.branch_id 
   WHERE t.`part_dt`=DATE_FORMAT('${pt_day}','%Y-%m-%d')  AND t.`account_time`>='2018-10-01'  AND t.`account_time`&lt;=DATE_ADD(DATE_FORMAT('${pt_day}','%Y-%m-%d'),INTERVAL -1 DAY)
) b
ON a.branch_id=b.branch_id AND a.tran_time=b.account_time AND a.qm_bal!=b.amount
) t ;</sql>

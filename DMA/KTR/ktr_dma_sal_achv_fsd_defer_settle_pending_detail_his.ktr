    <connection>dwd_data</connection>
    <sql> SELECT 
  DATE_FORMAT('${pt_day}','%Y%m%d') stat_dt,
  t.business_id,                                                                     #业务编号            
  IFNULL(a1.dept_name,'未知') area_nm,                                               #区域            
  IFNULL(a2.dept_name,'未知') branch_nm,                                             #分公司
  uo.user_name AS  original_id,                                                                   #业务获取人编号 
  xb.customer_name,  								     #主借人   
  xb.customer_list ,                                                                 #共借人
  IFNULL(a7.repay_way_nm,'未知')  repay_type_nm,                                      #还款方式   
  CASE WHEN t.cur_repay_period LIKE '%XYD%' THEN '信用加成类型' ELSE '纯抵押类型' END AS product_type, #产品类型
  IFNULL(xb.audit_borrow_money,0)      borrow_amt,                                           #借款金额
  t.cur_repay_period ,								     #期数 
  t.shd_repay_date  AS shd_repay_date,     #[应还款日期]
  t.shd_amt_total - t.current_Breach AS  shd_amt ,              			# 应还金额,即本金+利息+服务费+其他费用 
  t.current_Breach,                                                        #应收滞纳金   
  DATEDIFF(DATE_FORMAT('${pt_day}','%Y-%m-%d') ,t.shd_repay_date ) AS ovd_days,                   #逾期天数    
  'tuandai_bm' AS dw_src_sys,
  'dws_agr_xd_business_after,dws_agr_xd_business,tb_department_ods,dim_evt_xd_repay_way' AS dw_src_tbl,
  CURRENT_TIMESTAMP AS dw_ins_tm,
  CURRENT_TIMESTAMP AS dw_upd_tm,
  'caihl' AS dw_ins_usr,
  'caihl' AS dw_upd_usr,
  DATE_FORMAT('${pt_day}','%Y-%m-%d')	AS	part_dt	     --	分区日期
 FROM 
 ( SELECT 
   t.business_id,
   t.cur_repay_period,
   MAX(DATE_FORMAT(t.shd_repay_date,'%Y%m%d')) AS shd_repay_date,
   SUM(IFNULL(t.shd_amt,0))  AS shd_amt_total, -- 总金额
   SUM(CASE WHEN t.fee_id IN ('79069922-e13a-4229-8656-2a1e19b44879') THEN IFNULL(t.shd_amt,0) ELSE 0 END) AS current_Breach
  FROM DWS_DATA.dws_agr_xd_business_after t
  WHERE t.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d') 
   AND t.business_type  IN ('房速贷标准件','房速贷非标准件','T600')  -- 判断房速贷 
   AND t.is_issue=1                                                   -- 是否上标，1：是，:0：否         
  -- AND t.business_defer_id IS    NULL                                 -- 未展期 
   AND t.repay_status_type &lt;&gt; '已还款'
  GROUP BY t.business_id,
           t.cur_repay_period
    ) t
  LEFT JOIN 
  (SELECT 
    xb.business_id,
    xb.original_id ,                                                                   #业务获取人编号 
    xb.customer_name,  								     #主借人   
    xb.customer_list ,								     #共借人
    xb.audit_borrow_money,
    xb.settle_way,
    xb.area_id,
    xb.branch_id,
    xb.apply_repayment_type_id                                                               
  FROM DWS_DATA.dws_agr_xd_business xb
   WHERE  xb.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')  
  ) xb
  ON t.business_id = xb.business_id
  LEFT JOIN ODS_XD_TUANDAI_BM.tb_department_ods a1
  ON xb.area_id=a1.DEPT_ID
   AND a1.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d')
 LEFT JOIN ODS_XD_TUANDAI_BM.tb_department_ods a2
  ON xb.branch_id=a2.DEPT_ID
   AND a2.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d') 
   LEFT JOIN DIM_DATA.dim_evt_xd_repay_way a7 
  ON xb.apply_repayment_type_id = a7.repay_way_id  
   LEFT JOIN ODS_XD_TUANDAI_BM.tb_user_ods uo
   ON xb.original_id = uo.user_id
   AND uo.part_dt=DATE_FORMAT('${pt_day}','%Y-%m-%d') 
 WHERE  xb.settle_way NOT IN (1,2,3)                                 -- 未结清  
;</sql>

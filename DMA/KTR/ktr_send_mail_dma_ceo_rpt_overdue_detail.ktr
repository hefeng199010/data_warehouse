    <sql>
SELECT t.`stat_dt` 统计日期,
       t.`area_nm` 片区,
       t.`branch_nm` AS 分公司,
       t.`business_id` AS 业务编号,
	   t.customer_nm as 客户姓名,
       t.`business_type_nm` AS 业务类型,
       t.`type_nm` AS 类型,
       t.`out_dt` AS 出款日期,
       t.`repay_type_nm` AS 还款方式,
       t.`cur_repay_period` AS 当前期数,
       t.`borrow_amt` AS 借款金额,
       t.`left_principa_amt` AS 剩余本金,
       t.`max_ovd_days` AS 最大逾期天数,
       t.`cur_ovd_days` AS 当期逾期天数,
       t.`asset_type` AS 资产分类,
       t.`overdue_stage` AS 逾期阶段,
       t.`receive_repayment_date` AS 应收日期,
       IFNULL(t.receive_principal, 0) AS '应收本金（元）',
  IFNULL(t.receive_interest, 0) AS '应收利息（元）',
  IFNULL(t.receive_other_fee, 0) AS '应收其他费用（元）',
  IFNULL(t.receive_little, 0) AS '应收小计（本息）',
  IFNULL(t.receive_total, 0) AS '应收合计（元）',
       t.`actual_delaying_payment` AS 实收日期,
  IFNULL(t.actual_principal, 0) AS '实收本金（元）',
  IFNULL(t.actual_interest, 0) AS '实收利息（元）',
  IFNULL(t.actual_other_fee, 0) AS '实收其他费用（元）',
  IFNULL(t.actual_delaying_payment, 0) AS '实收滞纳金（元）',
  IFNULL(t.actual_little, 0) AS '实收小计（本息）',
  IFNULL(t.actual_total, 0) AS '实收合计（元）'
FROM `DMA_CEO_RPT_DATA`.`dma_ceo_rpt_overdue_detail` t 
where t.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
  AND t.`cur_ovd_days` >0</sql>

	`WithdrawDate` DATETIME NULL DEFAULT NULL COMMENT '提现时间',
	`ResultWithdrawAmount` DECIMAL(10,2) NULL DEFAULT NULL COMMENT '提现总金额',
	`WithdrawId` INT(11) NULL DEFAULT NULL COMMENT '提现编号',
key `tb_output_list_tmp8_ProjectId`(ProjectId)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert into tb_output_list_tmp8
select t2.projectid,
	   t2.WithdrawDate,
	   t2.ResultWithdrawAmount,
	   t2.WithdrawId 
from ODS_XD_TUANDAI_BM.tb_issue_withdraw_ods t2 
where IFNULL(t2.WithdrawStatus,0) = 2 
  AND t2.WithdrawDate IS NOT NULL 
  and ifnull(t2.ResultWithdrawType,0)&lt;&gt; 3  -- 20180907 黄辉建议剔除车易贷线下提现押金类的金额
  and t2.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d');
  
  insert into public_data.tb_duty_detail_log
  (duty_id,duty_nm,duty_type, deal_nm,step_id, step_desc, row_cnt, state_dt,create_tm, create_usr)
values
  ('${pt_dutyno}', '${p_duty_nm}','1','tb_output_list_tmp8', '800', 'tb_output_list_tmp8！！', 0,'${p_day}',  NOW(),  'dl' );

  alter table DWS_DATA.dws_evt_xd_output_list truncate partition dws_evt_xd_output_list_${pt_day};

  insert into public_data.tb_duty_detail_log
  (duty_id,duty_nm,duty_type, deal_nm,step_id, step_desc, row_cnt, state_dt,create_tm, create_usr)
values
  ('${pt_dutyno}', '${p_duty_nm}','1','truncate partition', '900', 'truncate partition！！', 0,'${p_day}',  NOW(),  'dl' );

  insert into DWS_DATA.dws_evt_xd_output_list
  select  B.BUSINESS_ID                ,
		B.OUTPUT_ID                  ,
		B.OUTPUT_TIME                ,
		B.OUTPUT_TYPE                ,
		B.OUTPUT_MONEY               ,
		B.BANK_NAME                  ,
		B.CUSTOMER_NAME              ,
		B.BANK_ACCOUNT               ,
		B.UPDATE_USER                ,
		B.UPDATE_TIME                ,
		B.regist_out_flag                  ,
		B.Actual_OutTime             ,
		B.Actual_OutOperator         ,
		B.BANK_SUBNAME               ,
		B.Bank_Provice               ,
		B.Bank_City                  ,
		B.merge_time                 ,
		B.repay_id                   ,
		B.user_guid                  ,
		B.CreditorId_platformUserId  ,
		B.business_type              ,
		B.business_type_detail       ,
		B.input_time                 ,
		B.branch_id                  ,
		B.issue_id                   ,
		B.is_issue                   ,
	   A.WithdrawDate as out_date,
	   IFNULL(A.ResultWithdrawAmount,0.00) as out_amt,
	   IFNULL(A.fee_value,0.00) as out_fee,
		'1' as Is_Valid,
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'dwd_data.dwd_evt_xd_output_list,DWS_DATA.dws_agr_xd_business,dwd_data.dwd_agr_xd_issue_business..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	,--	更新用户
		DATE_FORMAT('${pt_day}','%Y-%m-%d')	as	part_dt	--	分区日期
from tb_output_list_tmp4 B
left join (
			select t2.projectid,
				   t2.WithdrawDate,
				   t2.ResultWithdrawAmount,
				   CASE WHEN t2.WithdrawId = t5.min_WithdrawId THEN tt.fee_value ELSE 0.00 END AS `fee_value`
			from tb_output_list_tmp8 t2 
			left join tb_output_list_tmp6 tt on tt.issue_id=t2.ProjectId
			left join tb_output_list_tmp7 t5 on t5.ProjectId = t2.ProjectId
		 ) A 
	   on A.projectid = B.issue_id;
  
 
  insert into DWS_DATA.dws_evt_xd_output_list 
  select  B.BUSINESS_ID                ,
		B.OUTPUT_ID                  ,
		B.OUTPUT_TIME                ,
		B.OUTPUT_TYPE                ,
		B.OUTPUT_MONEY               ,
		B.BANK_NAME                  ,
		B.CUSTOMER_NAME              ,
		B.BANK_ACCOUNT               ,
		B.UPDATE_USER                ,
		B.UPDATE_TIME                ,
		B.regist_out_flag                  ,
		B.Actual_OutTime             ,
		B.Actual_OutOperator         ,
		B.BANK_SUBNAME               ,
		B.Bank_Provice               ,
		B.Bank_City                  ,
		B.merge_time                 ,
		B.repay_id                   ,
		B.user_guid                  ,
		B.CreditorId_platformUserId  ,
		B.business_type              ,
		B.business_type_detail       ,
		B.input_time                 ,
		B.branch_id                  ,
		B.issue_id                   ,
		B.is_issue                   ,
	    B.out_date,
	    B.out_amt,
	    B.out_fee,
		'1' as Is_Valid,
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'dwd_data.dwd_evt_xd_output_list,DWS_DATA.dws_agr_xd_business,dwd_data.dwd_agr_xd_issue_business..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	,--	更新用户
		DATE_FORMAT('${pt_day}','%Y-%m-%d')	as	part_dt	--	分区日期
from tb_output_list_tmp5 B;

  insert into public_data.tb_duty_detail_log
  (duty_id,duty_nm,duty_type, deal_nm,step_id, step_desc, row_cnt, state_dt,create_tm, create_usr)
values
  ('${pt_dutyno}', '${p_duty_nm}','1','>>>>>>>>>> JOB END >>>>>>>>>>', '1000', 'sucucess！！', 0,'${p_day}',  NOW(),  'dl' );

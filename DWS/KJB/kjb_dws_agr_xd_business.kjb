	-- `shd_principa_amt` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '年化利率',
`customer_class` INT(11) DEFAULT NULL COMMENT '0:新增客户,1:结清再贷客户',
`platform_id` TINYINT(4) DEFAULT '1' COMMENT '[平台编号 1：团贷网 2：你我金融]',
	KEY `dws_agr_xd_business_tmp10_business_id` (`business_id`)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert  into dws_agr_xd_business_tmp10
select tb.business_id, 
		tb.input_time,
		'' as customer_id, 
		tb.customer_name,
		tb.operator_id,
		tb.business_type,
		tb.original_id,
		tb.branch_id,
		xd.district_id as area_id,
		tb.wf_id,
		tb.wf_node_id,
		tb.process_id,
		tb.business_out_type,
		tb.approver_id,
		tb.repayment_type_id as apply_repayment_type_id,
		tba.approve_way as audit_repayment_type_id,
		tb.borrow_money as apply_borrow_money,
		-- tb.borrow_limit as apply_borrow_limit,
		case when tb.business_type ='T600' then ifnull(bec.extension_period,tb.borrow_limit) else tb.borrow_limit end as  apply_borrow_limit , -- updata by dailiang  20180428发现部分房速贷展期借款期限跟还款计划不一致，故修改
		tba.APPROVE_money as audit_borrow_money,
		tba.APPROVE_MONTH as audit_borrow_limit,
		-- xxx.plan_settle_dt,
	    '' as pledge_type,
		case when tb.business_type ='T600' then bec.extension_rate 
		     when tb.business_type ='车易贷' then xcb.final_repay_rate 
		     when tb.business_type ='T500' then cbe.Rollover_Rates
		     when tb.business_type ='A100' then t12.borrowing_rate
		     when tb.business_type ='T290' then t13.borrowing_rate
		else  tba.APPROVE_RATE end as loan_rate,
	    tba.approve_rate,
		tb.flow_branch_id,
		tb.business_dept,
		tb.edit_procss_id,
		tb.customer_type,
		tb.after_load_process_id,
		tb.after_wf_id,
		tb.after_wf_node_id,
		tb.business_type_detail,
		tb.issue_type,
		tb.customer_list,
		tb.source_type,
		tb.source_business_id,
		-- tb.issue_split_type,
		case when ifnull(t11.cnt,0) &lt;=1 then 0 else 1 end as  issue_split_type,
		tb.is_old,
		tb.is_tuandai_repay,
		-- case when cbe.Car_Business_Extension_Id is null then 0 else 1 end as  is_extension,
		case when tb.business_type in ('T500','T600') then 1 else 0 end as  is_extension,
		case when cbe.Car_Business_Id is null then tb.business_id else cbe.Car_Business_Id END as first_business_id,
		cbe.Extension_amount,
		cbe.Renewal_Term,
		cbe.Rollover_Reimbursement_Means,
		cbe.Principal_TotalCost,
		cbe.Interest_TotalCost,
		cbe.Modify_Time as Extension_modify_time,
		cbll.mofify_time as process_modify_time,
		cbe.extension_charges,
		cbe.extension_repay_date,
		cbe.original_business_id,
		cbe.business_after_guid,
		-- ifnull(xxx.shd_principa_amt,0) as  shd_principa_amt,
        tb.customer_class, -- add by dailiang 20180706
        tb.platform_id    -- add by dailiang 20180706
from dws_agr_xd_business_tmp3 tb
left join dws_agr_xd_business_tmp8 xd  on  tb.branch_id = xd.dept_id
left join dws_agr_xd_business_tmp4 tba on tb.business_id = tba.business_id
left join dws_agr_xd_business_tmp5 cbe  on tb.business_id = cbe.Car_Business_Extension_Id
left join dws_agr_xd_business_tmp6 bec on tb.business_id = bec.extension_id
left join dws_agr_xd_business_tmp7 xcb on tb.business_id = xcb.car_business_id
left join dws_agr_xd_business_tmp1 cbll  on cbll.business_id = tb.business_id
-- left join dws_agr_xd_business_tmp23 xxx on tb.business_id = xxx.business_id
left join dws_agr_xd_business_tmp11 t11 on tb.business_id = t11.business_id
left join dws_agr_xd_business_tmp07 t12 on  tb.business_id = t12.car_business_id
left join dws_agr_xd_business_tmp007 t13 on  tb.business_id = t13.car_business_id;


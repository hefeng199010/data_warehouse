/*
处理提前结清的单begin
处理提前结清的单比如 TDF1062018051401 在1-05 财务确认结清日期为2018-09-27 15:09:16 但是实还日期为 2018-10-16 00:00:00
*/
DROP TEMPORARY TABLE IF EXISTS tb_repayment_biz_plan_dh_ods_tmp;
CREATE TEMPORARY TABLE `tb_repayment_biz_plan_dh_ods_tmp` (
  `original_business_id` varchar(50) DEFAULT NULL COMMENT '当前还款计划对应的原业务编号（若为展期还款计划，则存展期业务对应的原业务编号，若是原业务，则为空。）',
  `plan_status` int(11) DEFAULT NULL COMMENT '还款计划状态，0:还款中，10:提前结清，20:已结清，30:亏损结清，50:已申请展期',
  KEY `idx_original_business_id` (`original_business_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
;
insert into tb_repayment_biz_plan_dh_ods_tmp
SELECT 
DISTINCT t.`original_business_id`,t.plan_status
 FROM ODS_XD_TUANDAI_BM.`tb_repayment_biz_plan_dh_ods` t
WHERE t.part_dt = DATE_FORMAT('${pt_yesterday}','%Y-%m-%d') 
AND t.`plan_status`=10 -- 提前结清
and ifnull(t.src_type,1)=1 -- 只有信贷生成的还款计划 提前结清的期次为 repayed_flag=3的上一期 贷后的为正常 add by caihl20190106
;
DROP TEMPORARY TABLE IF EXISTS tb_car_business_after_ods_tqjq_tmp;
CREATE TEMPORARY TABLE `tb_car_business_after_ods_tqjq_tmp` (
  `car_business_id` varchar(50) DEFAULT NULL COMMENT '[业务编号]',
  `borrow_date` datetime DEFAULT NULL COMMENT '[还款日期]',
  KEY `idx_car_business_id` (`car_business_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
;
insert into tb_car_business_after_ods_tqjq_tmp
SELECT cba.`car_business_id`,MIN(cba.`borrow_date`) AS borrow_date
FROM ODS_XD_TUANDAI_BM.tb_car_business_after_ods cba -- 贷后管理主表
WHERE cba.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
AND EXISTS (SELECT 1 FROM tb_repayment_biz_plan_dh_ods_tmp bp WHERE cba.`car_business_id` = bp.original_business_id)
AND cba.`repayed_flag`=3
-- and cba.`car_business_id`='TDF1062018051401'
GROUP BY cba.`car_business_id`
;

DROP TEMPORARY TABLE IF EXISTS tb_car_business_after_ods_tqjq_tmp1;
CREATE TEMPORARY TABLE `tb_car_business_after_ods_tqjq_tmp1` (
  `car_business_id` varchar(50) NOT NULL,
  `borrow_date` datetime DEFAULT NULL,
  `fatct_replayDate` datetime DEFAULT NULL,
  `finance_confirmed_date` datetime DEFAULT NULL COMMENT '[财务还款确认日期]',
    KEY `idx_car_business_id` (`car_business_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
;
insert into tb_car_business_after_ods_tqjq_tmp1
SELECT
aft.`car_business_id`
,MAX(aft.`borrow_date`) AS  borrow_date
,MAX(aft.`fatct_replayDate`) AS fatct_replayDate
,MAX(aft.`finance_confirmed_date`) AS finance_confirmed_date
-- aft.`repayed_flag`,aft.`reserve_2`,aft.`borrow_date`,aft.`fatct_replayDate`,aft.`finance_confirmed_date`,aft.* 
FROM ODS_XD_TUANDAI_BM.tb_car_business_after_ods aft
WHERE aft.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
AND EXISTS (SELECT 1 FROM tb_repayment_biz_plan_dh_ods_tmp bp WHERE aft.`car_business_id` = bp.original_business_id)  -- 只取提前结清的单
AND  NOT EXISTS (SELECT 1 FROM tb_car_business_after_ods_tqjq_tmp tt WHERE aft.`car_business_id` = tt.`car_business_id` AND aft.`borrow_date` >= tt.`borrow_date` )  -- 取提前结清之前的期次
GROUP BY aft.`car_business_id`;



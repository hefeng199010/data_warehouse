CREATE TEMPORARY TABLE `dws_agr_xd_business_tmp2` (
`business_id` varchar(50) DEFAULT NULL COMMENT '业务单号',
-- `plan_settle_dt` datetime DEFAULT NULL COMMENT '修改时间',
 `act_settle_dt` datetime DEFAULT NULL COMMENT '修改时间',
 `fin_act_settle_dt` datetime DEFAULT NULL COMMENT '修改时间',
`repay_status_subtype` INT(10) DEFAULT NULL COMMENT '业务单号',
key `dws_agr_xd_business_tmp2_business_id`(business_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert into dws_agr_xd_business_tmp2
SELECT cba.car_business_id AS business_id,
       -- max(cba.borrow_date) as plan_settle_dt,
        MAX(IFNULL(tqjq.fatct_replayDate,cba.fatct_replayDate)) AS  act_settle_dt,
        MAX(IFNULL(tqjq.finance_confirmed_date,cba.finance_confirmed_date)) AS  fin_act_settle_dt,
        MAX(IFNULL(rss.repay_status_subtype_id,0)) AS repay_status_subtype
from ODS_XD_TUANDAI_BM.tb_car_business_after_ods cba -- 贷后管理主表
left join DIM_DATA.dim_evt_xd_repay_status_subtype rss
	on ifnull(cba.reserve_2,'其他')= rss.repay_status_subtype_nm
left join tb_car_business_after_ods_tqjq_tmp1 tqjq on cba.car_business_id = tqjq.car_business_id
where cba.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
group by cba.car_business_id;





		`business_id` VARCHAR(50) NOT NULL DEFAULT '' COMMENT '业务单号',
	`input_time` DATETIME NULL DEFAULT NULL COMMENT '受理时间',
	`customer_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '客户id',
	`customer_name` VARCHAR(50) NULL DEFAULT NULL COMMENT '客户名称',
	`operator_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '主办人编号',
	`business_type` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务类型',
	`original_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务获取人编号',
	`branch_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务所属分公司编号',
	`area_id` VARCHAR(51) NULL DEFAULT NULL COMMENT '区域编号',
	`wf_id` INT(11) NULL DEFAULT NULL COMMENT '主流程编号',
	`wf_node_id` INT(11) NULL DEFAULT NULL COMMENT '子流程编号',
	`process_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务流程编号',
	`business_out_type` INT(11) NULL DEFAULT NULL COMMENT '1为线上费率，0为线下费率',
	`approver_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '最后审批人,用于撤销',
	`apply_repayment_type_id` INT(11) NULL DEFAULT NULL COMMENT '线下出款则为null，还款方式ID，1到期还本息，2每月付息到期还本，5每月等额本息',
	`audit_repayment_type_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '审批还款方式',
	`apply_borrow_money` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '申请借款金额',
	`apply_borrow_limit` INT(11) NULL DEFAULT NULL COMMENT '申请借款期限',
	`audit_borrow_money` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '审批借款金额',
	`audit_borrow_limit` INT(11) NULL DEFAULT NULL COMMENT '审批借款期限',
	-- `plan_settle_dt` DATETIME NULL DEFAULT NULL COMMENT '计划结清时间',
	`pledge_type` VARCHAR(50) NULL DEFAULT NULL COMMENT '抵押类型',
	`loan_rate` DECIMAL(10,4) NULL DEFAULT NULL COMMENT '年化利率',
	`approve_rate` VARCHAR(50) NULL DEFAULT NULL COMMENT '最终借款利率',
	`flow_branch_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '流程所属分公司编号',
	`business_dept` VARCHAR(50) NULL DEFAULT NULL COMMENT '业务部门',
	`edit_procss_id` VARCHAR(500) NULL DEFAULT NULL COMMENT '已编辑过的节点',
	`customer_type` INT(11) NULL DEFAULT NULL COMMENT '客户类型(0和1为个人借款, 2为企业借款)',
	`after_load_process_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '贷后业务流程编号',
	`after_wf_id` INT(11) NULL DEFAULT NULL COMMENT '贷后主流程编号',
	`after_wf_node_id` INT(11) NULL DEFAULT NULL COMMENT '贷后子流程编号',
	`business_type_detail` VARCHAR(50) NULL DEFAULT NULL COMMENT '[具体业务类型分类]',
	`issue_type` INT(11) NULL DEFAULT NULL COMMENT '发标类型0:线下放款, 1:P2p,2:定期理财',
	`customer_list` VARCHAR(2000) NULL DEFAULT NULL COMMENT '所有客户姓名,已逗号隔开',
	`source_type` TINYINT(4) NULL DEFAULT NULL COMMENT '业务来源：0-常规录入 1-结清续贷新业务 2-结清续贷续贷业务 3-线下历史导入 4-扫码业务 5-优质车抵贷',
	`source_business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '原始来源业务的业务编号（目前用于结清续贷业务）',
	`issue_split_type` INT(11) NULL DEFAULT NULL COMMENT '标识是否P2P拆标业务，0：非P2P拆标业务，1：P2P拆标业务',
	`is_old` INT(11) NULL DEFAULT NULL COMMENT '1是历史数据，其他不是',
	`is_tuandai_repay` INT(11) NULL DEFAULT NULL,
	`is_extension` INT(11) NULL DEFAULT NULL COMMENT '是否展期 1：是 0：否',
	`first_business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '原业务编号',
	`Extension_amount` DECIMAL(10,2) NULL DEFAULT NULL COMMENT '展期金额',
	`Renewal_Term` INT(11) NULL DEFAULT NULL COMMENT '展期期限',

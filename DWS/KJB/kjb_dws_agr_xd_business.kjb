DROP TEMPORARY TABLE IF EXISTS `dws_agr_xd_business_tmp9`;
CREATE TEMPORARY TABLE `dws_agr_xd_business_tmp9` (
`business_id` varchar(50) DEFAULT NULL COMMENT '业务单号',
`settle_way` INT(10) DEFAULT NULL COMMENT '是否提前结清',
`fin_settle_way` INT(10) DEFAULT NULL COMMENT '是否提前结清',
key `dws_agr_xd_business_tmp9_business_id`(business_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert into dws_agr_xd_business_tmp9
SELECT a.car_business_id AS business_id,
	CASE WHEN MAX(DATE_FORMAT(a.`fatct_replayDate`,'%Y%m%d')) - MAX(DATE_FORMAT(a.`borrow_date`,'%Y%m%d')) = 0 THEN 1	
	     WHEN MAX(DATE_FORMAT(a.`fatct_replayDate`,'%Y%m%d')) - MAX(DATE_FORMAT(a.`borrow_date`,'%Y%m%d')) > 0 THEN 2	
	     WHEN MAX(DATE_FORMAT(a.`fatct_replayDate`,'%Y%m%d')) - MAX(DATE_FORMAT(a.`borrow_date`,'%Y%m%d')) &lt; 0 THEN 3 END settle_way,
	CASE WHEN MAX(DATE_FORMAT(a.`finance_confirmed_date`,'%Y%m%d')) - MAX(DATE_FORMAT(a.`borrow_date`,'%Y%m%d')) = 0 THEN 1	
	     WHEN MAX(DATE_FORMAT(a.`finance_confirmed_date`,'%Y%m%d')) - MAX(DATE_FORMAT(a.`borrow_date`,'%Y%m%d')) > 0 THEN 2	
	     WHEN MAX(DATE_FORMAT(a.`finance_confirmed_date`,'%Y%m%d')) - MAX(DATE_FORMAT(a.`borrow_date`,'%Y%m%d')) &lt; 0 THEN 3 END fin_settle_way
 FROM ODS_XD_TUANDAI_BM.tb_car_business_after_ods a 
 LEFT JOIN (SELECT DISTINCT aa.car_business_id,aa.car_business_after_type  FROM ODS_XD_TUANDAI_BM.tb_car_business_after_ods aa WHERE aa.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d') and aa.car_business_after_type&lt;&gt;'已还款') aa
        ON aa.car_business_id = a.car_business_id
WHERE a.car_business_after_type ='已还款'
  AND aa.car_business_id IS NULL
  and a.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
GROUP BY a.car_business_id;


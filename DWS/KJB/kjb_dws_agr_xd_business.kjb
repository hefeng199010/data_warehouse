DROP TEMPORARY TABLE IF EXISTS `dws_agr_xd_business_tmp1`;
CREATE TEMPORARY TABLE `dws_agr_xd_business_tmp1` (
`business_id` varchar(50) DEFAULT NULL COMMENT '业务单号',
`mofify_time` datetime DEFAULT NULL COMMENT '修改时间',
key `dws_agr_xd_business_tmp1_business_id`(business_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8;

insert into dws_agr_xd_business_tmp1(`business_id`,`mofify_time`)
select cbl.car_business_id as business_id,
	   max(create_time)    as mofify_time
from ODS_XD_TUANDAI_BM.tb_car_business_log_ods cbl
where cbl.part_dt = DATE_FORMAT('${pt_yesterday}','%Y-%m-%d')
group by  cbl.car_business_id;


/*
dws_business_after_tmp01该表是为了使贷后系统结清期repay_flag=50，转为已还款状态（repay_flag=1）
dws_business_after_tmp02该表是为了处理你我金融在贷后系统的还款状态，你我金融没有结清标志，只有已还款标记（repay_flag=90）,首先更改结清期及其以后的标志，其次在修改结清期标识时需要关联实际还款日期
*/
DROP TEMPORARY TABLE IF EXISTS `dws_business_after_tmp01`;
CREATE TEMPORARY TABLE `dws_business_after_tmp01` (
	`plan_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	`business_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	`orig_business_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	due_date datetime NOT NULL COMMENT '应还日期',
key `dws_business_after_tmp01_plan_id`(plan_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

 insert into dws_business_after_tmp01
SELECT t1.plan_id,t1.`business_id`,t1.`orig_business_id`,MIN(t1.`due_date`) due_date
FROM `ODS_XD_TUANDAI_BM`.`tb_repayment_biz_plan_list_dh_ods` t1 
WHERE IFNULL(t1.`src_type`,1) != 1 
  AND part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
  AND ifnull(t1.`repay_flag`,0)=50
GROUP BY t1.plan_id,t1.`business_id`,t1.`orig_business_id` ;


DROP TEMPORARY TABLE IF EXISTS `dws_business_after_tmp02`;
CREATE TEMPORARY TABLE `dws_business_after_tmp02` (
	`plan_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	`plan_list_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	`business_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	fact_repay_date datetime NOT NULL COMMENT '应还日期',
key `dws_business_after_tmp02_list_id`(plan_id),
key `dws_business_after_tmp02_plan_list_id`(plan_list_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

INSERT INTO dws_business_after_tmp02

SELECT t01.plan_id,t01.plan_list_id,t01.business_id,MAX(t01.`fact_repay_date`) fact_repay_date
FROM 
(             SELECT tb2.plan_id,tb1.business_id,tb1.`fact_repay_date`,tb1.`period`,tb1.plan_list_id
              FROM (SELECT DISTINCT t1.business_id,t1.`fact_repay_date`,t1.`period`,t1.plan_list_id
	             FROM `ODS_XD_TUANDAI_BM`.`tb_repayment_biz_plan_list_detail_dh_ods` t1
		   WHERE IFNULL(t1.`src_type`,1) != 1 
	            AND t1.`fact_repay_date` IS NOT NULL
	            AND t1.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
	         )tb1
	         JOIN 
		  (SELECT DISTINCT t1.plan_id,t1.plan_list_id
		FROM `ODS_XD_TUANDAI_BM`.`tb_repayment_biz_plan_list_dh_ods` t1
		WHERE IFNULL(t1.`src_type`,1) != 1 
		  AND t1.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
		  AND IFNULL(t1.`repay_flag`,0) = 90
		  ) tb2 ON tb1.plan_list_id = tb2.plan_list_id
		  
		  )t01
JOIN(
SELECT tb2.plan_id,tb1.business_id,tb1.`fact_repay_date`,tb1.`period` period
		           FROM 
				   (SELECT t1.plan_list_id,t1.business_id,MAX(t1.`fact_repay_date`) fact_repay_date,MAX(t1.`period`) period
					FROM `ODS_XD_TUANDAI_BM`.`tb_repayment_biz_plan_list_detail_dh_ods` t1
				   WHERE IFNULL(t1.`src_type`,1) != 1 
					 AND t1.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
					 AND t1.`fact_repay_date` IS NOT NULL
					 GROUP BY t1.plan_list_id, t1.business_id) tb1
					 JOIN 
					  (SELECT DISTINCT t1.plan_id,t1.plan_list_id
					FROM `ODS_XD_TUANDAI_BM`.`tb_repayment_biz_plan_list_dh_ods` t1
					WHERE IFNULL(t1.`src_type`,1) != 1 
					  AND t1.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
					  AND IFNULL(t1.`repay_flag`,0) = 90
					  ) tb2 ON tb1.plan_list_id = tb2.plan_list_id
				 )t02 ON t01.business_id = t02.business_id AND t01.fact_repay_date = t02.fact_repay_date AND t01.period = t02.period  AND t01.plan_id = t02.plan_id
				 GROUP BY t01.plan_id,t01.plan_list_id,t01.business_id;
			   
	  
	

DROP TEMPORARY TABLE IF EXISTS `dws_business_after_tmp03`;
CREATE TEMPORARY TABLE `dws_business_after_tmp03` (
	`plan_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	`business_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	`orig_business_id` VARCHAR(50) NOT NULL COMMENT '[业务编号]',
	due_date datetime NOT NULL COMMENT '应还日期',
	fact_repay_date datetime NOT NULL COMMENT '应还日期',
key `dws_business_after_tmp03_plan_id`(plan_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;  

insert into dws_business_after_tmp03
	SELECT t1.plan_id,t1.`business_id`,t1.`orig_business_id`,MAX(t1.due_date),MAX(t01.`fact_repay_date`)
	FROM (select t1.plan_id,t1.plan_list_id,t1.`business_id`,t1.`orig_business_id`,t1.`due_date`,t1.`fact_repay_date`
			FROM `ODS_XD_TUANDAI_BM`.`tb_repayment_biz_plan_list_dh_ods` t1
			WHERE IFNULL(t1.`src_type`,1) != 1 
			  AND t1.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
			  AND ifnull(t1.`repay_flag`,0) = 90
			) t1
	join dws_business_after_tmp02 t01 on t1.plan_list_id = t01.plan_list_id
	JOIN (SELECT t2.`plan_id`,t2.`business_id`,t2.`original_business_id`
			FROM ODS_XD_TUANDAI_BM.`tb_repayment_biz_plan_dh_ods` t2 
		   WHERE t2.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
			 AND t2.`plan_status` IN (10,20,30)
			 AND IFNULL(t2.`src_type`,1) != 1 
			   )t2 ON t1.`plan_id` = t2.`plan_id`
        GROUP BY t1.plan_id,t1.`business_id`,t1.`orig_business_id`;


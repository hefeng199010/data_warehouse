DROP TEMPORARY TABLE IF EXISTS `dws_business_after_tmp2`;
CREATE TEMPORARY TABLE `dws_business_after_tmp2` ( 
    `car_business_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '[业务编号]',
	`car_business_after_defer` VARCHAR(50) NULL DEFAULT NULL COMMENT '[展期业务编号]',
	`car_business_after_id` VARCHAR(50) NULL DEFAULT NULL COMMENT '[当前还款期数]',
	`current_Principa` DECIMAL(12,2) NULL DEFAULT NULL COMMENT '[本期应还本金]',
	`current_accrual` VARCHAR(50) NULL DEFAULT NULL COMMENT '[本期应还利息]',
	`overdueDays` VARCHAR(50) NULL DEFAULT NULL COMMENT '[逾期天数]',
	`borrow_date` DATETIME NULL DEFAULT NULL COMMENT '[还款日期]',
	`car_business_after_type` VARCHAR(50) NULL DEFAULT NULL COMMENT '[还款状态分类]：还款中，已还款，逾期',
	`repayed_flag` INT(11) NULL DEFAULT NULL COMMENT '[已还款类型]0:还款中1:财务确认已还款 2:自动匹配已还款 3:财务确认全部结清,4:财务代扣已还款,5:自动代扣已还款,6:标识展期已还款,7:当期部分已还款,8:用户APP主动还款,9:代扣全部结清',
	`fatct_replayDate` DATETIME NULL DEFAULT NULL COMMENT '[实际还款日期]',
	`finance_confirmed_date` DATETIME NULL DEFAULT NULL COMMENT '[财务还款确认日期]',
	`fact_principa` VARCHAR(50) NULL DEFAULT NULL COMMENT '[实际归还本金]',
	`fact_accrual` VARCHAR(50) NULL DEFAULT NULL COMMENT '[实际归还利息]',
	`confirm_flag` INT(11) NULL DEFAULT NULL COMMENT '[财务还款金额确认(1:已确认,0:未确认)]',
	`src_type` INT(11) DEFAULT NULL COMMENT '来源类型：1.信贷生成，2.贷后管理生成',
key `dws_business_after_tmp2_car_business_id`(car_business_id)
)ENGINE=MEMORY DEFAULT CHARSET=utf8 ;

insert into dws_business_after_tmp2
SELECT t1.`orig_business_id` car_business_id,
       t1.`business_id` car_business_after_defer,
       t1.`after_id` car_business_after_id,
       t2.current_Principa current_Principa,
       t2.current_accrual current_accrual,
       t1.`overdue_days` overdueDays,
       t1.`due_date` borrow_date,
       t1.`current_status` car_business_after_type,
       t1.`repay_flag` repayed_flag,
       t1.`fact_repay_date` fatct_replayDate,
       t1.`finance_comfirm_date` finance_confirmed_date,
       t2.fact_principa fact_principa,
       t2.fact_accrual fact_accrual,
       t1.`confirm_flag` confirm_flag,
       2 src_type
FROM(
		SELECT t1.`plan_list_id`,
			   t1.`orig_business_id`,
			   t1.`business_id`,
			   t1.`after_id`,
			   t1.`overdue_days`,
			   t1.`due_date`,
			   t1.`current_status`,
			   -- 信贷：[已还款类型]0:还款中1:财务确认已还款 2:自动匹配已还款 3:财务确认全部结清,4:财务代扣已还款,5:自动代扣已还款,6:标识展期已还款,7:当期部分已还款
			   -- 贷后：已还款类型标记，null：还款中，0：申请展期已还款，10：线下确认已还款，20：自动线下代扣已还款，21，人工代扣已还款，30：自动银行代扣已还款，31：人工银行代扣已还款，40：用户APP主动还款，50：线下财务确认全部结清，60：线下代扣全部结清，70：银行代扣全部结清
			   CASE WHEN IFNULL(t1.`repay_flag`,0) = 50 AND t2.due_date IS NULL THEN 3
			        WHEN IFNULL(t1.`repay_flag`,0) = 50 AND t2.due_date IS NOT NULL THEN 1
			        WHEN IFNULL(t1.`repay_flag`,0) = 90 AND DATE_FORMAT(t1.fact_repay_date,'%Y-%m-%d') &lt;= DATE_FORMAT(t3.fact_repay_date,'%Y-%m-%d') AND t1.due_date &lt;= t3.due_date THEN 1
			        WHEN IFNULL(t1.`repay_flag`,0) = 90 AND DATE_FORMAT(t1.fact_repay_date,'%Y-%m-%d') >= DATE_FORMAT(t3.fact_repay_date,'%Y-%m-%d') AND t1.due_date > t3.due_date THEN 3
                    WHEN IFNULL(t1.`repay_flag`,0) = 10 THEN 1	
                    WHEN IFNULL(t1.`repay_flag`,0) IN (20,30) THEN 5
                    WHEN IFNULL(t1.`repay_flag`,0) IN (21,31) THEN 4	
                    WHEN IFNULL(t1.`repay_flag`,0) = 40 THEN 8
                    WHEN IFNULL(t1.`repay_flag`,0) = 6  THEN 6	 -- add by caihl20181214				
					WHEN IFNULL(t1.`repay_flag`,0) = 0 AND t1.fact_repay_date IS NOT NULL AND t1.repay_status IN (1,2)	THEN 7		-- add by caihl20190107 部分还款数据
					ELSE t1.repay_flag END AS repay_flag   ,
			   t1.`fact_repay_date`,
			   t1.`finance_comfirm_date`,
			   t1.`confirm_flag`,
			   t1.`src_type`
		FROM `ODS_XD_TUANDAI_BM`.`tb_repayment_biz_plan_list_dh_ods` t1 
		LEFT JOIN dws_business_after_tmp01 t2 ON t1.plan_id = t2.plan_id AND t1.business_id = t2.business_id AND t1.due_date = t2.due_date -- 贷后系统结清期repay_flag=50 ，相当于信贷系统repayed_flag=1，要将结清期repay_flag=50，转为3
		LEFT JOIN dws_business_after_tmp03 t3 ON t1.plan_id = t3.plan_id AND t1.business_id = t3.business_id  -- 你我金融贷后系统已还款repay_flag=90，如果已结清需要将其转为3
		WHERE IFNULL(t1.`src_type`,1) != 1 
		  and part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
		)t1 
LEFT JOIN 
		(SELECT a.`plan_list_id`,
			   a.`business_id`,
				MAX(CASE WHEN a.plan_item_name ='本金' THEN a.`plan_amount` ELSE 0 END ) current_Principa,
				MAX(CASE WHEN a.plan_item_name ='利息' THEN a.`plan_amount` ELSE 0 END ) current_accrual,
				MAX(CASE WHEN a.plan_item_name ='本金' THEN a.`fact_amount` ELSE 0 END ) fact_principa,
				MAX(CASE WHEN a.plan_item_name ='利息' THEN a.`fact_amount` ELSE 0 END ) fact_accrual
		FROM  `ODS_XD_TUANDAI_BM`.`tb_repayment_biz_plan_list_detail_dh_ods` a
		WHERE a.`plan_item_name` IN ('本金','利息')
		  AND IFNULL(a.`src_type`,1) != 1
		  and part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')
		  GROUP BY a.`plan_list_id`,
			   a.`business_id`)t2 ON t1.plan_list_id = t2.plan_list_id;
			   
 

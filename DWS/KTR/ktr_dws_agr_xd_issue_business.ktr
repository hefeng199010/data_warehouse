    <connection>dwd_data</connection>
    <sql>select 
				tib.issue_id                ,
				tib.business_id             ,
				tib.status_flag             ,
				tib.full_time               ,
				tib.full_borrow_money       ,
				tib.borrow_limit            ,
				tib.repayment_type          ,
				tib.nick_name               ,
				tib.accounter_confirm_user  ,
				tib.accounter_confirm_time  ,
				tib.td_loan_time            ,
				tib.td_loan_money           ,
				tib.create_user             ,
				tib.create_time             ,
				tib.update_user             ,
				tib.update_time             ,
				tib.extend_flag             ,
				tib.catsed_amount           ,
				tib.loaner_id               ,
				tib.org_issue_id            ,
				tib.master_issue_id         ,
				tib.issue_order             ,
				tib.user_guid               ,
				tib.business_after_guid     ,
				ti.queryFullsuccessDate   as full_issue_date  ,
				tb.apply_borrow_money as borrow_money,
	            tb.input_time,
                tb.apply_borrow_limit as audit_borrow_limit,
				ti.RealName                 ,
				ti.IdentityCard             ,
		'1' as Is_Valid,
		'tuandai_bm'	as	dw_src_sys	,--	来源系统
		'dwd_data.dwd_agr_xd_issue_business,dwd_data.dwd_agr_xd_issue..'	as	dw_src_tbl	,--	来源表
		now()	as	dw_ins_tm	,--	插入时间
		now()	as	dw_upd_tm	,--	更新时间
		'dailiang'	as	dw_ins_usr	,--	插入用户
		'dailiang'	as	dw_upd_usr	,--	更新用户
		DATE_FORMAT('${pt_day}','%Y-%m-%d')	as	part_dt,	--	分区日期
        ifnull(ti.UserTypeId,1) as UserTypeId,
		ifnull(ti.amount,0)     as begin_issue_amount
from ODS_XD_TUANDAI_BM.tb_issue_business_ods tib
left join (SELECT ti.IssueId,ti.RealName, ti.queryFullsuccessDate,ti.IdentityCard,ti.UserTypeId,ti.amount FROM ODS_XD_TUANDAI_BM.tb_issue_ods ti WHERE ti.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d'))ti
      on  tib.issue_id = ti.IssueId
left join (select tb.business_id,tb.apply_borrow_limit,tb.input_time, tb.apply_borrow_money from DWS_DATA.dws_agr_xd_business tb where tb.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d'))tb
       on tb.business_id = tib.business_id
where tib.part_dt = DATE_FORMAT('${pt_day}','%Y-%m-%d')</sql>
